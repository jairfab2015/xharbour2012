    <TfrxIBXQuery Name="COMISSAO" UserName="COMISSAO" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT P.CODIGO , P.PEDIDO, P.DATAEMISSAO, P.VALORTOTAL, P.PEDIDOCARGA,&#13;&#10;P.CODIGOFUNCIONARIO, F.NOME AS VENDEDOR, H.SUPERIOR,&#13;&#10;P.CODIGORELACAO,C.NOME AS CLIENTE, PR.NOME, PR.CLASSIFICACAO, CL.DESCRICAO,&#13;&#10;PP.CODIGOPRODUTO, PP.QUANTIDADE AS QTDA, PP.VALORUNITARIO, PP.VALORCUSTO,&#13;&#10;PP.VALORDESCONTO, PP.VALORTOTAL AS TOTALITEM,&#13;&#10;PP.COMISSAO AS PERCENTUAL, (PP.VALORTOTAL * (PP.COMISSAO /100)) AS VALORCOMISSAO,&#13;&#10;pb.documento As Doc, nf.codigonf, nf.dataemissao as NFDATA&#13;&#10;FROM PEDIDO P, CLIENTE C, RELACAO RL, PEDIDOPRODUTO PP, PRODUTO PR,&#13;&#10;FUNCIONARIO F, HIERARQUIAFUNC H ,  CLASSIFICACAOPRODUTO CL, pedidobaixaproduto pb, notafiscal nf&#13;&#10;WHERE (P.PEDIDOTIPO = 'V')&#13;&#10;AND (P.CONVERTEPARA = 'N')&#13;&#10;AND (P.CODIGORELACAO = RL.CODIGORELACAO)&#13;&#10;AND (RL.CODIGO = C.CODIGOCLIENTE)&#13;&#10;AND (P.CODIGO = PP.CODIGOPEDIDO)&#13;&#10;AND (PP.CODIGOPRODUTO = PR.CODIGO)&#13;&#10;AND (P.CODIGOFUNCIONARIO  = F.CODIGO)&#13;&#10;AND (F.CODIGO = H.SUBFUNCIONARIO)&#13;&#10;AND (P.CODIGOFUNCIONARIO BETWEEN :FUNCI AND :FUNCF)&#13;&#10;AND (PR.CLASSIFICACAO = CL.CODIGO)&#13;&#10;AND (NF.DATAEMISSAO BETWEEN :DATAI AND :DATAF)&#13;&#10;AND (pp.codigo = pb.codigopedidoproduto)&#13;&#10;AND (nf.documento = pb.documento)&#13;&#10;AND (NF.CODIGOFUNCIONARIO = P.CODIGOFUNCIONARIO)&#13;&#10;&#13;&#10;ORDER BY H.SUPERIOR, P.CODIGOFUNCIONARIO, PR.CLASSIFICACAO , P.DATAEMISSAO" PropData=""/>
