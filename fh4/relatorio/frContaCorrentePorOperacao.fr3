<TfrxReport Version="3.24" DotMatrixReport="False" EngineOptions.UseFileCache="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4031" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="37784,3793292361" ReportOptions.Description.Text="" ReportOptions.Name="RELATÓRIO DE CONTA CORRENTE DE CLIENTES - POR OPERAÇÃO" ReportOptions.LastChange="39539,6328589699" ScriptLanguage="PascalScript" ScriptText.Text="VAR SALDO : REAL;&#13;&#10;    CLIENTEFUNCIONARIO, OPER_SERV, RESULT : STRING;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF (sighaparam('TIPO') = 'C') THEN BEGIN&#13;&#10;    IF (&#60;DSCC.&#34;TIPOOPERACAO&#34;&#62; = 'D') THEN BEGIN&#13;&#10;       IF (&#60;DSCC.&#34;CANCELADO&#34;&#62; = 1) THEN&#13;&#10;         SALDO         := SALDO - &#60;DSCC.&#34;VALOR&#34;&#62;&#13;&#10;       ELSE&#13;&#10;         SALDO         := SALDO + &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;    END&#13;&#10;    ELSE BEGIN&#13;&#10;      IF (&#60;DSCC.&#34;CANCELADO&#34;&#62; = 1) THEN&#13;&#10;         SALDO         := SALDO + &#60;DSCC.&#34;VALOR&#34;&#62;&#13;&#10;      ELSE&#13;&#10;         SALDO         := SALDO - &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;    END&#13;&#10;  END;&#13;&#10;&#13;&#10;  IF (sighaparam('TIPO') = 'F') THEN BEGIN&#13;&#10;    IF (&#60;DSCC.&#34;TIPOOPERACAO&#34;&#62; = 'D') THEN BEGIN&#13;&#10;       IF (&#60;DSCC.&#34;CANCELADO&#34;&#62; = 1) THEN&#13;&#10;         SALDO         := SALDO + &#60;DSCC.&#34;VALOR&#34;&#62;&#13;&#10;       ELSE&#13;&#10;         SALDO         := SALDO - &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;    END&#13;&#10;    ELSE BEGIN&#13;&#10;       IF (&#60;DSCC.&#34;CANCELADO&#34;&#62; = 1) THEN&#13;&#10;         SALDO         := SALDO - &#60;DSCC.&#34;VALOR&#34;&#62;&#13;&#10;       ELSE&#13;&#10;         SALDO         := SALDO + &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;    END;&#13;&#10;  END;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SALDO := 0;&#13;&#10;  IF (SIGHAPARAM('CODIGOLANCAMENTO') = '0') OR (STRTOINT(SIGHAPARAM('CODIGOLANCAMENTO')) &#62;= 16000) THEN&#13;&#10;    OPER_SERV := &#60;DSCC.&#34;NOME_OPERACAO&#34;&#62;&#13;&#10;  ELSE&#13;&#10;    OPER_SERV := &#60;DSCC.&#34;NOME_SERVICO&#34;&#62;;&#13;&#10;&#13;&#10;  IF (SIGHAPARAM('CLASSIFICACAO') &#60;&#62; '') then begin&#13;&#10;     MEMO21.TEXT          := 'Classificação : ' + SIGHAPARAM('CLASSIFICACAO') + '-' + SIGHAPARAM('NOMECLASSIFICACAO')&#13;&#10;  end&#13;&#10;  ELSE&#13;&#10;     MEMO21.TEXT          := '';&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo25OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF COPY(&#60;DSCC.&#34;HISTORICO&#34;&#62;, 1, LENGTH(OPER_SERV)) = OPER_SERV THEN&#13;&#10;     RESULT := '...' + COPY(&#60;DSCC.&#34;HISTORICO&#34;&#62;, LENGTH(OPER_SERV) + 1, 60)&#13;&#10;  ELSE&#13;&#10;     RESULT := &#60;DSCC.&#34;HISTORICO&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  CLIENTEFUNCIONARIO := sighaparam('TIPORELATORIO');&#13;&#10;  IF (SIGHAPARAM('CODIGOLANCAMENTO') = '0') OR (STRTOINT(SIGHAPARAM('CODIGOLANCAMENTO')) &#62;= 16000) THEN BEGIN&#13;&#10;    GroupHeader1.CONDITION := '&#60;DSCC.&#34;CODIGOLANCAMENTO&#34;&#62;';&#13;&#10;    MEMO1.TEXT             := '[DSCC.&#34;CODIGOLANCAMENTO&#34;] - [DSCC.&#34;NOME_OPERACAO&#34;]...';&#13;&#10;  END&#13;&#10;  ELSE BEGIN&#13;&#10;    GroupHeader1.CONDITION := '&#60;DSCC.&#34;CODIGOSERVICO&#34;&#62;';&#13;&#10;    MEMO1.TEXT             := '[DSCC.&#34;CODIGOSERVICO&#34;] - [DSCC.&#34;NOME_SERVICO&#34;]...';&#13;&#10;  END;&#13;&#10;end." PropData="">
