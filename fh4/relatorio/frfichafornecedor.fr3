    <TfrxIBXQuery Name="FICHA" UserName="FICHA" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT CL.*, CI.CIDADE AS NOMECIDADE, CI.ESTADO, CI.CEP,&#13;&#10;CLC.DESCRICAO AS CLASSIFICACAO_DESCRICAO&#13;&#10;&#13;&#10;&#13;&#10;FROM FORNECEDOR CL, CIDADE CI,&#13;&#10;CLASSIFICACAOFORNECEDOR CLC&#13;&#10;&#13;&#10;WHERE&#13;&#10;(CL.CIDADE = CI.CODIGO) AND&#13;&#10;(CL.CODIGOCLASSIFICACAO = CLC.CODIGO) AND&#13;&#10;(CL.CODIGO BETWEEN :CODIGOI AND :CODIGOF)&#13;&#10;ORDER BY CL.NOME" PropData=""/>
