    <TfrxIBXQuery Name="NOTAFISCALCC" UserName="NOTAFISCALCC" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT NF.VALORTOTALNF AS TOTALNF, R.NOME, CC.*&#13;&#10;FROM NOTAFISCAL NF, CONTACORRENTE CC, RELACAO R&#13;&#10;WHERE NF.CODIGOCAIXA = :CODIGOCAIXAABERTURA&#13;&#10;AND NF.TIPOMOVIMENTO = 'S'&#13;&#10;AND NF.NOTAFISCALCANCELADA = 0&#13;&#10;AND NF.CNPJ = :CNPJ&#13;&#10;AND CC.NUMERODOCUMENTO = NF.DOCUMENTO&#13;&#10;AND CC.CODIGORELACAO = R.CODIGORELACAO" PropData=""/>
