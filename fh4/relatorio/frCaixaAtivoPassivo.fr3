tions.VersionMinor="èˆ" ReportOptions.VersionRelease="üÿ’˜Çe4" ScriptLanguage="PascalScript" ScriptText.Text="Var    TOTAL,&#13;&#10;       RECEBIMENTOS,&#13;&#10;       PAGAMENTOS,&#13;&#10;       EXTRA,&#13;&#10;       VENDAPRODUTO,&#13;&#10;       TOTALAFERICAO,&#13;&#10;       TOTALQTDEBOMBA,&#13;&#10;       TOTALVENDABOMBA,&#13;&#10;       TOTALVENDA,&#13;&#10;       TOTALQTDEVENDA,&#13;&#10;       TOTALDESCONTO,&#13;&#10;       TOTALDIFERENCA,&#13;&#10;       TOTALDIFERENCALITRO,&#13;&#10;       TOTALCHEQUE,&#13;&#10;       VENDAOPERACAO,&#13;&#10;       TOTALPRODUTO, SALDOCAIXA,&#13;&#10;       TOTALPRODUTODESC, TOTALRECEBIDO,&#13;&#10;       SALDOTROCO,TOTALGRUPO,&#13;&#10;       RECEBIMENTO,TOTALPRAZO,&#13;&#10;       PAGAMENTO,TOTALCC,&#13;&#10;       VALORFORMA, DIFERENCA, TOTALFISICO,VALORDIFERENCA, VALORDIFERENCAN_FORM,&#13;&#10;       FCHEQUEPRE   : REAL;&#13;&#10;&#13;&#10;       TIPOCH, TIPOMOV : STRING;&#13;&#10;       DATAI, DATAF : DATE;&#13;&#10;&#13;&#10;procedure MasterHeader3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALCHEQUE := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF (&#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'D') THEN&#13;&#10;    TIPOMOV       := 'Pagamento'&#13;&#10;  ELSE IF (&#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'C') THEN&#13;&#10;    TIPOMOV       := 'Recebimento'&#13;&#10;  ELSE&#13;&#10;    TIPOMOV       := 'Extra';&#13;&#10;  TOTALGRUPO    := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportSummary1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SALDOCAIXA := 0;&#13;&#10;  TOTALFISICO          := 0;&#13;&#10;  SALDOTROCO           := ((&#60;CAIXAABERTURA.&#34;TROCOINICIAL&#34;&#62; - &#60;CAIXAABERTURA.&#34;TROCOFINAL&#34;&#62;) + (&#60;CAIXAABERTURA.&#34;REPOSICAO&#34;&#62; - &#60;CAIXAABERTURA.&#34;RETIRADA&#34;&#62;));&#13;&#10;  TOTALFISICO          := (&#60;CAIXAABERTURA.&#34;DINHEIRO&#34;&#62;+&#60;CAIXAABERTURA.&#34;CHEQUEAVISTA&#34;&#62;+&#60;CAIXAABERTURA.&#34;CHEQUEPRAZO&#34;&#62;+&#60;CAIXAABERTURA.&#34;CARTAO&#34;&#62;+&#60;CAIXAABERTURA.&#34;VENDAPRAZO&#34;&#62; + &#60;CAIXAABERTURA.&#34;CONTACORRENTE&#34;&#62; + &#60;CAIXAABERTURA.&#34;VALES&#34;&#62;+&#60;CAIXAABERTURA.&#34;OUTROS&#34;&#62; - &#60;CAIXAABERTURA.&#34;CONTRAVALE&#34;&#62;);&#13;&#10;&#13;&#10;{  VALORDIFERENCA       := FORMATFLOAT('#,##0.00', ((SALDOTROCO + RECEBIMENTO - PAGAMENTO) - TOTALFISICO));&#13;&#10;  VALORDIFERENCAN_FORM := FORMATFLOAT('0.0',      ((SALDOTROCO + RECEBIMENTO - PAGAMENTO) - TOTALFISICO));&#13;&#10;  IF (STRTOFLOAT(VALORDIFERENCAN_FORM) = 0) THEN&#13;&#10;    DIFERENCA := 'FECHAMENTO FÍSICO CONFERE COM O FECHAMENTO CONTÁBIL.'&#13;&#10;  ELSE&#13;&#10;    IF (VALORDIFERENCA &#62; 0) THEN&#13;&#10;        DIFERENCA := 'Atenção!  Diferença de SOBRA R$ '+VALORDIFERENCA+' no caixa'&#13;&#10;    ELSE&#13;&#10;        DIFERENCA := 'Atenção!  Diferença de FALTA R$ '+VALORDIFERENCA+' no caixa';&#13;&#10; }&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Band2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF &#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'D' THEN BEGIN&#13;&#10;    TOTAL      := TOTAL      - &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;    TOTALGRUPO := TOTALGRUPO + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;    PAGAMENTO  := PAGAMENTO  + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;  END&#13;&#10;  ELSE IF &#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'C' THEN BEGIN&#13;&#10;    TOTAL      := TOTAL      + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;    TOTALGRUPO := TOTALGRUPO + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;    RECEBIMENTO:= RECEBIMENTO+ &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;  END&#13;&#10;  ELSE IF &#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'E' THEN BEGIN&#13;&#10;      TOTAL      := TOTAL      + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;      TOTALGRUPO := TOTALGRUPO + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;  END;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterHeader6OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALRECEBIDO := 0;&#13;&#10;  SALDOTROCO    := ((&#60;CAIXAABERTURA.&#34;TROCOINICIAL&#34;&#62; - &#60;CAIXAABERTURA.&#34;TROCOFINAL&#34;&#62;) + (&#60;CAIXAABERTURA.&#34;REPOSICAO&#34;&#62; - &#60;CAIXAABERTURA.&#34;RETIRADA&#34;&#62;));&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData4OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALRECEBIDO := TOTALRECEBIDO + &#60;CRRECEBIDAS.&#34;VALORPAGO&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData5OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALAFERICAO  := TOTALAFERICAO + &#60;BOMBAS_MAPA.&#34;AFERICAO&#34;&#62;;&#13;&#10;     TOTALQTDEBOMBA := TOTALQTDEBOMBA + (&#60;BOMBAS_MAPA.&#34;L_FINAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;L_INICIAL_MANUAL&#34;&#62;);&#13;&#10;     TOTALVENDABOMBA:= TOTALVENDABOMBA + ((&#60;BOMBAS_MAPA.&#34;L_FINAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;L_INICIAL_MANUAL&#34;&#62;) * &#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;);&#13;&#10;     TOTALVENDA     := TOTALVENDA + (&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;) ;&#13;&#10;     TOTALQTDEVENDA := TOTALQTDEVENDA + &#60;BOMBAS_MAPA.&#34;VENDA&#34;&#62;;&#13;&#10;     TOTALDESCONTO  := TOTALDESCONTO + ((&#60;BOMBAS_MAPA.&#34;VENDA&#34;&#62;*&#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;) -(&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;));&#13;&#10;     TOTALDIFERENCALITRO := TOTALDIFERENCALITRO + ((&#60;BOMBAS_MAPA.&#34;L_FINAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;L_INICIAL_MANUAL&#34;&#62;) - &#60;BOMBAS_MAPA.&#34;VENDA&#34;&#62;);&#13;&#10;     TOTALDIFERENCA := TOTALDIFERENCA + ((((&#60;BOMBAS_MAPA.&#34;L_FINAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;L_INICIAL_MANUAL&#34;&#62;) * &#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;) - (&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;)+((&#60;BOMBAS_MAPA.&#34;VENDA&#34;&#62;*&#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;) -(&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;))));&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALAFERICAO  := 0;&#13;&#10;     TOTALQTDEBOMBA := 0;&#13;&#10;     TOTALVENDABOMBA:= 0;&#13;&#10;     TOTALVENDA     := 0;&#13;&#10;     TOTALQTDEVENDA := 0;&#13;&#10;     TOTALDESCONTO  := 0;&#13;&#10;     TOTALDIFERENCA := 0;&#13;&#10;     TOTALDIFERENCALITRO := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     VENDAPRODUTO := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData6OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Subreport2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;    VENDAOPERACAO := 0;&#13;&#10;    MOVIMENTONF.OPEN;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData7OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     VALORFORMA    := 0;&#13;&#10;     DIFERENCA     := 0;&#13;&#10;     DIFERENCA     := (VENDAOPERACAO + &#60;MOVIMENTONF.&#34;TOTALNF&#34;&#62;) - (VENDAPRODUTO + TOTALVENDA);&#13;&#10;     MOVIMENTONF.NEXT;&#13;&#10;     IF (NOT MOVIMENTONF.EOF) THEN BEGIN&#13;&#10;        MOVIMENTONF.PRIOR;&#13;&#10;        VALORFORMA    := &#60;MOVIMENTONF.&#34;TOTALNF&#34;&#62;;&#13;&#10;     END&#13;&#10;     ELSE BEGIN&#13;&#10;        VALORFORMA    := (&#60;MOVIMENTONF.&#34;TOTALNF&#34;&#62; - DIFERENCA);&#13;&#10;     END;&#13;&#10;&#13;&#10;     VENDAOPERACAO := VENDAOPERACAO + VALORFORMA;&#13;&#10;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header4OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;    TOTALCHEQUE := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData8OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALCHEQUE := TOTALCHEQUE + &#60;CHEQUE.&#34;VALOR&#34;&#62;;&#13;&#10;     IF &#60;CHEQUE.&#34;TIPOCHEQUE&#34;&#62; = 'P' THEN&#13;&#10;        TIPOCH := 'Pré-datado';&#13;&#10;     IF &#60;CHEQUE.&#34;TIPOCHEQUE&#34;&#62; = 'D' THEN&#13;&#10;        TIPOCH := 'Devolvido';&#13;&#10;     IF &#60;CHEQUE.&#34;TIPOCHEQUE&#34;&#62; = 'T' THEN&#13;&#10;        TIPOCH := 'Troca';&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header5OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALCC := 0;&#13;&#10;  TOTALPRAZO := 0;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData9OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALCC := TOTALCC + &#60;NOTAFISCALCC.&#34;VALOR&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData10OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALPRAZO := TOTALPRAZO + &#60;NOTAFISCALCR.&#34;VALORSALDO&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header7OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALPRODUTO := 0;&#13;&#10;     TOTALPRODUTODESC := 0;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData11OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;       TOTALPRODUTO     := TOTALPRODUTO + &#60;NOTAFISCALPRODUTO.&#34;TOTAL&#34;&#62;;&#13;&#10;     TOTALPRODUTODESC := TOTALPRODUTODESC + &#60;NOTAFISCALPRODUTO.&#34;DESCONTO&#34;&#62;;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  SALDOTROCO           := 0;&#13;&#10;  TOTAL := 0;&#13;&#10;  RECEBIMENTO := 0;&#13;&#10;  PAGAMENTO   := 0;&#13;&#10;&#13;&#10;  TOTAL        := 0;&#13;&#10;&#13;&#10;  fCHEQUEPRE   := 0;&#13;&#10;  MOVIMENTO_MAPA.FIRST;&#13;&#10;  Recebimentos := 0;&#13;&#10;&#13;&#10;  BOMBAS_MAPA.close;&#13;&#10;  PAGE2.VISIBLE := True;&#13;&#10;&#13;&#10;  CAIXAABERTURA.OPEN;&#13;&#10;  FUNCIONARIO.OPEN;&#13;&#10;&#13;&#10;  MOVIMENTO_MAPA.CLOSE;&#13;&#10;  MOVIMENTONF_MAPA.CLOSE;&#13;&#10;&#13;&#10;  DATAI := STRTODATE('01/' + INTTOSTR(MONTHOF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)) + '/' + INTTOSTR(YEAROF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)));&#13;&#10;  IF (MONTHOF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;) = 12) THEN&#13;&#10;    DATAF := STRTODATE('31/12/'+INTTOSTR(YEAROF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)))&#13;&#10;  ELSE&#13;&#10;    DATAF := STRTODATE('01/'+ INTTOSTR(MONTHOF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)+1) + '/' + INTTOSTR(YEAROF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)));&#13;&#10;&#13;&#10;  MOVIMENTO_MAPA.OPEN;&#13;&#10;  MOVIMENTONF_MAPA.OPEN;&#13;&#10;&#13;&#10;  If (&#60;TEM_COMBUST.&#34;ACHOU&#34;&#62; = 'TRUE') Then&#13;&#10;     BOMBAS_MAPA.OPEN;&#13;&#10;&#13;&#10;&#13;&#10;end." PropData="">
