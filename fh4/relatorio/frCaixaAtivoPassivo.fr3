    <TfrxIBXQuery Name="MOVIMENTO_MAPA" UserName="MOVIMENTO_MAPA" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT M.CODIGOLANCAMENTO, O.HISTORICO AS DESCRICAO, M.TIPOMOVIMENTO, M.STATUS,&#13;&#10;V.DESCRICAO AS DESC_VARIAVEL, SUM(M.VALOR) AS VALOR&#13;&#10;FROM MOVIMENTOCONTABIL M&#13;&#10;INNER JOIN OPERACAOAUTOMATICO O ON O.CODIGOCAIXA = M.CODIGOLANCAMENTO&#13;&#10;INNER JOIN VARIAVEL V ON V.CODIGO = O.CODIGOVARIAVEL&#13;&#10;WHERE (M.CODIGOCAIXAABERTURA = :CODIGOCAIXAABERTURA) AND M.CNPJ = :CNPJ&#13;&#10;GROUP BY M.CODIGOLANCAMENTO, O.HISTORICO, M.TIPOMOVIMENTO, M.STATUS, V.DESCRICAO&#13;&#10;ORDER BY M.CODIGOLANCAMENTO, O.HISTORICO, M.TIPOMOVIMENTO, M.STATUS, V.DESCRICAO" PropData=""/>
