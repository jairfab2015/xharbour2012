<TfrxReport Version="3.24" DotMatrixReport="False" EngineOptions.DoublePass="True" EngineOptions.UseFileCache="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4031" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PrintOptions.Printer="Padrão" ReportOptions.CreateDate="39496,4229093287" ReportOptions.Description.Text="" ReportOptions.Name="Personalizado - 0" ReportOptions.LastChange="40024,4253618634" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  DATAI, DATAF, SITUACAO : STRING;&#13;&#10;&#13;&#10;  COMISSAO, TOTALCOMISSAO : REAL;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  COMISSAO := &#60;RELATORIO.&#34;VALOR&#34;&#62; * &#60;RELATORIO.&#34;PERCENTUALCOMISSAO&#34;&#62; / 100;&#13;&#10;  TOTALCOMISSAO := TOTALCOMISSAO + COMISSAO;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALCOMISSAO := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  IF SIGHAPARAM('TIPOCONTRATO') &#60;&#62; 'Todos' THEN&#13;&#10;    RELATORIO.SQL.ADD('AND JP.TIPOPUBLICIDADE = ''' + SIGHAPARAM('TIPOCONTRATO') + '''');&#13;&#10;&#13;&#10;  IF SIGHAPARAM('VENDEDOR') &#60;&#62; '0' THEN&#13;&#10;    RELATORIO.SQL.ADD('AND F.CODIGO = ' + SIGHAPARAM('VENDEDOR'));&#13;&#10;&#13;&#10;  IF SIGHAPARAM('CLIENTE') &#60;&#62; '0' THEN&#13;&#10;    RELATORIO.SQL.ADD('AND CL.CODIGOCLIENTE = ' + SIGHAPARAM('CLIENTE'));&#13;&#10;&#13;&#10;  IF SIGHAPARAM('SITUACAO') = '0' THEN BEGIN&#13;&#10;    RELATORIO.SQL.ADD('AND JD.SITUACAO = ''N''');&#13;&#10;    SITUACAO := 'NORMAL';&#13;&#10;  END;&#13;&#10;  IF SIGHAPARAM('SITUACAO') = '1' THEN BEGIN&#13;&#10;    RELATORIO.SQL.ADD('AND JD.SITUACAO = ''P''');&#13;&#10;    SITUACAO := 'PENDENTE';&#13;&#10;  END;&#13;&#10;  IF SIGHAPARAM('SITUACAO') = '2' THEN BEGIN&#13;&#10;    RELATORIO.SQL.ADD('AND JD.SITUACAO = ''O''');&#13;&#10;    SITUACAO := 'OUTRO(A)';&#13;&#10;  END;&#13;&#10;  IF SIGHAPARAM('SITUACAO') = '3' THEN&#13;&#10;    SITUACAO := 'TODAS';&#13;&#10;&#13;&#10;  IF SIGHAPARAM('PRODUTO') &#60;&#62; '0' THEN&#13;&#10;    RELATORIO.SQL.ADD('AND JE.CODIGOPRODUTO = '+ SIGHAPARAM('PRODUTO'));&#13;&#10;&#13;&#10;  DATAI := BUSCATROCA(SIGHAPARAM('DATAI'), '/', '.');&#13;&#10;  DATAF := BUSCATROCA(SIGHAPARAM('DATAF'), '/', '.');&#13;&#10;  MEMO2.TEXT := 'PERÍODO DA EDIÇÃO: ' + SIGHAPARAM('DATAI') + ' ATÉ ' + SIGHAPARAM('DATAF') + '  SITUAÇÃO: ' + SITUACAO ;&#13;&#10;  RELATORIO.SQL.ADD('AND JD.DATA BETWEEN ''' + DATAI + ''' AND ''' + DATAF + '''');&#13;&#10;&#13;&#10;  RELATORIO.SQL.ADD('ORDER BY F.CODIGO, ');&#13;&#10;  IF SIGHAPARAM('ORDEM') = '0' THEN&#13;&#10;    RELATORIO.SQL.ADD('JD.CODIGOCONTRATO')&#13;&#10;  ELSE&#13;&#10;    RELATORIO.SQL.ADD('JP.CODIGOCLIENTE');&#13;&#10;  RELATORIO.OPEN;&#13;&#10;end." PropData="044C656674021C03546F70020808446174617365747301010C2C00000020446174615365743D2252454C41544F52494F2220446174615365744E616D653D2252454C41544F52494F220000095661726961626C65730100055374796C650100">
