    <TfrxIBXQuery Name="RELATORIO" UserName="RELATORIO" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT DISTINCT CMCR.CODIGOFUNCIONARIO AS CODIGOVENDEDOR, F.NOME AS NOMEVENDEDOR, JP.CODIGO AS CODIGOCONTRATO, CL.CODIGOCLIENTE, CL.NOME AS NOMECLIENTE, CMCR.PERCENTUALCOMISSAO, JD.DATA, JD.TAMANHO, JD.OBSERVACOES, JD.VALOR&#13;&#10;FROM JDATAS JD&#13;&#10;   INNER JOIN JPUBLICIDADE JP ON (JD.CODIGOCONTRATO = JP.CODIGO)&#13;&#10;   INNER JOIN CONTARECEBER CR ON (JP.CODIGO = CR.CONTRATOJORNAL)&#13;&#10;   INNER JOIN CLIENTE CL ON (CR.CODIGOCLIENTE = CL.CODIGOCLIENTE)&#13;&#10;   INNER JOIN COMISSAOCR CMCR ON (CR.NUMERODUPLICATA = CMCR.NUMERODUPLICATA) AND (CR.NUMEROPARCELA = CMCR.NUMEROPARCELA) AND (CR.STATUS = CMCR.STATUS) --AND (F.CODIGO = CMCR.CODIGOFUNCIONARIO)&#13;&#10;   INNER JOIN FUNCIONARIO F ON (F.CODIGO = CMCR.CODIGOFUNCIONARIO)&#13;&#10;   INNER JOIN JEDICOES JE ON (JD.CODIGOEDICAO = JE.CODIGO)&#13;&#10;&#13;&#10;WHERE CR.TIPOCONTRATO = 'P'" PropData="0A506172616D65746572730100"/>
