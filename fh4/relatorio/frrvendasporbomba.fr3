    <TfrxIBXQuery Name="QRVENDASBOMBA" UserName="QRVENDASBOMBA" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT I.DATA,I.CODIGOBOMBA, C.DESCRICAO, SUM(I.QUANTIDADE) AS QUANTIDADE,AVG(I.VALORUNITARIO) AS VALORUNITARIO, SUM(I.VALORDESCONTO) AS VALORDESCONTO,SUM(I.VALORTOTAL) AS VALORTOTAL &#13;&#10;FROM NOTAFISCALCOMBUSTIVEL I INNER JOIN COMBUSTIVEL C ON I.CODIGOCOMBUSTIVEL = C.CODIGO WHERE I.TIPOMOVIMENTO = 'S' AND I.CONTROLETIPO &#60;&#62; 'X' AND I.CNPJ = '03.655.787/0001-18' AND I.DATA BETWEEN  Cast('07/01/2007' AS DATE)  AND Cast('07/03/2007' AS DATE)&#13;&#10;AND I.STATUS = '#'&#13;&#10;GROUP BY I.CODIGOBOMBA, I.DATA, C.DESCRICAO&#13;&#10;FROM (NOTAFISCALCOMBUSTIVEL I JOIN NOTAFISCAL N ON I.CODIGONF = N.CODIGONF AND N.CNPJ = '03.655.787/0001-18')INNER JOIN COMBUSTIVEL C ON I.CODIGOCOMBUSTIVEL = C.CODIGO WHERE I.TIPOMOVIMENTO = 'S' AND N.NOTAFISCALCANCELADA = 0 AND I.CNPJ = '03.655.787/0001-18' AND N.CODIGOCAIXA =76&#13;&#10;AND I.STATUS = '#'&#13;&#10; GROUP BY I.CODIGOBOMBA, C.DESCRICAO" PropData="0A506172616D65746572730100"/>
    <TfrxIBXQuery Name="QRVENDASOPERACAO" UserName="QRVENDASOPERACAO" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT N.CODIGOOPERACAO, I.CODIGOCOMBUSTIVEL, O.DESCRICAO AS DESC_OPERACAO, C.DESCRICAO AS DESC_COMBUSTIVEL, SUM(I.QUANTIDADE) AS QUANTIDADE,AVG(I.VALORUNITARIO) AS VALORUNITARIO, SUM(I.VALORDESCONTO) AS VALORDESCONTO, SUM(I.VALORTOTAL) AS VALORTOTAL FROM NOTAFISCALCOMBUSTIVEL I INNER JOIN NOTAFISCAL N ON I.CODIGONF = N.CODIGONF AND N.CNPJ = '03.655.787/0001-18' INNER JOIN OPERACAO O ON N.CODIGOOPERACAO = O.CODIGOLANCAMENTO INNER JOIN COMBUSTIVEL C ON I.CODIGOCOMBUSTIVEL = C.CODIGO WHERE N.TIPOMOVIMENTO = 'S' AND N.NOTAFISCALCANCELADA = 0 AND I.CNPJ = '03.655.787/0001-18' &#13;&#10;AND N.CODIGOCAIXA =&#13;&#10;AND I.STATUS = '#'&#13;&#10; GROUP BY N.CODIGOOPERACAO, I.CODIGOCOMBUSTIVEL, O.DESCRICAO, C.DESCRICAO ORDER BY I.CODIGOCOMBUSTIVEL, N.CODIGOOPERACAO" PropData="0A506172616D65746572730100"/>
