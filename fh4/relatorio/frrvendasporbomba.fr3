<TfrxReport Tag="1" Version="3.24" DotMatrixReport="False" EngineOptions.DoublePass="True" EngineOptions.UseFileCache="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4031" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="37820,5533685185" ReportOptions.Description.Text="" ReportOptions.Name="VENDAS POR BOMBA" ReportOptions.LastChange="40284,4956296991" ScriptLanguage="PascalScript" ScriptText.Text="VAR QUANTIDADE, VALORTOTAL, DESCONTO, QUANTIDADEG, VALORTOTALG : REAL;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  QRVENDASBOMBA.CLOSE;&#13;&#10;&#13;&#10;  If SIGHAPARAM('MODELO') = 'I' Then Begin&#13;&#10;     PAGE1.VISIBLE := FALSE;&#13;&#10;     PAGE2.VISIBLE := FALSE;&#13;&#10;      QRVENDASBOMBA.SQL.TEXT := 'SELECT I.DATA,I.CODIGOBOMBA, C.DESCRICAO, SUM(I.QUANTIDADE) AS QUANTIDADE,' +&#13;&#10;     'AVG(I.VALORUNITARIO) AS VALORUNITARIO, SUM(I.VALORDESCONTO) AS VALORDESCONTO,' +&#13;&#10;     'SUM(I.VALORTOTAL) AS VALORTOTAL ';&#13;&#10;&#13;&#10;      QRVENDASBOMBA.SQL.TEXT := QRVENDASBOMBA.SQL.TEXT +&#13;&#10;      'FROM NOTAFISCALCOMBUSTIVEL I, NOTAFISCAL NF ' +&#13;&#10;      'INNER JOIN COMBUSTIVEL C ON I.CODIGOCOMBUSTIVEL = C.CODIGO ' +&#13;&#10;        // Comentado e add c祚igo abaixo por Fernando Brasil - 16.04.2010&#13;&#10;        // Alterado o filtro por CFOP&#13;&#10;//      'WHERE I.CODIGONF = NF.CODIGONF AND I.CANCELADOITEM = 0 AND I.TIPOMOVIMENTO = ''S'' AND I.CONTROLETIPO &#60;&#62; ''X''AND I.CODIGOFISCAL &#60;&#62; ''5.922'' ' +&#13;&#10;      'WHERE I.CODIGONF = NF.CODIGONF AND I.CANCELADOITEM = 0 AND I.TIPOMOVIMENTO = ''S'' AND I.CONTROLETIPO &#60;&#62; ''X''AND I.CODIGOFISCAL IN (''6.655'',''6.656'',''5.655'',''5.656'') ' +&#13;&#10;      //ADD BY TUBA ON 17.08.2009 - SIGHAPAF&#13;&#10;      'AND NOT (NF.TIPODOCUMENTO = ''N'' AND NF.MODELO = ''01'' AND NF.OBSERVACAO IS NOT NULL AND NF.OBSERVACAO LIKE ''%NF Ref. Cupom Fiscal%'') ' +&#13;&#10;&#13;&#10;      'AND I.CNPJ = ''' + SIGHAPARAM('CNPJ') + ''' ' +&#13;&#10;      'AND I.DATA BETWEEN  Cast(''' + COPY(SIGHAPARAM('DATAI'), 4, 3) + COPY(SIGHAPARAM('DATAI'), 1, 3) + COPY(SIGHAPARAM('DATAI'), 7, 4) + ''' AS DATE) ' +&#13;&#10;      ' AND Cast(''' + COPY(SIGHAPARAM('DATAF'), 4, 3) + COPY(SIGHAPARAM('DATAF'), 1, 3) + COPY(SIGHAPARAM('DATAF'), 7, 4) + ''' AS DATE)';&#13;&#10;&#13;&#10;      if (CBCOMB.TEXT &#60;&#62; '') THEN&#13;&#10;         QRVENDASBOMBA.SQL.TEXT := QRVENDASBOMBA.SQL.TEXT + 'AND I.CODIGOCOMBUSTIVEL = ' + IntToStr(&#60;QRCOMBUSTIVEL.&#34;CODIGO&#34;&#62;);&#13;&#10;&#13;&#10;       IF SIGHAPARAM('STATUS') = '#' THEN&#13;&#10;         QRVENDASBOMBA.SQL.TEXT := QRVENDASBOMBA.SQL.TEXT + 'AND I.STATUS = ''#''';&#13;&#10;&#13;&#10;      If SIGHAPARAM('ORDEM') = 'B' Then begin&#13;&#10;          QRVENDASBOMBA.SQL.TEXT := QRVENDASBOMBA.SQL.TEXT +  'GROUP BY I.CODIGOBOMBA, I.DATA, C.DESCRICAO';&#13;&#10;          PAGE3.VISIBLE := TRUE;&#13;&#10;          PAGE4.VISIBLE := FALSE;&#13;&#10;      End&#13;&#10;      ELSE begin&#13;&#10;          QRVENDASBOMBA.SQL.TEXT := QRVENDASBOMBA.SQL.TEXT +  'GROUP BY I.DATA,I.CODIGOBOMBA, C.DESCRICAO';&#13;&#10;          PAGE4.VISIBLE := TRUE;&#13;&#10;          PAGE3.VISIBLE := FALSE;&#13;&#10;      end;&#13;&#10;&#13;&#10;  END&#13;&#10;&#13;&#10;  ELSE BEGIN&#13;&#10;     PAGE1.VISIBLE := TRUE;&#13;&#10;     PAGE3.VISIBLE := FALSE;&#13;&#10;     PAGE4.VISIBLE := FALSE;&#13;&#10;     QRVENDASBOMBA.SQL.TEXT := 'SELECT I.CODIGOBOMBA, C.DESCRICAO, SUM(I.QUANTIDADE) AS QUANTIDADE,' +&#13;&#10;     'AVG(I.VALORUNITARIO) AS VALORUNITARIO, SUM(I.VALORDESCONTO) AS VALORDESCONTO,' +&#13;&#10;     'SUM(I.VALORTOTAL) AS VALORTOTAL ';&#13;&#10;&#13;&#10;     If SIGHAPARAM('DIATURNO') = 'D' Then Begin&#13;&#10;        QRVENDASBOMBA.SQL.TEXT := QRVENDASBOMBA.SQL.TEXT +&#13;&#10;        'FROM NOTAFISCALCOMBUSTIVEL I, NOTAFISCAL NF ' +&#13;&#10;        'INNER JOIN COMBUSTIVEL C ON I.CODIGOCOMBUSTIVEL = C.CODIGO ' +&#13;&#10;        // Comentado e add c祚igo abaixo por Fernando Brasil - 16.04.2010&#13;&#10;        // Alterado o filtro por CFOP&#13;&#10;//        'WHERE I.CODIGONF = NF.CODIGONF AND I.CANCELADOITEM = 0 AND I.TIPOMOVIMENTO = ''S'' AND I.CONTROLETIPO &#60;&#62; ''X'' AND I.CODIGOFISCAL &#60;&#62; ''5.922'' ' +&#13;&#10;        'WHERE I.CODIGONF = NF.CODIGONF AND I.CANCELADOITEM = 0 AND I.TIPOMOVIMENTO = ''S'' AND I.CONTROLETIPO &#60;&#62; ''X'' AND I.CODIGOFISCAL IN (''6.655'',''6.656'',''5.655'',''5.656'') ' +&#13;&#10;        //ADD BY TUBA ON 17.08.2009 - SIGHAPAF&#13;&#10;        'AND NOT (NF.TIPODOCUMENTO = ''N'' AND NF.MODELO = ''01'' AND NF.OBSERVACAO IS NOT NULL AND NF.OBSERVACAO LIKE ''%NF Ref. Cupom Fiscal%'') ' +&#13;&#10;        'AND I.CNPJ = ''' + SIGHAPARAM('CNPJ') + ''' ' +&#13;&#10;        'AND I.DATA = Cast(''' + COPY(SIGHAPARAM('PARAMETRO'), 4, 3) + COPY(SIGHAPARAM('PARAMETRO'), 1, 3) + COPY(SIGHAPARAM('PARAMETRO'), 7, 4) + ''' AS DATE) ';&#13;&#10;         IF SIGHAPARAM('STATUS') = '#' THEN&#13;&#10;           QRVENDASBOMBA.SQL.TEXT := QRVENDASBOMBA.SQL.TEXT + 'AND I.STATUS = ''#''';&#13;&#10;&#13;&#10;        QRVENDASBOMBA.SQL.TEXT := QRVENDASBOMBA.SQL.TEXT + 'GROUP BY I.CODIGOBOMBA, C.DESCRICAO';&#13;&#10;      End&#13;&#10;      Else Begin&#13;&#10;        QRVENDASBOMBA.SQL.TEXT := QRVENDASBOMBA.SQL.TEXT +&#13;&#10;        'FROM (NOTAFISCALCOMBUSTIVEL I JOIN NOTAFISCAL N ON I.CODIGONF = N.CODIGONF AND I.CANCELADOITEM = 0 AND N.CNPJ = ''' + SIGHAPARAM('CNPJ') + ''')' +&#13;&#10;        'INNER JOIN COMBUSTIVEL C ON I.CODIGOCOMBUSTIVEL = C.CODIGO ' +&#13;&#10;        // Comentado e add c祚igo abaixo por Fernando Brasil - 16.04.2010&#13;&#10;        // Alterado o filtro por CFOP&#13;&#10;//        'WHERE I.TIPOMOVIMENTO = ''S'' AND I.CODIGOFISCAL &#60;&#62; ''5.922'' AND N.NOTAFISCALCANCELADA = 0 ' +&#13;&#10;        'WHERE I.TIPOMOVIMENTO = ''S'' AND I.CODIGOFISCAL IN (''6.655'',''6.656'',''5.655'',''5.656'') AND N.NOTAFISCALCANCELADA = 0 ' +&#13;&#10;        //ADD BY TUBA ON 17.08.2009 - SIGHAPAF&#13;&#10;        'AND NOT (N.TIPODOCUMENTO = ''N'' AND N.MODELO = ''01'' AND N.OBSERVACAO IS NOT NULL AND N.OBSERVACAO LIKE ''%NF Ref. Cupom Fiscal%'') ' +&#13;&#10;        'AND I.CNPJ = ''' + SIGHAPARAM('CNPJ') + ''' ' +&#13;&#10;        'AND N.CODIGOCAIXA =' + SIGHAPARAM('PARAMETRO');&#13;&#10;         IF SIGHAPARAM('STATUS') = '#' THEN&#13;&#10;           QRVENDASBOMBA.SQL.TEXT := QRVENDASBOMBA.SQL.TEXT + 'AND I.STATUS = ''#''';&#13;&#10;&#13;&#10;        QRVENDASBOMBA.SQL.TEXT := QRVENDASBOMBA.SQL.TEXT + ' GROUP BY I.CODIGOBOMBA, C.DESCRICAO';&#13;&#10;      End;&#13;&#10;&#13;&#10;  END;&#13;&#10;&#13;&#10;  QRVENDASBOMBA.OPEN;&#13;&#10;&#13;&#10;  QRVENDASOPERACAO.CLOSE;&#13;&#10;  QRVENDASOPERACAO.SQL.TEXT := 'SELECT N.CODIGOOPERACAO, I.CODIGOCOMBUSTIVEL, O.DESCRICAO AS DESC_OPERACAO, C.DESCRICAO AS DESC_COMBUSTIVEL, SUM(I.QUANTIDADE) AS QUANTIDADE,'+&#13;&#10;  'AVG(I.VALORUNITARIO) AS VALORUNITARIO, SUM(I.VALORDESCONTO) AS VALORDESCONTO, SUM(I.VALORTOTAL) AS VALORTOTAL '+&#13;&#10;  'FROM NOTAFISCALCOMBUSTIVEL I '+&#13;&#10;  'INNER JOIN NOTAFISCAL N ON I.CODIGONF = N.CODIGONF AND N.CNPJ = ''' + SIGHAPARAM('CNPJ') + ''' ' +&#13;&#10;  'INNER JOIN OPERACAO O ON N.CODIGOOPERACAO = O.CODIGOLANCAMENTO ' +&#13;&#10;  'INNER JOIN COMBUSTIVEL C ON I.CODIGOCOMBUSTIVEL = C.CODIGO ' +&#13;&#10;  'WHERE I.CANCELADOITEM = 0 AND N.TIPOMOVIMENTO = ''S'' AND N.NOTAFISCALCANCELADA = 0 AND I.CODIGOFISCAL &#60;&#62; ''5.922'' ' +&#13;&#10;  //ADD BY TUBA ON 17.08.2009 - SIGHAPAF&#13;&#10;  'AND NOT (N.TIPODOCUMENTO = ''N'' AND N.MODELO = ''01'' AND N.OBSERVACAO IS NOT NULL AND N.OBSERVACAO LIKE ''%NF Ref. Cupom Fiscal%'') ' +&#13;&#10;  'AND I.CNPJ = ''' + SIGHAPARAM('CNPJ') + ''' ';&#13;&#10;&#13;&#10;  If SIGHAPARAM('DIATURNO') = 'D' Then&#13;&#10;    QRVENDASOPERACAO.SQL.TEXT := QRVENDASOPERACAO.SQL.TEXT + 'AND N.DATACONTABIL = Cast(''' + COPY(SIGHAPARAM('PARAMETRO'), 4, 3) + COPY(SIGHAPARAM('PARAMETRO'), 1, 3) + COPY(SIGHAPARAM('PARAMETRO'), 7, 4) + ''' AS DATE)'&#13;&#10;  Else&#13;&#10;    QRVENDASOPERACAO.SQL.TEXT := QRVENDASOPERACAO.SQL.TEXT + 'AND N.CODIGOCAIXA =' + SIGHAPARAM('PARAMETRO');&#13;&#10;&#13;&#10;  IF SIGHAPARAM('STATUS') = '#' THEN&#13;&#10;    QRVENDASOPERACAO.SQL.TEXT := QRVENDASOPERACAO.SQL.TEXT + 'AND I.STATUS = ''#''';&#13;&#10;&#13;&#10;  QRVENDASOPERACAO.SQL.TEXT := QRVENDASOPERACAO.SQL.TEXT +' GROUP BY N.CODIGOOPERACAO, I.CODIGOCOMBUSTIVEL, O.DESCRICAO, C.DESCRICAO '+&#13;&#10;  'ORDER BY I.CODIGOCOMBUSTIVEL, N.CODIGOOPERACAO';&#13;&#10;  QRVENDASOPERACAO.OPEN;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  QUANTIDADE := 0;&#13;&#10;  VALORTOTAL := 0;&#13;&#10;  DESCONTO   := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  QUANTIDADE := QUANTIDADE + &#60;QRVENDASBOMBA.&#34;QUANTIDADE&#34;&#62;;&#13;&#10;  VALORTOTAL := VALORTOTAL + &#60;QRVENDASBOMBA.&#34;VALORTOTAL&#34;&#62;;&#13;&#10;  DESCONTO   := DESCONTO   + &#60;QRVENDASBOMBA.&#34;VALORDESCONTO&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Band1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  QUANTIDADE := 0;&#13;&#10;  VALORTOTAL := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Cabe_alhoDe_Grupo1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  QUANTIDADEG := 0;&#13;&#10;  VALORTOTALG := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DadosMestre1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  QUANTIDADE  := QUANTIDADE  + &#60;QRVENDASOPERACAO.&#34;QUANTIDADE&#34;&#62;;&#13;&#10;  VALORTOTAL  := VALORTOTAL  + &#60;QRVENDASOPERACAO.&#34;VALORTOTAL&#34;&#62;;&#13;&#10;  QUANTIDADEG := QUANTIDADEG + &#60;QRVENDASOPERACAO.&#34;QUANTIDADE&#34;&#62;;&#13;&#10;  VALORTOTALG := VALORTOTALG + &#60;QRVENDASOPERACAO.&#34;VALORTOTAL&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  QUANTIDADE := 0;&#13;&#10;  VALORTOTAL := 0;&#13;&#10;  DESCONTO   := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DialogFormOnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  QRCOMBUSTIVEL.OPEN;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;end." PropData="">
