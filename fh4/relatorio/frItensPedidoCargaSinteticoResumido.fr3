    <TfrxIBXQuery Name="Carga" UserName="Carga" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select C.*,  ci.cidade, fo.nome, rg.descricao&#13;&#10;from cargaromaneio c, cidade ci, fornecedor fo, regiao rg&#13;&#10;where ci.codigo = c.codigodestino&#13;&#10;and fo.codigo = c.codigotransportador&#13;&#10;and c.codigoregiao = rg.codigo&#13;&#10;and c.codigo = :carga" PropData="0A506172616D657465727301010C42000000204E616D653D226361726761222044617461547970653D226674537472696E67222045787072657373696F6E3D227369676861706172616D282763617267612729220000"/>
    <TfrxIBXQuery Name="cargapedido" UserName="cargapedido" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select codigopedido&#13;&#10;from cargapedido&#13;&#10;where codigocarga = :carga" PropData="0A506172616D657465727301010C42000000204E616D653D226361726761222044617461547970653D226674537472696E67222045787072657373696F6E3D227369676861706172616D282763617267612729220000"/>
    <TfrxIBXQuery Name="PEDIDO" UserName="PEDIDO" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select  distinct pp.codigoproduto, Sum(pp.quantidade) as QTDA, pp.unidade, pp.descricaoproduto, pr.peso, um.descricao as unidadedescricao&#13;&#10;from pedido p, pedidoproduto pp, produto pr, unidademedida um&#13;&#10;where p.codigo = pp.codigopedido&#13;&#10;and p.pedidotipo = 'V'&#13;&#10;--and p.CONVERTEPARA = 'P'&#13;&#10;and p.pedidocarga = '1'&#13;&#10;and p.codigocarga = :codigocarga&#13;&#10;and pr.codigo = pp.codigoproduto&#13;&#10;and pp.unidade = um.unidade&#13;&#10;group by pp.codigoproduto, pp.descricaoproduto, pp.unidade, pr.peso, um.descricao&#13;&#10;order by pp.unidade, pp.descricaoproduto" PropData="0A506172616D657465727301010C48000000204E616D653D22636F6469676F6361726761222044617461547970653D226674537472696E67222045787072657373696F6E3D227369676861706172616D282763617267612729220000"/>
    <TfrxIBXQuery Name="TOTALDEPEDIDOS" UserName="TOTALDEPEDIDOS" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select count(DISTINCT (p.pedido))&#13;&#10;&#13;&#10;&#13;&#10;from pedido p, pedidoproduto pp, produto pr&#13;&#10;where p.codigo = pp.codigopedido&#13;&#10;and p.pedidotipo = 'V'&#13;&#10;and p.pedidocarga = '1'&#13;&#10;and p.codigocarga = :codigocarga&#13;&#10;and pr.codigo = pp.codigoproduto" PropData="0A506172616D657465727301010C48000000204E616D653D22636F6469676F6361726761222044617461547970653D226674537472696E67222045787072657373696F6E3D227369676861706172616D282763617267612729220000"/>
    <TfrxIBXQuery Name="BONIFICACAO" UserName="BONIFICACAO" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select  distinct pp.codigoproduto,&#13;&#10;                 Sum(pp.quantidade) as QTDA,&#13;&#10;                 pp.unidade,&#13;&#10;                 pp.descricaoproduto,&#13;&#10;                 pr.peso,&#13;&#10;                 Sum(pp.valortotal + pp.valoripi + pp.valoricmssub) as ValorTotal,&#13;&#10;                 cl.codigo as classificacao_codigo,&#13;&#10;                 cl.descricao as classificacao_descricao,&#13;&#10;                 li.codigo as linha_codigo,&#13;&#10;                 li.descricao as linha_descricao&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;from pedido p, pedidoproduto pp, produto pr, classificacaoproduto cl, linha li&#13;&#10;where p.codigo = pp.codigopedido&#13;&#10;and p.pedidotipo = 'V'&#13;&#10;--and p.CONVERTEPARA = 'P'&#13;&#10;and p.pedidocarga = '1'&#13;&#10;and p.codigooperacao = 1910&#13;&#10;and p.codigocarga = :codigocarga&#13;&#10;and pr.codigo = pp.codigoproduto&#13;&#10;and pr.classificacao = cl.codigo&#13;&#10;and pr.linha = li.codigo&#13;&#10;&#13;&#10;group by pp.codigoproduto, pp.descricaoproduto, pp.unidade, pr.peso, li.codigo, li.descricao, cl.codigo, cl.descricao&#13;&#10;--group by cl.codigo, cl.descricao, pp.codigoproduto, pp.descricaoproduto, pp.unidade, pr.peso&#13;&#10;order by cl.descricao, li.descricao, pp.descricaoproduto" PropData="0A506172616D657465727301010C48000000204E616D653D22636F6469676F6361726761222044617461547970653D226674537472696E67222045787072657373696F6E3D227369676861706172616D282763617267612729220000"/>
    <TfrxIBXQuery Name="TROCA" UserName="TROCA" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select  distinct pp.codigoproduto,&#13;&#10;                 Sum(pp.quantidade) as QTDA,&#13;&#10;                 pp.unidade,&#13;&#10;                 pp.descricaoproduto,&#13;&#10;                 pr.peso,&#13;&#10;                 Sum(pp.valortotal + pp.valoripi + pp.valoricmssub) as ValorTotal,&#13;&#10;                 cl.codigo as classificacao_codigo,&#13;&#10;                 cl.descricao as classificacao_descricao,&#13;&#10;                 li.codigo as linha_codigo,&#13;&#10;                 li.descricao as linha_descricao&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;from pedido p, pedidoproduto pp, produto pr, classificacaoproduto cl, linha li&#13;&#10;where p.codigo = pp.codigopedido&#13;&#10;and p.pedidotipo = 'V'&#13;&#10;--and p.CONVERTEPARA = 'P'&#13;&#10;and p.pedidocarga = '1'&#13;&#10;AND p.codigooperacao in ('1108', '1109', '1110','1111')&#13;&#10;and p.codigocarga = :codigocarga&#13;&#10;and pr.codigo = pp.codigoproduto&#13;&#10;and pr.classificacao = cl.codigo&#13;&#10;and pr.linha = li.codigo&#13;&#10;&#13;&#10;group by pp.codigoproduto, pp.descricaoproduto, pp.unidade, pr.peso, li.codigo, li.descricao, cl.codigo, cl.descricao&#13;&#10;--group by cl.codigo, cl.descricao, pp.codigoproduto, pp.descricaoproduto, pp.unidade, pr.peso&#13;&#10;order by cl.descricao, li.descricao, pp.descricaoproduto" PropData="0A506172616D657465727301010C48000000204E616D653D22636F6469676F6361726761222044617461547970653D226674537472696E67222045787072657373696F6E3D227369676861706172616D282763617267612729220000"/>
