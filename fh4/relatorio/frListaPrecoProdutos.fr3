    <TfrxIBXQuery Name="PRECOVENDA" UserName="PRECOVENDA" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT P.CODIGO, P.NOME, PM.PRECOVENDA,&#13;&#10;&#13;&#10;(CASE WHEN (PM.PRECOVENDA &#60;= (SELECT PIE.VALORBC FROM PAUTASICMS PI, PAUTASICMSESTADO PIE  WHERE PIE.CODIGOPAUTASICMS = PI.CODIGO AND PI.CODIGO = PM.PAUTAICMSSUBSTITUTOVALOR AND PI.TIPOPAUTA = 3)) THEN&#13;&#10;(((((SELECT PIE.VALORBC FROM PAUTASICMS PI, PAUTASICMSESTADO PIE WHERE PIE.CODIGOPAUTASICMS = PI.CODIGO AND PI.CODIGO = PM.PAUTAICMSSUBSTITUTOVALOR AND PI.TIPOPAUTA = 3) - PM.PRECOVENDA) * (TI.PERCENTUAL / 100)) * (TI.CARGATRIBUTARIA / 100)) * (TI.ALIQUOTA / 100))&#13;&#10;ELSE&#13;&#10;NULL END) AS VALORICMSSUB,&#13;&#10;&#13;&#10;(CASE WHEN (PM.PRECOVENDA &#60;= (SELECT PIE.VALORBC FROM PAUTASICMS PI, PAUTASICMSESTADO PIE WHERE PIE.CODIGOPAUTASICMS = PI.CODIGO AND PI.CODIGO = PM.PAUTAICMSSUBSTITUTOVALOR AND PI.TIPOPAUTA = 3)) THEN&#13;&#10;NULL&#13;&#10;ELSE&#13;&#10;(((((SELECT (((PIE.VALORBC / 100) + 1) * PM.PRECOVENDA) FROM PAUTASICMS PI, PAUTASICMSESTADO PIE WHERE PIE.CODIGOPAUTASICMS = PI.CODIGO AND PI.CODIGO = PM.PAUTAICMSSUBSTITUTOPERC AND PI.TIPOPAUTA = 4) - PM.PRECOVENDA) * (TI.PERCENTUAL / 100)) * (TI.CARGATRIBUTARIA / 100)) * (TI.ALIQUOTA / 100))&#13;&#10;END) AS VALORICMSSUBPERC,&#13;&#10;&#13;&#10;(SELECT PIE.VALORBC FROM PAUTASICMS PI, PAUTASICMSESTADO PIE WHERE PIE.CODIGOPAUTASICMS = PI.CODIGO AND PI.CODIGO = PM.PAUTAIPIVALOR AND PI.TIPOPAUTA = 5) AS VALORIPI,&#13;&#10;&#13;&#10;(CASE WHEN (PM.PRECOVENDA &#60;= (SELECT PIE.VALORBC FROM PAUTASICMS PI, PAUTASICMSESTADO PIE WHERE PIE.CODIGOPAUTASICMS = PI.CODIGO AND PI.CODIGO = PM.PAUTAICMSSUBSTITUTOVALOR AND PI.TIPOPAUTA = 3)) THEN&#13;&#10;PM.PRECOVENDA +&#13;&#10;(((((SELECT PIE.VALORBC FROM PAUTASICMS PI, PAUTASICMSESTADO PIE WHERE PIE.CODIGOPAUTASICMS = PI.CODIGO AND PI.CODIGO = PM.PAUTAICMSSUBSTITUTOVALOR AND PI.TIPOPAUTA = 3) - PM.PRECOVENDA) * (TI.PERCENTUAL / 100)) * (TI.CARGATRIBUTARIA / 100)) * (TI.ALIQUOTA / 100)) +&#13;&#10;(SELECT PIE.VALORBC FROM PAUTASICMS PI, PAUTASICMSESTADO PIE WHERE PIE.CODIGOPAUTASICMS = PI.CODIGO AND PI.CODIGO = PM.PAUTAIPIVALOR AND PI.TIPOPAUTA = 5)&#13;&#10;ELSE&#13;&#10;PM.PRECOVENDA +&#13;&#10;(((((SELECT (((PIE.VALORBC / 100) + 1) * PM.PRECOVENDA) FROM PAUTASICMS PI, PAUTASICMSESTADO PIE WHERE PIE.CODIGOPAUTASICMS = PI.CODIGO AND PI.CODIGO = PM.PAUTAICMSSUBSTITUTOPERC AND PI.TIPOPAUTA = 4) - PM.PRECOVENDA) * (TI.PERCENTUAL / 100)) * (TI.CARGATRIBUTARIA / 100)) * (TI.ALIQUOTA / 100)) +&#13;&#10;(SELECT PIE.VALORBC FROM PAUTASICMS PI, PAUTASICMSESTADO PIE WHERE PIE.CODIGOPAUTASICMS = PI.CODIGO AND PI.CODIGO = PM.PAUTAIPIVALOR AND PI.TIPOPAUTA = 5)&#13;&#10;END) AS PRECOVENDAFINAL&#13;&#10;&#13;&#10;FROM PRODUTO P, PRODUTOMULTI PM, TRIBUTACAOICMS T, TRIBUTACAOICMSITENS TI, ESTADO E, REGIAOTRIBUTARIA R&#13;&#10;WHERE P.CODIGO = PM.CODIGOPRODUTO&#13;&#10;AND PM.TRIBUTACAOICMS = T.CODIGO&#13;&#10;AND E.SIGLA = (SELECT ESTADO FROM EMPRESA WHERE CNPJ = :CNPJ)&#13;&#10;AND R.CODIGO = E.REGIAOTRIBUTARIA&#13;&#10;AND TI.CODIGOREGIAO = R.CODIGO&#13;&#10;AND T.CODIGO = TI.CODIGO&#13;&#10;AND PM.ATIVO = 1&#13;&#10;AND PM.CNPJ = :CNPJ&#13;&#10;ORDER BY P.NOME" PropData=""/>
