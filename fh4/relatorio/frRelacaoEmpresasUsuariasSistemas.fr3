    <TfrxIBXQuery Name="QRCONSULTA" UserName="QRCONSULTA" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT DISTINCT C.CODIGOCLIENTE, C.APELIDO, CS.LICENCAS, C.COMERCIALFONE,&#13;&#10;C.EMAIL, C.MSN, C.CONTATO, (SW.CODIGO) AS CODIGOSOFTWARE,&#13;&#10;(CASE WHEN C.LIBERACAO = 0 THEN 'B' ELSE 'L' END) AS SITUACAO, SW.DESCRICAO,&#13;&#10;(CASE WHEN&#13;&#10;    (SELECT SUM(CASE WHEN CE.DATAULTEXEC + C.LIBERACAO &#60; CURRENT_TIMESTAMP THEN 1 ELSE 0 END)&#13;&#10;     FROM SCS_CLIENTEEQUIPAMENTO CE&#13;&#10;     WHERE CE.CODIGOCLIENTE = C.CODIGOCLIENTE&#13;&#10;     AND CE.CODIGOSOFTWARE = SW.CODIGO) IS NULL&#13;&#10; THEN 0&#13;&#10; ELSE&#13;&#10;    (SELECT SUM(CASE WHEN CE.DATAULTEXEC + C.LIBERACAO &#60; CURRENT_TIMESTAMP THEN 1 ELSE 0 END)&#13;&#10;     FROM SCS_CLIENTEEQUIPAMENTO CE&#13;&#10;     WHERE CE.CODIGOCLIENTE = C.CODIGOCLIENTE&#13;&#10;     AND CE.CODIGOSOFTWARE = SW.CODIGO)&#13;&#10; END) AS QTDE&#13;&#10;FROM CLIENTE C, CLIENTELANCAMENTO L, SERVICO S, SCS_CLIENTESOFTWARE CS, SCS_SOFTWARE SW&#13;&#10;WHERE L.CODIGOCLIENTE = C.CODIGOCLIENTE&#13;&#10;AND L.CODIGOSERVICO = S.CODIGO&#13;&#10;AND C.CODIGOCLIENTE &#60;&#62; 0&#13;&#10;AND S.CODIGO IN (8,74,100,101,102,104,105,106,107,108,109,125,126,127,128,129,130,143,153,155,157,169)&#13;&#10;AND CS.CODIGOCLIENTE = C.CODIGOCLIENTE&#13;&#10;AND CS.CODIGOSOFTWARE = SW.CODIGO&#13;&#10;AND SW.CODIGO = :CODIGOSOFTWARE&#13;&#10;ORDER BY 1" PropData="0A506172616D657465727301010C55000000204E616D653D22434F4449474F534F465457415245222044617461547970653D226674496E7465676572222045787072657373696F6E3D225349474841504152414D2827434F4449474F534F4654574152452729220000"/>
