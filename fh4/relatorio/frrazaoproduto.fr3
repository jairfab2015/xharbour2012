<TfrxReport Version="3.24" DotMatrixReport="False" EngineOptions.UseFileCache="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4031" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="38061,4215997917" ReportOptions.Description.Text="" ReportOptions.Name="RAZÃO DE PRODUTOS" ReportOptions.LastChange="40392,7065369907" ScriptLanguage="PascalScript" ScriptText.Text="Var   SALDO, ENTRADAS, SAIDAS : Real;&#13;&#10;      CUSTO_ACUM , CUSTOTOTAL_ACUM : REAL;&#13;&#10;      TEMDADO : BOOLEAN;&#13;&#10;procedure Page1OnShow(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if (SIGHAPARAM('DATAI') = '01/01/1990') and (SIGHAPARAM('DATAF') = '31/12/2029') then begin&#13;&#10;&#13;&#10;    MOVIMENTOPRODUTO.CLOSE;&#13;&#10;    MOVIMENTOPRODUTO.SQL.TEXT := 'SELECT * FROM RAZAOPRODUTO(:PRODUTOI, :PRODUTOF, :DATAI, :DATAF, :CNPJ, :STATUS, :LINHA, :CLASSIFICACAO, :ORIGEM, :LOCAL, '+SIGHAPARAM('GRUPO')+')';&#13;&#10;&#13;&#10;    IF SIGHAPARAM('TIPOMOVIMENTO') = 'E' THEN&#13;&#10;       MOVIMENTOPRODUTO.SQL.TEXT := MOVIMENTOPRODUTO.SQL.TEXT + ' WHERE TIPOMOVIMENTO IN (''E'', ''ED'', ''ET'', ''EA'') '&#13;&#10;    ELSE IF SIGHAPARAM('TIPOMOVIMENTO') = 'S' THEN&#13;&#10;       MOVIMENTOPRODUTO.SQL.TEXT := MOVIMENTOPRODUTO.SQL.TEXT + ' WHERE TIPOMOVIMENTO IN (''S'', ''SD'', ''ST'', ''SC'', ''SA'') ';&#13;&#10;&#13;&#10;    IF (RBPRODUTO.CHECKED = TRUE) THEN&#13;&#10;       MOVIMENTOPRODUTO.SQL.TEXT := MOVIMENTOPRODUTO.SQL.TEXT + ' ORDER BY CODIGO, DATA'&#13;&#10;    ELSE IF (RBLINHA.CHECKED = TRUE) THEN&#13;&#10;       MOVIMENTOPRODUTO.SQL.TEXT := MOVIMENTOPRODUTO.SQL.TEXT + ' ORDER BY LINHA, CODIGO, DATA'&#13;&#10;    ELSE IF (RBCLASSIFICACAO.CHECKED = TRUE) THEN&#13;&#10;       MOVIMENTOPRODUTO.SQL.TEXT := MOVIMENTOPRODUTO.SQL.TEXT + ' ORDER BY CLASSIFICACAO, CODIGO, DATA'&#13;&#10;    ELSE IF (RBLOCALESTOQUE.CHECKED = TRUE) THEN&#13;&#10;       MOVIMENTOPRODUTO.SQL.TEXT := MOVIMENTOPRODUTO.SQL.TEXT + ' ORDER BY LOCAL, CODIGO, DATA';&#13;&#10;&#13;&#10; //   BUTTON1.ENABLED        := FALSE;&#13;&#10;    DIALOG1.MODALRESULT    := mrok;&#13;&#10;    MOVIMENTOPRODUTO.OPEN;&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SALDO           := 0;&#13;&#10;&#13;&#10;  IF (RBPRODUTO.CHECKED = TRUE) THEN BEGIN&#13;&#10;    GRUPO1.VISIBLE       := FALSE;&#13;&#10;    GRUPO2.CONDITION     := '&#60;MOVIMENTOPRODUTO.&#34;CODIGO&#34;&#62;';&#13;&#10;    FOOTER2.VISIBLE      := FALSE;&#13;&#10;  END&#13;&#10;  ELSE IF (RBAGRUPADOLINHA.CHECKED = TRUE) THEN BEGIN&#13;&#10;    GRUPO2.CONDITION      := '&#60;MOVIMENTOPRODUTO.&#34;LINHA&#34;&#62;';&#13;&#10;//    GRUPO1LABEL.TEXT := 'LINHA: [MOVIMENTOPRODUTO.&#34;LINHA&#34;] - [MOVIMENTOPRODUTO.&#34;NOMELINHA&#34;]';&#13;&#10;    DADOS.VISIBLE    := FALSE;&#13;&#10;    GRUPO1.VISIBLE   := FALSE;&#13;&#10;    GRUPO2.VISIBLE   := FALSE;&#13;&#10;    MEMO37.VISIBLE   := TRUE;&#13;&#10;  END&#13;&#10;  ELSE IF (RBLINHA.CHECKED = TRUE) THEN BEGIN&#13;&#10;    GRUPO1.CONDITION      := '&#60;MOVIMENTOPRODUTO.&#34;LINHA&#34;&#62;';&#13;&#10;    GRUPO1LABEL.TEXT := 'LINHA: [MOVIMENTOPRODUTO.&#34;LINHA&#34;] - [MOVIMENTOPRODUTO.&#34;NOMELINHA&#34;]';&#13;&#10;    GRUPO2.CONDITION := '&#60;MOVIMENTOPRODUTO.&#34;CODIGO&#34;&#62;';&#13;&#10;  END&#13;&#10;  ELSE IF (RBCLASSIFICACAO.CHECKED = TRUE) THEN BEGIN&#13;&#10;    GRUPO1.CONDITION      := '&#60;MOVIMENTOPRODUTO.&#34;CLASSIFICACAO&#34;&#62;';&#13;&#10;    GRUPO1LABEL.TEXT := 'CLASSIFICAÇÃO: [MOVIMENTOPRODUTO.&#34;CLASSIFICACAO&#34;] - [MOVIMENTOPRODUTO.&#34;NOMECLASSIFICACAO&#34;]';&#13;&#10;    GRUPO2.CONDITION := '&#60;MOVIMENTOPRODUTO.&#34;CODIGO&#34;&#62;';&#13;&#10;  END&#13;&#10;  ELSE IF (RBLOCALESTOQUE.CHECKED = TRUE) THEN BEGIN&#13;&#10;    GRUPO1.CONDITION      := '&#60;MOVIMENTOPRODUTO.&#34;LOCAL&#34;&#62;';&#13;&#10;    GRUPO1LABEL.TEXT := 'LOCAL DE ESTOQUE: [MOVIMENTOPRODUTO.&#34;LOCAL&#34;] - [MOVIMENTOPRODUTO.&#34;LOCALNOME&#34;]';&#13;&#10;    GRUPO2.CONDITION := '&#60;MOVIMENTOPRODUTO.&#34;CODIGO&#34;&#62;';&#13;&#10;  END;&#13;&#10;&#13;&#10;  FOOTER2.VISIBLE := (CBTOTALIZA.CHECKED = TRUE);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Button1OnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  MOVIMENTOPRODUTO.CLOSE;&#13;&#10;  MOVIMENTOPRODUTO.SQL.TEXT := 'SELECT * FROM RAZAOPRODUTO(:PRODUTOI, :PRODUTOF, :DATAI, :DATAF, :CNPJ, :STATUS, :LINHA, :CLASSIFICACAO, :ORIGEM, :LOCAL, '+ SIGHAPARAM('GRUPO')+') ';&#13;&#10;&#13;&#10;  IF SIGHAPARAM('TIPOMOVIMENTO') = 'E' THEN&#13;&#10;    MOVIMENTOPRODUTO.SQL.TEXT := MOVIMENTOPRODUTO.SQL.TEXT + ' WHERE TIPOMOVIMENTO STARTING ''E'''&#13;&#10;  ELSE IF SIGHAPARAM('TIPOMOVIMENTO') = 'S' THEN&#13;&#10;    MOVIMENTOPRODUTO.SQL.TEXT := MOVIMENTOPRODUTO.SQL.TEXT + ' WHERE TIPOMOVIMENTO STARTING ''S''';&#13;&#10;&#13;&#10;  IF (RBPRODUTO.CHECKED = TRUE) THEN&#13;&#10;    MOVIMENTOPRODUTO.SQL.TEXT := MOVIMENTOPRODUTO.SQL.TEXT + ' ORDER BY CODIGO, DATA'&#13;&#10;  ELSE IF (RBLINHA.CHECKED = TRUE) OR (RBAGRUPADOLINHA.CHECKED) THEN&#13;&#10;    MOVIMENTOPRODUTO.SQL.TEXT := MOVIMENTOPRODUTO.SQL.TEXT + ' ORDER BY LINHA, CODIGO, DATA'&#13;&#10;  ELSE IF (RBCLASSIFICACAO.CHECKED = TRUE) THEN&#13;&#10;    MOVIMENTOPRODUTO.SQL.TEXT := MOVIMENTOPRODUTO.SQL.TEXT + ' ORDER BY CLASSIFICACAO, CODIGO, DATA'&#13;&#10;  ELSE IF (RBLOCALESTOQUE.CHECKED = TRUE) THEN&#13;&#10;    MOVIMENTOPRODUTO.SQL.TEXT := MOVIMENTOPRODUTO.SQL.TEXT + ' ORDER BY LOCAL, CODIGO, DATA';&#13;&#10;&#13;&#10;  BUTTON1.ENABLED        := FALSE;&#13;&#10;  MOVIMENTOPRODUTO.OPEN;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DADOSOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF COPY(&#60;MOVIMENTOPRODUTO.&#34;TIPOMOVIMENTO&#34;&#62;, 1, 1) = 'E' THEN BEGIN&#13;&#10;    SALDO          := SALDO + &#60;MOVIMENTOPRODUTO.&#34;QUANTIDADE&#34;&#62;;&#13;&#10;    ENTRADAS       := ENTRADAS + &#60;MOVIMENTOPRODUTO.&#34;QUANTIDADE&#34;&#62;;&#13;&#10;    DADOS.VISIBLE  := CBMOSTRAMOVIMENTO.CHECKED;&#13;&#10;  END&#13;&#10;  ELSE IF COPY(&#60;MOVIMENTOPRODUTO.&#34;TIPOMOVIMENTO&#34;&#62;, 1, 1) = 'S' THEN BEGIN&#13;&#10;    SALDO          := SALDO - &#60;MOVIMENTOPRODUTO.&#34;QUANTIDADE&#34;&#62;;&#13;&#10;    SAIDAS         := SAIDAS + &#60;MOVIMENTOPRODUTO.&#34;QUANTIDADE&#34;&#62;;&#13;&#10;    DADOS.VISIBLE  := CBMOSTRAMOVIMENTO.CHECKED;&#13;&#10;  END&#13;&#10;  ELSE IF COPY(&#60;MOVIMENTOPRODUTO.&#34;TIPOMOVIMENTO&#34;&#62;, 1, 1) = 'A' THEN BEGIN&#13;&#10;    SALDO          := SALDO + &#60;MOVIMENTOPRODUTO.&#34;QUANTIDADE&#34;&#62;;&#13;&#10;    DADOS.VISIBLE  := FALSE;&#13;&#10;  END;&#13;&#10;  CUSTOTOTAL_ACUM := CUSTOTOTAL_ACUM + &#60;MOVIMENTOPRODUTO.&#34;CUSTO&#34;&#62;;&#13;&#10;&#13;&#10;&#13;&#10;  DADOS.VISIBLE := NOT RBAGRUPADOLINHA.CHECKED;&#13;&#10;&#13;&#10;  IF SIGHAPARAM('TIPOMOVIMENTO') = 'E' THEN BEGIN&#13;&#10;      IF ( &#60;MOVIMENTOPRODUTO.&#34;TIPOMOVIMENTO&#34;&#62; &#60;&#62; 'E') THEN BEGIN&#13;&#10;          DADOS.VISIBLE  := FALSE;&#13;&#10;      END ELSE BEGIN&#13;&#10;         TEMDADO := TRUE;&#13;&#10;      END;&#13;&#10;  END;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GRUPO2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SALDO       := 0;&#13;&#10;  ENTRADAS    := 0;&#13;&#10;  SAIDAS      := 0;&#13;&#10;  IF SIGHAPARAM('TIPOMOVIMENTO') = 'A' THEN BEGIN&#13;&#10;    Memo25.MEMO.TEXT := '[MOVIMENTOPRODUTO.&#34;QUANTIDADE&#34;]'&#13;&#10;  END ELSE BEGIN&#13;&#10;    Memo25.MEMO.TEXT := '';&#13;&#10;  END;&#13;&#10;&#13;&#10;   IF SIGHAPARAM('TIPOMOVIMENTO') = 'E' THEN BEGIN&#13;&#10;      IF ( &#60;MOVIMENTOPRODUTO.&#34;TIPOMOVIMENTO&#34;&#62; &#60;&#62; 'E') THEN BEGIN&#13;&#10;          GRUPO2.VISIBLE  := FALSE;&#13;&#10;      END ELSE BEGIN&#13;&#10;          GRUPO2.VISIBLE  := TRUE;&#13;&#10;          TEMDADO := TRUE;&#13;&#10;      END;&#13;&#10;   END;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GRUPO2OnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  CUSTOTOTAL_ACUM := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure DADOSOnAfterPrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure FOOTER2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     FOOTER2.VISIBLE  := TEMDADO;&#13;&#10;     TEMDADO := FALSE;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." PropData="">
