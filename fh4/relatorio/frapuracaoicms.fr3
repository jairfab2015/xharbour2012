LO];&#13;&#10;      ICMS_2  := ICMS_2 + [VALORICMSTOTAL];&#13;&#10;    END&#13;&#10;    ELSE&#13;&#10;      OUTRO_2 := OUTRO_2 + [VALORICMSTOTALBASECALCULO];&#13;&#10;    ISENTO_2  := ISENTO_2 + [VALORICMSISENTO];&#13;&#10;    OUTRO_2   := OUTRO_2 + [VALORICMSOUTRO];&#13;&#10;  END ELSE&#13;&#10;  IF (CFOP = '3') OR (CFOP = '7') THEN BEGIN&#13;&#10;    CONTABIL_3:= CONTABIL_3 + [VALORCONTABIL];&#13;&#10;    IF [CREDITOICMS] = 1 THEN BEGIN&#13;&#10;      BASE_3  := BASE_3 + [VALORICMSTOTALBASECALCULO];&#13;&#10;      ICMS_3  := ICMS_3 + [VALORICMSTOTAL];&#13;&#10;    END&#13;&#10;    ELSE&#13;&#10;      OUTRO_3 := OUTRO_3 + [VALORICMSTOTALBASECALCULO];&#13;&#10;    ISENTO_3  := ISENTO_3 + [VALORICMSISENTO];&#13;&#10;    OUTRO_3   := OUTRO_3 + [VALORICMSOUTRO];&#13;&#10;  END;&#13;&#10;&#13;&#10;  GR_CONTABIL  := GR_CONTABIL + [VALORCONTABIL];&#13;&#10;  IF [CREDITOICMS] = 1 THEN BEGIN&#13;&#10;    GR_BASE    := GR_BASE   + [VALORICMSTOTALBASECALCULO];&#13;&#10;    GR_ICMS    := GR_ICMS   + [VALORICMSTOTAL];&#13;&#10;  END&#13;&#10;  ELSE&#13;&#10;    GR_OUTRO   := GR_OUTRO  + [VALORICMSTOTALBASECALCULO];&#13;&#10;  GR_ISENTO    := GR_ISENTO + [VALORICMSISENTO];&#13;&#10;  GR_OUTRO     := GR_OUTRO  + [VALORICMSOUTRO];&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Rodap_De_P_gina1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Rodap_De_P_gina1, Engine do&#13;&#10;  begin&#13;&#10;  PAGINA := PAGINA + 1;&#13;&#10;  [SIGHAPARAMVALOR(PAGINA, [PAGINA])];&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Cabe_alhoDe_Grupo1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  with Cabe_alhoDe_Grupo1, Engine do&#13;&#10;  begin&#13;&#10;  GR_CONTABIL  := 0;&#13;&#10;  GR_BASE      := 0;&#13;&#10;  GR_ICMS      := 0;&#13;&#10;  GR_ISENTO    := 0;&#13;&#10;  GR_OUTRO     := 0;&#13;&#10;  end&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." PropData="044C65667403A00103546F7003C8000844617461736574730100095661726961