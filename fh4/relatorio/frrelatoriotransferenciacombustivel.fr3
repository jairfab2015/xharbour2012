    <TfrxIBXQuery Name="QRTransferenciaCombustivel" UserName="QRTransferenciaCombustivel" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT T.CODIGO, C.DESCRICAO, TC.QUANTIDADE AS QTDERAZAO,&#13;&#10;TC.QTDECADASTROAJUSTADA AS QTDECADASTRO, TC.TIPOMOVIMENTO,&#13;&#10;TC.DATA, TC.STATUS, TC.JUSTIFICATIVA, T.ESTOQUE AS QTDECADASTROATUAL,&#13;&#10;CE.ESTOQUE AS QTDERAZAOATUAL, CEG.ESTOQUE AS QTDERAZAOGATUAL&#13;&#10;FROM TANQUE T, COMBUSTIVEL C, TRANSFERENCIACOMBUSTIVEL TC&#13;&#10;JOIN CALCULAESTOQUECOMBUSTIVEL((SELECT FIRST 1 DATA FROM TRANSFERENCIACOMBUSTIVEL WHERE CODIGOTRANSFERENCIA = :CODIGO), :CNPJ, 0, '#', 0, 999999, 99) CE ON CE.TANQUE = T.CODIGO&#13;&#10;JOIN CALCULAESTOQUECOMBUSTIVEL((SELECT FIRST 1 DATA FROM TRANSFERENCIACOMBUSTIVEL WHERE CODIGOTRANSFERENCIA = :CODIGO), :CNPJ, 0, '*', 0, 999999, 99) CEG ON CEG.TANQUE = T.CODIGO&#13;&#10;WHERE (T.CODIGOCOMBUSTIVEL = C.CODIGO)&#13;&#10;AND TC.TANQUE = T.CODIGO&#13;&#10;AND TC.CODIGOTRANSFERENCIA = :CODIGO&#13;&#10;ORDER BY 1" PropData=""/>
