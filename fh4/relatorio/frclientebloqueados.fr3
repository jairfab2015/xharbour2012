<TfrxReport Version="3.24" DotMatrixReport="False" EngineOptions.DoublePass="True" EngineOptions.UseFileCache="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4031" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PrintOptions.Printer="\\192.168.1.61\HP LaserJet 1200 Series PCL" ReportOptions.CreateDate="38856,4558872222" ReportOptions.Description.Text="" ReportOptions.Name="0" ReportOptions.LastChange="40004,7213720602" ScriptLanguage="PascalScript" ScriptText.Text="VAR I : INTEGER;&#13;&#10;    TIPOBLOQ : STRING;&#13;&#10;    TOTALPROFISSAO, TOTALCLASSIFICAO, TOTALREGIAO : REAL;&#13;&#10;&#13;&#10;procedure ghProfissaoOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;    I := &#60;CLIENTE.&#34;BLOQUEARVENDA&#34;&#62;;&#13;&#10;&#13;&#10;    IF &#60;CLIENTE.&#34;BLOQUEARVENDA&#34;&#62; = 511 THEN BEGIN&#13;&#10;       TIPOBLOQ := 'BLOQUEADO VENDA A VISTA/A PRAZO';&#13;&#10;       EXIT;&#13;&#10;    END;&#13;&#10;&#13;&#10;&#13;&#10;    IF &#60;CLIENTE.&#34;BLOQUEARVENDA&#34;&#62; = 510 THEN BEGIN&#13;&#10;       TIPOBLOQ := 'BLOQUEADO VENDA A VISTA';&#13;&#10;       EXIT;&#13;&#10;    END;&#13;&#10;&#13;&#10;    if I &#62;= 256 then begin&#13;&#10;        I := I - 256;&#13;&#10;        TIPOBLOQ := TIPOBLOQ + ' - BLOQUEADO CARTÃO DE DÉBITO';&#13;&#10;    end;&#13;&#10;&#13;&#10;    if I &#62;= 128 then begin&#13;&#10;        I := I - 128;&#13;&#10;        TIPOBLOQ := TIPOBLOQ +' - BLOQUEADO CARTÃO DE CRÉDITO';&#13;&#10;    end;&#13;&#10;&#13;&#10;    if I &#62;= 64  then begin&#13;&#10;       I := I - 64;&#13;&#10;       TIPOBLOQ := TIPOBLOQ +' - BLOQUEADO CHEQUE PRÉDATADO';&#13;&#10;    end;&#13;&#10;&#13;&#10;    if I &#62;= 32  then begin&#13;&#10;       I := I - 32;&#13;&#10;       TIPOBLOQ := TIPOBLOQ +' - BLOQUEADO OUTRAS';&#13;&#10;    end;&#13;&#10;&#13;&#10;    if I &#62;= 16  then begin&#13;&#10;       I := I - 16;&#13;&#10;       TIPOBLOQ := TIPOBLOQ +' - BLOQUEADO DÉBITO CONTA CORRENTE';&#13;&#10;    end;&#13;&#10;&#13;&#10;    if I &#62;= 8   then begin&#13;&#10;       I := I - 8;&#13;&#10;       TIPOBLOQ := TIPOBLOQ +' - BLOQUEADO CRÉDITO CONTA CORRENTE';&#13;&#10;    end;&#13;&#10;&#13;&#10;    if I &#62;= 4   then begin&#13;&#10;       I := I - 4;&#13;&#10;       TIPOBLOQ := TIPOBLOQ +' - BLOQUEADO DEVOLUÇÃO';&#13;&#10;    end;&#13;&#10;&#13;&#10;    if I &#62;= 2   then begin&#13;&#10;       I := I - 2;&#13;&#10;       TIPOBLOQ := TIPOBLOQ +' - BLOQUEADO VENDA A PRAZO';&#13;&#10;    end;&#13;&#10;&#13;&#10;    if I =  1   then&#13;&#10;        TIPOBLOQ := 'BLOQUEADO VENDA A VISTA';&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btConfirmaOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;&#13;&#10;  if (RBOperacao.checked = True) Then Begin&#13;&#10;     CLIENTE.OPEN;&#13;&#10;     Page2.Visible := False;&#13;&#10;     Page1.Visible := True;&#13;&#10;  end&#13;&#10;  else if (RBVendedor.checked = True) Then Begin&#13;&#10;     BLOQPORVENDEDOR.OPEN;&#13;&#10;     Page1.Visible := False;&#13;&#10;     Page2.Visible := True;&#13;&#10;  End;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;     CLIENTE.CLOSE;&#13;&#10;     // parametros dos Ativos/Inativos&#13;&#10;     // IF [SIGHAPARAM('TIPO')] = 0 THEN&#13;&#10;     //    CLIENTE.SQL.ADD('AND C.ATIVO = 1');&#13;&#10;     //  IF [SIGHAPARAM('TIPO')] = 1 THEN&#13;&#10;     //    CLIENTE.SQL.ADD('AND C.ATIVO = 0');&#13;&#10;     //  IF [SIGHAPARAM('REGIAO')] &#60;&#62; 0  THEN&#13;&#10;     //    CLIENTE.SQL.ADD('AND R.CODIGO = '+[SIGHAPARAM('REGIAO')]);&#13;&#10;     //  IF [SIGHAPARAM('PROFISSAO')] &#60;&#62; 0  THEN&#13;&#10;     //    CLIENTE.SQL.ADD('AND F.CODIGO = '+[SIGHAPARAM('PROFISSAO')]);&#13;&#10;     //  IF [SIGHAPARAM('CLASSIFICACAO')] &#60;&#62; 0  THEN&#13;&#10;     //    CLIENTE.SQL.ADD('AND CL.CODIGO = '+[SIGHAPARAM('CLASSIFICACAO')]);&#13;&#10;     CLIENTE.SQL.ADD('ORDER BY C.BLOQUEARVENDA, C.POSICAO, R.DESCRICAO, CL.DESCRICAO, C.NOME');&#13;&#10;&#13;&#10;  TOTALPROFISSAO         := 0;&#13;&#10;  TOTALCLASSIFICAO       := 0;&#13;&#10;  TOTALREGIAO            := 0;&#13;&#10;  TIPOBLOQ               := '';&#13;&#10;  GHPROFISSAO.HEIGHT     := 0;&#13;&#10;  GFPROFISSAO.HEIGHT     := 0;&#13;&#10;&#13;&#10;  IF SIGHAPARAM('PROFISSAO') &#60;&#62; '0' THEN BEGIN&#13;&#10;    GHPROFISSAO.VISIBLE := TRUE;&#13;&#10;    GFPROFISSAO.VISIBLE := TRUE;&#13;&#10;    GHPROFISSAO.HEIGHT  := 19;&#13;&#10;    GFPROFISSAO.HEIGHT  := 19;&#13;&#10;  END;&#13;&#10;&#13;&#10;  // se for selecionado todos&#13;&#10;  IF (SIGHAPARAM('CLASSIFICACAO') = '0') AND (SIGHAPARAM('PROFISSAO') = '0') AND (SIGHAPARAM('REGIAO') = '0') THEN BEGIN&#13;&#10;    GHPROFISSAO.VISIBLE := TRUE;&#13;&#10;    GFPROFISSAO.VISIBLE := TRUE;&#13;&#10;    GHPROFISSAO.HEIGHT  := 19;&#13;&#10;    GFPROFISSAO.HEIGHT  := 19;&#13;&#10;  END;&#13;&#10;&#13;&#10;end." PropData="">
