  <TfrxDialogPage Name="Page2" Font.Charset="1" Font.Color="0" Font.Height="-11" Font.Name="Tahoma" Font.Style="0" BorderStyle="bsDialog" Caption="Avaliação Histórica do CMV" Color="-2147483633" Height="136" Left="182" Top="179" Width="374" OnShow="Page2OnShow">
    <TfrxIBXQuery Name="HISTORICO" UserName="HISTORICO" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT CODIGOEMPRESA, ANO, COMPRAS, VENDAS,&#13;&#10;CMV, ESTOQUE, (VENDAS - CMV) AS LUCROBRUTO,&#13;&#10;CASE WHEN (VENDAS &#62; 0) THEN&#13;&#10;      ((CMV / VENDAS) * 100)&#13;&#10;ELSE  0&#13;&#10;END AS PERC_LUCROBRUTO,&#13;&#10;&#13;&#10;CASE WHEN (VENDAS &#62; 0) THEN&#13;&#10;      ((COMPRAS / VENDAS) * 100)&#13;&#10;ELSE  0&#13;&#10;END AS PERC_COMPRAS&#13;&#10;&#13;&#10;FROM EMPRESAS_CMV WHERE CODIGOEMPRESA = :CODIGOEMPRESA&#13;&#10;AND ANO &#60; :ANO&#13;&#10;&#13;&#10;ORDER BY ANO DESC" PropData=""/>
    <TfrxIBXQuery Name="MOVIMENTO" UserName="MOVIMENTO" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT&#13;&#10;EXTRACT(MONTH FROM DATAENTRADA) AS MES,&#13;&#10;&#13;&#10;/* ==================================== */&#13;&#10;/*          COMPRAS DO PERÍODO          */&#13;&#10;/* ==================================== */&#13;&#10;SUM(CASE&#13;&#10;WHEN&#13;&#10;/* COMPRA DE MERCADORIA*/&#13;&#10;(CODIGOFISCAL BETWEEN '1.101' AND '1.115') OR&#13;&#10;(CODIGOFISCAL BETWEEN '2.101' AND '2.115') OR&#13;&#10;(CODIGOFISCAL BETWEEN '1.118' AND '1.123') OR&#13;&#10;(CODIGOFISCAL BETWEEN '2.118' AND '2.123') OR&#13;&#10;(CODIGOFISCAL BETWEEN '3.101' AND '3.127') OR&#13;&#10;(CODIGOFISCAL BETWEEN '1.401' AND '1.405') OR&#13;&#10;(CODIGOFISCAL BETWEEN '2.401' AND '2.405') OR&#13;&#10;/* ENERGIA ELETRICA */&#13;&#10;(CODIGOFISCAL BETWEEN '1.251' AND '1.258') OR&#13;&#10;(CODIGOFISCAL BETWEEN '2.251' AND '2.258') OR&#13;&#10;(CODIGOFISCAL BETWEEN '3.251' AND '3.258') OR&#13;&#10;/* SERVICO DE COMUNICACAO */&#13;&#10;(CODIGOFISCAL BETWEEN '1.301' AND '1.307') OR&#13;&#10;(CODIGOFISCAL BETWEEN '2.301' AND '2.307') OR&#13;&#10;(CODIGOFISCAL BETWEEN '3.301' AND '3.307') OR&#13;&#10;/* SERVICO DE TRANSPORTE */&#13;&#10;(CODIGOFISCAL BETWEEN '1.351' AND '1.357') OR&#13;&#10;(CODIGOFISCAL BETWEEN '2.351' AND '2.357') OR&#13;&#10;(CODIGOFISCAL BETWEEN '3.351' AND '3.357')&#13;&#10;THEN VALORCONTABIL&#13;&#10;WHEN&#13;&#10;/* DEVOLUCAO DE COMPRA */&#13;&#10;(CODIGOFISCAL BETWEEN '5.201' AND '5.204') OR&#13;&#10;(CODIGOFISCAL BETWEEN '5.208' AND '5.209') OR&#13;&#10;(CODIGOFISCAL BETWEEN '6.201' AND '6.204') OR&#13;&#10;(CODIGOFISCAL BETWEEN '6.208' AND '6.209') OR&#13;&#10;(CODIGOFISCAL BETWEEN '7.201' AND '7.202') OR&#13;&#10;(CODIGOFISCAL BETWEEN '7.211' AND '7.211') OR&#13;&#10;(CODIGOFISCAL BETWEEN '5.410' AND '5.411') OR&#13;&#10;(CODIGOFISCAL BETWEEN '6.410' AND '6.411') OR&#13;&#10;/* ANULACAO DE ENERGIA ELETRICA */&#13;&#10;(CODIGOFISCAL BETWEEN '5.207' AND '5.207') OR&#13;&#10;(CODIGOFISCAL BETWEEN '6.207' AND '6.207') OR&#13;&#10;(CODIGOFISCAL BETWEEN '7.207' AND '7.207') OR&#13;&#10;/* ANULACAO DE SERVICO DE COMUNICACAO */&#13;&#10;(CODIGOFISCAL BETWEEN '5.205' AND '5.205') OR&#13;&#10;(CODIGOFISCAL BETWEEN '6.205' AND '6.205') OR&#13;&#10;(CODIGOFISCAL BETWEEN '7.205' AND '7.205') OR&#13;&#10;/* ANULACAO DE SERVICO DE COMUNICACAO */&#13;&#10;(CODIGOFISCAL BETWEEN '5.206' AND '5.206') OR&#13;&#10;(CODIGOFISCAL BETWEEN '6.206' AND '6.206') OR&#13;&#10;(CODIGOFISCAL BETWEEN '7.206' AND '7.206')&#13;&#10;THEN -VALORCONTABIL&#13;&#10;ELSE 0 END) AS VALORCOMPRA,&#13;&#10;&#13;&#10;/* ==================================== */&#13;&#10;/*           VENDAS DO PERÍODO          */&#13;&#10;/* ==================================== */&#13;&#10;&#13;&#10;SUM(CASE&#13;&#10;WHEN&#13;&#10;/* VENDA DE MERCADORIA*/&#13;&#10;(CODIGOFISCAL BETWEEN '5.101' AND '5.115') OR&#13;&#10;(CODIGOFISCAL BETWEEN '6.101' AND '6.115') OR&#13;&#10;(CODIGOFISCAL BETWEEN '5.118' AND '5.123') OR&#13;&#10;(CODIGOFISCAL BETWEEN '6.118' AND '6.123') OR&#13;&#10;(CODIGOFISCAL BETWEEN '7.101' AND '7.127') OR&#13;&#10;(CODIGOFISCAL BETWEEN '5.401' AND '5.405') OR&#13;&#10;(CODIGOFISCAL BETWEEN '6.401' AND '6.405') OR&#13;&#10;/* ENERGIA ELETRICA */&#13;&#10;(CODIGOFISCAL BETWEEN '5.251' AND '5.258') OR&#13;&#10;(CODIGOFISCAL BETWEEN '6.251' AND '6.258') OR&#13;&#10;(CODIGOFISCAL BETWEEN '7.251' AND '7.258') OR&#13;&#10;/* SERVICO DE COMUNICACAO */&#13;&#10;(CODIGOFISCAL BETWEEN '5.301' AND '5.307') OR&#13;&#10;(CODIGOFISCAL BETWEEN '6.301' AND '6.307') OR&#13;&#10;(CODIGOFISCAL BETWEEN '7.301' AND '7.307') OR&#13;&#10;/* SERVICO DE TRANSPORTE */&#13;&#10;(CODIGOFISCAL BETWEEN '5.351' AND '5.357') OR&#13;&#10;(CODIGOFISCAL BETWEEN '6.351' AND '6.357') OR&#13;&#10;(CODIGOFISCAL BETWEEN '7.351' AND '7.357')&#13;&#10;THEN VALORCONTABIL&#13;&#10;WHEN&#13;&#10;/* DEVOLUCAO DE VENDA */&#13;&#10;(CODIGOFISCAL BETWEEN '1.201' AND '1.204') OR&#13;&#10;(CODIGOFISCAL BETWEEN '1.208' AND '1.209') OR&#13;&#10;(CODIGOFISCAL BETWEEN '2.201' AND '2.204') OR&#13;&#10;(CODIGOFISCAL BETWEEN '2.208' AND '2.209') OR&#13;&#10;(CODIGOFISCAL BETWEEN '3.201' AND '3.202') OR&#13;&#10;(CODIGOFISCAL BETWEEN '3.211' AND '3.211') OR&#13;&#10;(CODIGOFISCAL BETWEEN '1.410' AND '1.411') OR&#13;&#10;(CODIGOFISCAL BETWEEN '2.410' AND '2.411') OR&#13;&#10;/* ANULACAO DE ENERGIA ELETRICA */&#13;&#10;(CODIGOFISCAL BETWEEN '1.207' AND '1.207') OR&#13;&#10;(CODIGOFISCAL BETWEEN '2.207' AND '2.207') OR&#13;&#10;(CODIGOFISCAL BETWEEN '3.207' AND '3.207') OR&#13;&#10;/* ANULACAO DE SERVICO DE COMUNICACAO */&#13;&#10;(CODIGOFISCAL BETWEEN '1.205' AND '1.205') OR&#13;&#10;(CODIGOFISCAL BETWEEN '2.205' AND '2.205') OR&#13;&#10;(CODIGOFISCAL BETWEEN '3.205' AND '3.205') OR&#13;&#10;/* ANULACAO DE SERVICO DE COMUNICACAO */&#13;&#10;(CODIGOFISCAL BETWEEN '1.206' AND '1.206') OR&#13;&#10;(CODIGOFISCAL BETWEEN '2.206' AND '2.206') OR&#13;&#10;(CODIGOFISCAL BETWEEN '3.206' AND '3.206')&#13;&#10;THEN -VALORCONTABIL&#13;&#10;ELSE 0 END) AS VALORVENDA&#13;&#10;&#13;&#10;FROM MOVIMENTOFISCAL&#13;&#10;WHERE DATAENTRADA BETWEEN :DATAI AND :DATAF AND&#13;&#10;GERARELATORIO = 1 AND CANCELADA = 0&#13;&#10;AND CNPJ = :CNPJ&#13;&#10;&#13;&#10;GROUP BY EXTRACT(MONTH FROM DATAENTRADA)&#13;&#10;ORDER BY EXTRACT(MONTH FROM DATAENTRADA)&#13;&#10;" PropData=""/>
    <TfrxIBXQuery Name="ESTOQUE" UserName="ESTOQUE" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT ESTOQUE FROM EMPRESAS_CMV&#13;&#10;WHERE CODIGOEMPRESA = :CODIGOEMPRESA AND ANO = :ANO" PropData=""/>
  </TfrxDialogPage>
