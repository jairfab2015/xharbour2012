    <TfrxIBXQuery Name="CLIENTE" UserName="CLIENTE" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT C.*, D.CIDADE AS CIDADEENDERECO, D.ESTADO AS ESTADOENDERECO,&#13;&#10;D.CIDADE CIDADETRABALHO, E.ESTADO AS ESTADOTRABALHO, R.DESCRICAO AS REGIAO,&#13;&#10;F.DESCRICAO AS PROFISSAO, CL.DESCRICAO AS CLASSIFICACAO, D.CIDADE AS NOMECIDADE,&#13;&#10;D.ESTADO,&#13;&#10;FUNC.CODIGO AS CODIGOFUNCIONARIO, FUNC.NOME AS NOMEVENDEDOR&#13;&#10;&#13;&#10;FROM CLIENTE C, CIDADE D, CIDADE E, REGIAO R, FUNCAO F, CLASSIFICACAOCLIENTE CL, FUNCIONARIO FUNC&#13;&#10;&#13;&#10;WHERE C.CIDADE = D.CODIGO&#13;&#10;AND C.TRABALHOCIDADE = E.CODIGO&#13;&#10;AND C.CODIGOREGIAO = R.CODIGO&#13;&#10;AND C.CODIGOPROFISSAO = F.CODIGO&#13;&#10;AND C.CODIGOCLASSIFICACAO = CL.CODIGO&#13;&#10;AND C.REPRESENTANTE = FUNC.CODIGO" PropData="0A506172616D65746572730100"/>
