<TfrxReport Version="3.24" DotMatrixReport="False" EngineOptions.DoublePass="True" EngineOptions.UseFileCache="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4031" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="38856,4558872222" ReportOptions.Description.Text="" ReportOptions.Name="0" ReportOptions.LastChange="40460,4367380787" ScriptLanguage="PascalScript" ScriptText.Text="//Var TOTALPROFISSAO, TOTALCLASSIFICACAO, TOTALREGIAO : Real;&#13;&#10;&#13;&#10;VAR&#13;&#10;       TIPOBLOQUEIO : INTEGER;&#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;{  TOTALPROFISSAO := 0;&#13;&#10;  TOTALCLASSIFICACAO := 0;&#13;&#10;  TOTALREGIAO := 0;&#13;&#10;&#13;&#10;//  GHPROFISSAO.HEIGHT        := 0;&#13;&#10;//  GFPROFISSAO.HEIGHT        := 0;&#13;&#10;//  GHREGIAO.HEIGHT           := 0;&#13;&#10;//  GFREGIAO.HEIGHT           := 0;&#13;&#10;//  GHCLASSIFICACAO.HEIGHT    := 0;&#13;&#10;//  GFCLASSIFICACAO.HEIGHT    := 0;&#13;&#10;&#13;&#10;{  IF SIGHAPARAM('PROFISSAO') &#60;&#62; '0' THEN BEGIN&#13;&#10;    GHPROFISSAO.VISIBLE := TRUE;&#13;&#10;    GFPROFISSAO.VISIBLE := TRUE;&#13;&#10;    GHPROFISSAO.HEIGHT  := 19;&#13;&#10;    GFPROFISSAO.HEIGHT  := 19;&#13;&#10;  END;&#13;&#10;&#13;&#10;  IF SIGHAPARAM('REGIAO') &#60;&#62; '0' THEN BEGIN&#13;&#10;    GHREGIAO.VISIBLE := TRUE;&#13;&#10;    GFREGIAO.VISIBLE := TRUE;&#13;&#10;    GHREGIAO.HEIGHT  := 19;&#13;&#10;    GFREGIAO.HEIGHT  := 19;&#13;&#10;  END;&#13;&#10;&#13;&#10;  IF SIGHAPARAM('CLASSIFICACAO') &#60;&#62; '0' THEN BEGIN&#13;&#10;    GHCLASSIFICACAO.VISIBLE    := TRUE;&#13;&#10;    GFCLASSIFICACAO.VISIBLE    := TRUE;&#13;&#10;    GHCLASSIFICACAO.HEIGHT     := 19;&#13;&#10;    GFCLASSIFICACAO.HEIGHT     := 19;&#13;&#10;  END;&#13;&#10;&#13;&#10;  // se for selecionado todos&#13;&#10;  IF (SIGHAPARAM('CLASSIFICACAO') = '0') AND (SIGHAPARAM('PROFISSAO') = '0') AND (SIGHAPARAM('REGIAO') = '0') THEN BEGIN&#13;&#10;&#13;&#10;    GHPROFISSAO.VISIBLE := TRUE;&#13;&#10;    GFPROFISSAO.VISIBLE := TRUE;&#13;&#10;    GHPROFISSAO.HEIGHT  := 19;&#13;&#10;    GFPROFISSAO.HEIGHT  := 19;&#13;&#10;&#13;&#10;    GHREGIAO.VISIBLE := TRUE;&#13;&#10;    GFREGIAO.VISIBLE := TRUE;&#13;&#10;    GHREGIAO.HEIGHT  := 19;&#13;&#10;    GFREGIAO.HEIGHT  := 19;&#13;&#10;&#13;&#10;    GHCLASSIFICACAO.VISIBLE    := TRUE;&#13;&#10;    GFCLASSIFICACAO.VISIBLE    := TRUE;&#13;&#10;    GHCLASSIFICACAO.HEIGHT     := 19;&#13;&#10;    GFCLASSIFICACAO.HEIGHT     := 19;&#13;&#10;  END;}&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btOKOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  // Add by Fernando Brasil on 24.08.2010 - Poder escolher se quer agrupamento por cidade ou por estado.&#13;&#10;  if rbVendedorCidade.checked then begin&#13;&#10;    ghCidade.Condition := &#60;CLIENTE.&#34;CIDADE&#34;&#62;;&#13;&#10;    Memo7.Text         := 'Cidade:    [CLIENTE.&#34;NOMECIDADE&#34;]  -  [CLIENTE.&#34;ESTADO&#34;]';&#13;&#10;  end&#13;&#10;  else begin&#13;&#10;    ghCidade.Condition := &#60;QREstado.&#34;CODIGOUF&#34;&#62;;&#13;&#10;    Memo7.Text         := 'Estado:    [&#60;QREstado.&#34;ESTADO&#34;&#62;]  -  [&#60;QREstado.&#34;SIGLA&#34;&#62;]';&#13;&#10;  end;&#13;&#10;  // Fim Ord. Brasil&#13;&#10;end;&#13;&#10;&#13;&#10;procedure PageHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//  IF SIGHAPARAM('TIPO') = '0' THEN&#13;&#10;//    ATIVO.CHECKED := TRUE;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ghProfissaoOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//  TOTALPROFISSAO := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//  TOTALPROFISSAO := TOTALPROFISSAO + 1;&#13;&#10;//  TOTALREGIAO := TOTALREGIAO + 1;&#13;&#10;//  TOTALCLASSIFICACAO := TOTALCLASSIFICACAO + 1;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ghRegiaoOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//  TOTALREGIAO := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ghClassificacaoOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//  TOTALCLASSIFICACAO := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  CLIENTE.CLOSE;&#13;&#10;&#13;&#10;  // parametros dos Ativos/Inativos&#13;&#10;  IF SIGHAPARAM('TIPO') = '0' THEN&#13;&#10;     CLIENTE.SQL.ADD('AND C.ATIVO = 1');&#13;&#10;  IF SIGHAPARAM('TIPO') = '1' THEN&#13;&#10;     CLIENTE.SQL.ADD('AND C.ATIVO = 0');&#13;&#10;&#13;&#10;  IF SIGHAPARAM('REGIAO') &#60;&#62; '0'  THEN&#13;&#10;     CLIENTE.SQL.ADD('AND R.CODIGO = ' + SIGHAPARAM('REGIAO'));&#13;&#10;&#13;&#10;  IF SIGHAPARAM('PROFISSAO') &#60;&#62; '0'  THEN&#13;&#10;     CLIENTE.SQL.ADD('AND F.CODIGO = ' + SIGHAPARAM('PROFISSAO'));&#13;&#10;&#13;&#10;  IF SIGHAPARAM('CLASSIFICACAO') &#60;&#62; '0'  THEN&#13;&#10;     CLIENTE.SQL.ADD('AND CL.CODIGO = ' + SIGHAPARAM('CLASSIFICACAO'));&#13;&#10;&#13;&#10;  IF SIGHAPARAM('CIDADE') &#60;&#62; '0'  THEN&#13;&#10;     CLIENTE.SQL.ADD('AND C.CIDADE = ' + SIGHAPARAM('CIDADE'))&#13;&#10;  ELSE&#13;&#10;//     IF SIGHAPARAM('D.ESTADO') &#60;&#62; '0'  THEN // D.ESTADO não existe nos parâmetros, e sim ESTADO&#13;&#10;     IF SIGHAPARAM('ESTADO') &#60;&#62; '0'  THEN // Comentado por Fernando Brasil, Cleber e Catafesta no dia 09.10.2010&#13;&#10;       CLIENTE.SQL.ADD('AND D.ESTADO = ' + SIGHAPARAM('ESTADO'));&#13;&#10;&#13;&#10;  IF SIGHAPARAM('VENDEDOR') &#60;&#62; '0'  THEN&#13;&#10;     CLIENTE.SQL.ADD('AND FUNC.CODIGO = ' + SIGHAPARAM('VENDEDOR'));&#13;&#10;&#13;&#10;  TIPOBLOQUEIO := STRTOINT(SIGHAPARAM('BLOQUEIO'));&#13;&#10;  CASE TIPOBLOQUEIO OF&#13;&#10;   0 : BEGIN&#13;&#10;         mTitulo.Text := 'Relatório de Clientes Bloquedos';&#13;&#10;         CLIENTE.SQL.ADD('AND (RESTRICAODECREDITO = 0 OR RESTRICAODEDEBITO = 0)');&#13;&#10;       END;&#13;&#10;   1 : BEGIN&#13;&#10;         mTitulo.Text := 'Relatório de Clientes Não Bloquedos';&#13;&#10;         CLIENTE.SQL.ADD('AND (RESTRICAODECREDITO &#62; 0 OR RESTRICAODEDEBITO &#62; 0)');&#13;&#10;       END;&#13;&#10;  END;&#13;&#10;&#13;&#10;  IF SIGHAPARAM('ORDENAR') = '1'  THEN&#13;&#10;     CLIENTE.SQL.ADD('ORDER BY C.POSICAO') ELSE&#13;&#10;  CLIENTE.SQL.ADD('ORDER BY C.CIDADE, C.NOME');&#13;&#10;//  SHOWMESSAGE(CLIENTE.SQL.TEXT);&#13;&#10;  CLIENTE.OPEN;&#13;&#10;//  if (cliente.isempty) then&#13;&#10;//    showmessage('sim');&#13;&#10;&#13;&#10;&#13;&#10;end." PropData="">
