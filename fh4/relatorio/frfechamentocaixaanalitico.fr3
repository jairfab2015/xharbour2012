<TfrxReport Tag="1" Version="3.24" DotMatrixReport="False" EngineOptions.UseFileCache="True" IniFile="\Software\Fast Reports" OldStyleProgress="True" PreviewOptions.Buttons="4031" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="37893,9319499884" ReportOptions.Description.Text="" ReportOptions.Name="FECHAMENTO DE CAIXA - ANALÍTICO" ReportOptions.LastChange="40218,6595963542" ScriptLanguage="PascalScript" ScriptText.Text="&#13;&#10;VAR TOTAL, CUSTO, TOTALCONTABIL, TOTALGRUPO : REAL;&#13;&#10;    TIPOMOV : STRING;&#13;&#10;&#13;&#10;procedure ReportTitle1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTAL := 0;&#13;&#10;  CUSTO := 0;&#13;&#10;  TOTALCONTABIL := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTAL := TOTAL + &#60;MOVIMENTO.&#34;VALORTOTALNF&#34;&#62;;&#13;&#10;  CUSTO := CUSTO + &#60;MOVIMENTO.&#34;VALORCUSTO&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTAL := 0;&#13;&#10;  CUSTO := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Band1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF (&#60;MOVIMENTOCONTABIL.&#34;TIPO&#34;&#62; = 'D') THEN&#13;&#10;    TIPOMOV  := 'Pagamento'&#13;&#10;  ELSE IF (&#60;MOVIMENTOCONTABIL.&#34;TIPO&#34;&#62; = 'C') THEN&#13;&#10;    TIPOMOV  := 'Recebimento'&#13;&#10;  ELSE&#13;&#10;    TIPOMOV  := 'Extra';&#13;&#10;&#13;&#10;  TOTALGRUPO := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF &#60;MOVIMENTOCONTABIL.&#34;TIPO&#34;&#62; = 'D' THEN&#13;&#10;    TOTALCONTABIL := TOTALCONTABIL  - &#60;MOVIMENTOCONTABIL.&#34;VALOR&#34;&#62;&#13;&#10;  ELSE IF &#60;MOVIMENTOCONTABIL.&#34;TIPO&#34;&#62; = 'C' THEN&#13;&#10;    TOTALCONTABIL := TOTALCONTABIL  + &#60;MOVIMENTOCONTABIL.&#34;VALOR&#34;&#62;;&#13;&#10;  TOTALGRUPO      := TOTALGRUPO     + &#60;MOVIMENTOCONTABIL.&#34;VALOR&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  MOVIMENTO.OPEN;&#13;&#10;  MOVIMENTOCONTABIL.CLOSE;&#13;&#10;  MOVIMENTOCONTABIL.OPEN;&#13;&#10;end." PropData="">
  <TfrxDialogPage Name="DialogForm" Font.Charset="1" Font.Color="0" Font.Height="-11" Font.Name="Tahoma" Font.Style="0" BorderStyle="bsDialog" Caption="Form" Color="-2147483633" Height="165" Left="311" Top="235" Width="283" OnShow="Page1OnShow">
    <TfrxIBXQuery Name="MOVIMENTO" UserName="MOVIMENTO" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT&#13;&#10;N.CODIGOOPERACAO, O.DESCRICAO, R.NOME, N.DOCUMENTO,N.VALORTOTALNF,&#13;&#10;N.VALORCUSTO, (N.VALORTOTALNF - N.VALORCUSTO) AS MARGEMCONTRIBUICAO,&#13;&#10;(CASE WHEN (N.VALORTOTALNF &#62; 0) THEN (((N.VALORTOTALNF - N.VALORCUSTO) * 100) / N.VALORTOTALNF) ELSE 0 END) AS MARGEMCONTRIBUICAOPERCENTUAL,&#13;&#10;N.NOTAFISCALCANCELADA&#13;&#10;&#13;&#10;FROM NOTAFISCAL N, RELACAO R, OPERACAO O&#13;&#10;&#13;&#10;WHERE&#13;&#10;(N.CODIGORELACAO = R.CODIGORELACAO) AND&#13;&#10;(N.CODIGOOPERACAO = O.CODIGOLANCAMENTO) AND&#13;&#10;(N.CODIGOCAIXA = :CODIGOCAIXAABERTURA) AND&#13;&#10;(N.NOTAFISCALCANCELADA = 0) AND&#13;&#10;(N.CNPJ = :CNPJ)&#13;&#10;--Add by Tuba on 05/08/2009 - SighaPAF&#13;&#10;AND NOT (N.TIPODOCUMENTO = 'N' AND N.MODELO = '01'&#13;&#10;AND N.OBSERVACAO IS NOT NULL&#13;&#10;AND N.OBSERVACAO LIKE '%NF Ref. Cupom Fiscal%')&#13;&#10;&#13;&#10;&#13;&#10;ORDER BY N.CODIGOOPERACAO, N.DOCUMENTO, R.NOME" PropData=""/>
    <TfrxIBXQuery Name="CAIXAABERTURA" UserName="CAIXAABERTURA" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT CA.DATAFECHAMENTO, CA.CODIGO FROM CAIXAABERTURA CA&#13;&#10;WHERE (CODIGO = :CODIGOCAIXAABERTURA)&#13;&#10;AND CNPJ = :CNPJ" PropData=""/>
    <TfrxIBXQuery Name="MOVIMENTOCONTABIL" UserName="MOVIMENTOCONTABIL" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT M.TIPOMOVIMENTO AS TIPO, M.HISTORICO AS HISTORICOINDIVIDUAL, O.HISTORICO, M.STATUS, SUM(M.VALOR) AS VALOR&#13;&#10;FROM MOVIMENTOCONTABIL M, OPERACAOAUTOMATICO O&#13;&#10;WHERE (M.CODIGOCAIXAABERTURA = :CODIGOCAIXAABERTURA)&#13;&#10;AND (M.CODIGOLANCAMENTO = O.CODIGOCAIXA)&#13;&#10;AND M.CNPJ = :CNPJ&#13;&#10;GROUP BY M.TIPOMOVIMENTO, M.HISTORICO, O.HISTORICO, M.STATUS&#13;&#10;ORDER BY M.TIPOMOVIMENTO, O.HISTORICO, M.STATUS" PropData=""/>
