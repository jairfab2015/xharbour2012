    <TfrxIBXQuery Name="MESA" UserName="MESA" CloseDataSource="True" FieldAliases.Text="CODIGO=CODIGO&#13;&#10;MESA=MESA&#13;&#10;POSICAO=POSICAO&#13;&#10;CODIGOCLIENTE=CODIGOCLIENTE&#13;&#10;DATAHORA=DATAHORA&#13;&#10;CODIGOPRODUTO=CODIGOPRODUTO&#13;&#10;ESPECIAL=ESPECIAL&#13;&#10;QUANTIDADE=QUANTIDADE&#13;&#10;VALORUNITARIO=VALORUNITARIO&#13;&#10;VALORTOTAL=VALORTOTAL&#13;&#10;GARCOM=GARCOM&#13;&#10;SITUACAO=SITUACAO&#13;&#10;CORTESIA=CORTESIA&#13;&#10;NOMECARTAO=NOMECARTAO&#13;&#10;CODIGONF=CODIGONF&#13;&#10;CNPJ=CNPJ&#13;&#10;NOME=NOME&#13;&#10;UNIDADE=UNIDADE" SQL.Text="SELECT B.*, CAST(P.NOME||&#13;&#10;(CASE WHEN B.ESPECIAL IS NULL THEN '' ELSE '-'||B.ESPECIAL END)&#13;&#10;AS VARCHAR(255)) AS NOME,&#13;&#10;P.UNIDADE&#13;&#10;FROM BAR_CONTA B&#13;&#10;INNER JOIN PRODUTO P ON P.CODIGO = B.CODIGOPRODUTO&#13;&#10;LEFT JOIN PRODUTO P2 ON P2.CODIGO = B.CODIGOPRODUTO AND P2.CODIGO &#60;&#62; '0'&#13;&#10;LEFT JOIN PRODUTO P3 ON P3.CODIGO = B.CODIGOPRODUTO AND P3.CODIGO &#60;&#62; '0'&#13;&#10;LEFT JOIN PRODUTO P4 ON P4.CODIGO = B.CODIGOPRODUTO AND P4.CODIGO &#60;&#62; '0'&#13;&#10;LEFT JOIN LINHA L ON P.LINHA = L.CODIGO&#13;&#10;WHERE B.MESA = :MESA&#13;&#10;AND SITUACAO = 1&#13;&#10;ORDER BY B.POSICAO, B.DATAHORA" PropData="0A506172616D657465727301010C41000000204E616D653D224D455341222044617461547970653D226674496E7465676572222045787072657373696F6E3D225349474841504152414D28274D4553412729220000"/>
    <TfrxIBXQuery Name="DETALHES" UserName="DETALHES" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT B.POSICAO, B.CODIGOCLIENTE, C.NOME, SUM(B.VALORTOTAL) AS VALORTOTAL&#13;&#10;&#13;&#10;FROM BAR_CONTA B INNER JOIN CLIENTE C&#13;&#10;ON C.CODIGOCLIENTE = B.CODIGOCLIENTE&#13;&#10;&#13;&#10;WHERE (B.MESA = :MESA)&#13;&#10;&#13;&#10;AND SITUACAO = 1&#13;&#10;&#13;&#10;GROUP BY B.POSICAO, B.CODIGOCLIENTE, C.NOME&#13;&#10;&#13;&#10;ORDER BY B.POSICAO" PropData="0A506172616D657465727301010C41000000204E616D653D224D455341222044617461547970653D226674496E7465676572222045787072657373696F6E3D225349474841504152414D28274D4553412729220000"/>
