    <TfrxIBXQuery Name="QrCombustivel" UserName="QrCombustivel" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT CODIGO,DESCRICAO, CLASSIFICACAO FROM COMBUSTIVEL WHERE CODIGO &#60;&#62; 0 and&#13;&#10;codigo = :COMBUSTIVEL" PropData="0A506172616D657465727301010C4E000000204E616D653D22434F4D425553544956454C222044617461547970653D226674537472696E67222045787072657373696F6E3D225349474841504152414D2827434F4D425553544956454C2729220000"/>
    <TfrxIBXQuery Name="qrMedicao" UserName="qrMedicao" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT MEDICAO.CODIGOTANQUE AS CODIGO, SUM(MEDICAO.QUANTIDADE) AS QUANTIDADE&#13;&#10;&#13;&#10;FROM TANQUE, MEDICAO&#13;&#10;&#13;&#10;WHERE MEDICAO.CODIGOTANQUE = TANQUE.CODIGO AND&#13;&#10;      TANQUE.CODIGOCOMBUSTIVEL = :CODIGOCOMBUSTIVEL AND&#13;&#10;      (CAST(MEDICAO.DATAHORA AS DATE)= :DATA)&#13;&#10;      AND TANQUE.CNPJ = :CNPJ AND MEDICAO.CNPJ = :CNPJ&#13;&#10;&#13;&#10;GROUP BY MEDICAO.CODIGOTANQUE&#13;&#10;ORDER BY MEDICAO.CODIGOTANQUE" PropData=""/>
    <TfrxIBXQuery Name="qrTotalMedicao" UserName="qrTotalMedicao" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT SUM(MEDICAO.QUANTIDADE) AS QTDTOTALMEDICAO&#13;&#10;&#13;&#10;FROM TANQUE, MEDICAO&#13;&#10;&#13;&#10;WHERE MEDICAO.CODIGOTANQUE = TANQUE.CODIGO AND&#13;&#10;      TANQUE.CODIGOCOMBUSTIVEL = :CODIGOCOMBUSTIVEL AND&#13;&#10;      CAST(MEDICAO.DATAHORA AS DATE) = :DATA&#13;&#10;      AND TANQUE.CNPJ = :CNPJ AND MEDICAO.CNPJ = :CNPJ" PropData=""/>
    <TfrxIBXQuery Name="qrEntrada" UserName="qrEntrada" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT NF.DOCUMENTO, R.NOME, NF.SERIE, NF.DATAENTRADA, NFC.QUANTIDADE, NFC.CODIGOTANQUE&#13;&#10;FROM NOTAFISCAL NF, NOTAFISCALCOMBUSTIVEL NFC, RELACAO R&#13;&#10;WHERE NF.CODIGONF = NFC.CODIGONF&#13;&#10;AND NF.CODIGORELACAO = R.CODIGORELACAO&#13;&#10;AND NF.TIPOMOVIMENTO = 'E'&#13;&#10;AND NF.DATAENTRADA = :DATA&#13;&#10;AND NF.NOTAFISCALCANCELADA = 0&#13;&#10;AND NF.CODIGOFISCAL NOT IN ('1.922','2.922')&#13;&#10;AND NF.STATUS = '#'&#13;&#10;AND NFC.CODIGOCOMBUSTIVEL = :CODIGOCOMBUSTIVEL&#13;&#10;AND NF.CNPJ = :CNPJ&#13;&#10;AND NF.CNPJ = NFC.CNPJ" PropData=""/>
    <TfrxIBXQuery Name="qrBomba" UserName="qrBomba" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select distinct B.Codigo as CodBomba, C.PrecoVenda,&#13;&#10;T.Codigo as CodTanque, B.Ativo, NF.DOCUMENTO, NF.SERIE, NF.DATAEMISSAO, NFC.QUANTIDADE, R.NOME&#13;&#10;&#13;&#10;from Tanque T, Bomba B, Combustivel C, NOTAFISCALCOMBUSTIVEL NFC, NOTAFISCAL NF, RELACAO R&#13;&#10;&#13;&#10;where C.Codigo = :CodigoCombustivel and&#13;&#10;      T.CodigoCombustivel = C.Codigo and&#13;&#10;      B.CodigoTanque = T.Codigo&#13;&#10;      AND T.CNPJ = :CNPJ&#13;&#10;      AND NFC.CODIGONF = NF.CODIGONF&#13;&#10;      AND NFC.CODIGOTANQUE = T.CODIGO&#13;&#10;      AND NFC.CODIGOBOMBA = B.CODIGO&#13;&#10;      AND NFC.CNPJTANQUE = T.CNPJ&#13;&#10;      AND NF.CODIGORELACAO = R.CODIGORELACAO&#13;&#10;      AND NF.DATAEMISSAO = :DATA&#13;&#10;      AND NF.tipomovimento = 'S'&#13;&#10;      AND NF.notafiscalcancelada = 0" PropData=""/>
    <TfrxIBXQuery Name="qrAfericao" UserName="qrAfericao" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select distinct SUM(A.Quantidade) as Quantidade&#13;&#10;&#13;&#10;from Afericao A&#13;&#10;&#13;&#10;where A.CodigoBomba = :CodBomba and&#13;&#10;Cast(A.DataHora as Date) = :Data&#13;&#10;AND A.CNPJ = :CNPJ" PropData=""/>
    <TfrxIBXQuery Name="QRAbertura" UserName="QRAbertura" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT MAX(ENCERRANTE) AS ENCERRANTE&#13;&#10;FROM ENCERRANTE E, CAIXAABERTURA C&#13;&#10;WHERE E.CODIGOCAIXA = C.CODIGO&#13;&#10;AND E.CNPJCAIXAABERTURA = C.CNPJ&#13;&#10;AND E.CODIGOBOMBA = :CODIGOBOMBA&#13;&#10;AND CAST(C.DATAABERTURA AS DATE) = (SELECT MAX(CAST(CC.DATAABERTURA AS DATE)) FROM CAIXAABERTURA CC WHERE CAST(CC.DATAABERTURA AS DATE) &#60;= :DATA -2)&#13;&#10;AND E.CNPJ = :CNPJ" PropData=""/>
    <TfrxIBXQuery Name="qrFechamento" UserName="qrFechamento" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT MAX(ENCERRANTE) AS ENCERRANTE&#13;&#10;FROM ENCERRANTE E, CAIXAABERTURA C&#13;&#10;WHERE E.CODIGOCAIXA = C.CODIGO&#13;&#10;AND E.CNPJCAIXAABERTURA = C.CNPJ&#13;&#10;AND E.CODIGOBOMBA = :CODIGOBOMBA&#13;&#10;AND CAST(C.DATAABERTURA AS DATE) = (SELECT MAX(CAST(CC.DATAABERTURA AS DATE)) FROM CAIXAABERTURA CC WHERE CAST(CC.DATAABERTURA AS DATE) &#60;= :DATAFECHAMENTO -1)&#13;&#10;AND E.CNPJ = :CNPJ" PropData=""/>
    <TfrxIBXQuery Name="qrTotalMedicaoSeguinte" UserName="qrTotalMedicaoSeguinte" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT SUM(MEDICAO.QUANTIDADE) AS QTDTOTALMEDICAO&#13;&#10;&#13;&#10;FROM TANQUE, MEDICAO&#13;&#10;&#13;&#10;WHERE MEDICAO.CODIGOTANQUE = TANQUE.CODIGO AND&#13;&#10;      TANQUE.CODIGOCOMBUSTIVEL = :CODIGOCOMBUSTIVEL AND&#13;&#10;      CAST(MEDICAO.DATAHORA AS DATE) = :DATASEG&#13;&#10;      AND TANQUE.CNPJ = :CNPJ AND MEDICAO.CNPJ = :CNPJ" PropData=""/>
    <TfrxIBXQuery Name="qrVendasAcumuladas" UserName="qrVendasAcumuladas" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT SUM(C.VALORTOTAL) AS TOTAL&#13;&#10;FROM NOTAFISCAL NF, NOTAFISCALCOMBUSTIVEL C&#13;&#10;WHERE NF.CODIGONF = C.CODIGONF&#13;&#10;AND NF.TIPOMOVIMENTO = 'S'&#13;&#10;AND NF.DATAEMISSAO = :DATA&#13;&#10;AND NF.NOTAFISCALCANCELADA = 0&#13;&#10;AND C.CODIGOCOMBUSTIVEL = :CODCOMBUSTIVEL&#13;&#10;AND ((C.CODIGOFISCAL &#60;&#62; '5.922') AND (C.CODIGOFISCAL &#60;&#62; '6.922'))&#13;&#10;--Add by Tuba on 05/08/2009 - SighaPAF&#13;&#10;AND NOT (NF.TIPODOCUMENTO = 'N' AND NF.MODELO = '01' AND NF.OBSERVACAO LIKE '%NF Ref. Cupom Fiscal%')&#13;&#10;AND C.CANCELADOITEM = 0&#13;&#10;AND NF.CNPJ = :CNPJ AND C.CNPJ = :CNPJ" PropData=""/>
    <TfrxIBXQuery Name="qrMedicaoSeguinte" UserName="qrMedicaoSeguinte" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT M.CODIGOTANQUE AS CODIGO, M.QUANTIDADE&#13;&#10;FROM TANQUE T, MEDICAO M&#13;&#10;WHERE M.CODIGOTANQUE = T.CODIGO&#13;&#10;AND T.CODIGOCOMBUSTIVEL = :CODIGOCOMBUSTIVEL&#13;&#10;AND CAST(M.DATAHORA AS DATE) = :DATASEG&#13;&#10;AND T.CNPJ = :CNPJ&#13;&#10;AND T.CNPJ = M.CNPJTANQUE" PropData=""/>
    <TfrxIBXQuery Name="qrValorvendas" UserName="qrValorvendas" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT SUM(C.VALORTOTAL) AS TOTAL&#13;&#10;FROM NOTAFISCAL NF, NOTAFISCALCOMBUSTIVEL C&#13;&#10;WHERE NF.CODIGONF = C.CODIGONF&#13;&#10;AND NF.TIPOMOVIMENTO = 'S'&#13;&#10;AND NF.DATAEMISSAO = :DATA&#13;&#10;AND NF.NOTAFISCALCANCELADA = 0&#13;&#10;AND C.CODIGOCOMBUSTIVEL = :CODCOMBUSTIVEL&#13;&#10;AND ((C.CODIGOFISCAL &#60;&#62; '5.922') AND (C.CODIGOFISCAL &#60;&#62; '6.922'))&#13;&#10;--Add by Tuba on 05/08/2009 - SighaPAF&#13;&#10;AND NOT (NF.TIPODOCUMENTO = 'N' AND NF.MODELO = '01' AND NF.OBSERVACAO LIKE '%NF Ref. Cupom Fiscal%')&#13;&#10;AND C.CANCELADOITEM = 0&#13;&#10;AND NF.CNPJ = :CNPJ AND C.CNPJ = :CNPJ" PropData=""/>
    <TfrxIBXQuery Name="QRGeral" UserName="QRGeral" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT C.CODIGO, C.DESCRICAO, CAST(M.DATAHORA AS DATE) AS DATA&#13;&#10;FROM MEDICAO M&#13;&#10;INNER JOIN TANQUE T ON T.CODIGO = M.CODIGOTANQUE AND T.CNPJ = :CNPJ&#13;&#10;INNER JOIN COMBUSTIVEL C ON C.CODIGO = T.CODIGOCOMBUSTIVEL&#13;&#10;WHERE (CAST(M.DATAHORA AS DATE)= :DATA)&#13;&#10;AND C.CODIGO = :CODIGO AND M.CNPJ = :CNPJ" PropData=""/>
    <TfrxIBXQuery Name="qrPRECO" UserName="qrPRECO" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT CAST(AVG((VALORTOTAL - VALORDESCONTO) / QUANTIDADE) AS NUMERIC(15,3)) AS PRECOMEDIO&#13;&#10;FROM NOTAFISCALCOMBUSTIVEL&#13;&#10;WHERE  CODIGOBOMBA = :CODIGOBOMBA AND DATA = :DATACONTABIL&#13;&#10;AND TIPOMOVIMENTO = 'S'&#13;&#10;AND CANCELADOITEM = 0&#13;&#10;AND CNPJ = :CNPJ" PropData=""/>
    <TfrxIBXQuery Name="qrPrecoAcumulado" UserName="qrPrecoAcumulado" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT CAST(AVG(VALORTOTAL / QUANTIDADE) AS NUMERIC(15,3)) AS PRECOMEDIOACUMULADO&#13;&#10;FROM NOTAFISCALCOMBUSTIVEL&#13;&#10;WHERE  CODIGOBOMBA = :CODIGOBOMBA AND DATA BETWEEN :DATAINICIOMES&#13;&#10;AND :DATACONTABIL&#13;&#10;AND CANCELADOITEM = 0&#13;&#10;AND CNPJ = :CNPJ" PropData=""/>
    <TfrxIBXQuery Name="qrEncerranteInicialMes" UserName="qrEncerranteInicialMes" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT MIN(INICIALMANUAL) AS ENCERRANTEINICIALMES&#13;&#10;FROM ENCERRANTE E, CAIXAABERTURA C&#13;&#10;WHERE E.CODIGOCAIXA = C.CODIGO&#13;&#10;AND E.CNPJCAIXAABERTURA = C.CNPJ&#13;&#10;AND E.CODIGOBOMBA = :CODIGOBOMBA&#13;&#10;AND Extract(MONTH from CAST(C.DATAABERTURA AS DATE)) = :MES&#13;&#10;AND Extract(YEAR from CAST(C.DATAABERTURA AS DATE)) = :ANO&#13;&#10;AND E.CNPJ = :CNPJ" PropData=""/>
