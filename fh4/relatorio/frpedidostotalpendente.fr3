RODUTOFATURADO&#34;&#62;);&#13;&#10;  ADIANTAMENTO   := &#60;PEDIDO.&#34;PDVALORSALDO&#34;&#62;;&#13;&#10;  TOTAL          := FRETE + IPI + SERVICO + PRODUTO;&#13;&#10;&#13;&#10;  IF FRETE &#62; 0 THEN BEGIN&#13;&#10;    VALORFRETECLIENTE        := VALORFRETECLIENTE + FRETE;&#13;&#10;    VALORFRETE               := VALORFRETE + FRETE;&#13;&#10;    VALORTOTALCLIENTE        := VALORTOTALCLIENTE + FRETE;&#13;&#10;    VALORTOTAL               := VALORTOTAL + FRETE;&#13;&#10;  END&#13;&#10;  ELSE IF FRETE &#60; 0 THEN&#13;&#10;    FRETEMAIOR               := FRETEMAIOR + FRETE;&#13;&#10;&#13;&#10;  IF SERVICO &#62; 0 THEN BEGIN&#13;&#10;    VALORSERVICOCLIENTE      := VALORSERVICOCLIENTE + SERVICO;&#13;&#10;    VALORSERVICO             := VALORSERVICO + SERVICO;&#13;&#10;    VALORTOTALCLIENTE        := VALORTOTALCLIENTE + SERVICO;&#13;&#10;    VALORTOTAL               := VALORTOTAL + SERVICO;&#13;&#10;  END&#13;&#10;  ELSE IF SERVICO &#60; 0 THEN&#13;&#10;    SERVICOMAIOR             := SERVICOMAIOR + SERVICO;&#13;&#10;&#13;&#10;  IF IPI &#62; 0 THEN BEGIN&#13;&#10;    VALORIPICLIENTE          := VALORIPICLIENTE + IPI;&#13;&#10;    VALORIPI                 := VALORIPI + IPI;&#13;&#10;    VALORTOTALCLIENTE        := VALORTOTALCLIENTE + IPI;&#13;&#10;    VALORTOTAL               := VALORTOTAL + IPI;&#13;&#10;  END&#13;&#10;  ELSE IF IPI &#60; 0 THEN&#13;&#10;    IPIMAIOR                 := IPIMAIOR + IPI;&#13;&#10;&#13;&#10;  IF ICMSSUB &#62; 0 THEN BEGIN&#13;&#10;    VALORICMSSUBCLIENTE      := VALORICMSSUBCLIENTE + ICMSSUB;&#13;&#10;    VALORICMSSUB             := VALORICMSSUB + ICMSSUB;&#13;&#10;    VALORTOTALCLIENTE        := VALORTOTALCLIENTE + ICMSSUB;&#13;&#10;    VALORTOTAL               := VALORTOTAL + ICMSSUB;&#13;&#10;  END&#13;&#10;  ELSE IF ICMSSUB &#60; 0 THEN&#13;&#10;    ICMSSUBMAIOR                 := ICMSSUBMAIOR + ICMSSUB;&#13;&#10;&#13;&#10;&#13;&#10;  IF PRODUTO &#62; 0 THEN BEGIN&#13;&#10;    VALORPRODUTOCLIENTE      := VALORPRODUTOCLIENTE + PRODUTO;&