    <TfrxIBXQuery Name="MOVIMENTO" UserName="MOVIMENTO" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT OB.CODIGOLANCAMENTO, O.DESCRICAO, M.DATA, M.COMPLEMENTO,&#13;&#10;M.VALOR, M.TIPOMOVIMENTO, OB.HISTORICO, M.DATAQUITACAO,&#13;&#10;C.CODIGO, C.DESCRICAO AS DESCRICAOCENTROCUSTO&#13;&#10;&#13;&#10;FROM MOVIMENTOBANCARIO M, OPERACAOBANCARIA OB, OPERACAO O, CENTROCUSTO C&#13;&#10;&#13;&#10;WHERE&#13;&#10;&#13;&#10;(M.CODIGOLANCAMENTO = OB.CODIGOCAIXA) AND&#13;&#10;(OB.CODIGOLANCAMENTO = O.CODIGOLANCAMENTO) AND&#13;&#10;(M.CENTROCUSTO = C.CODIGO) AND&#13;&#10;(M.CNPJ = :CNPJ) AND&#13;&#10;(M.DATA BETWEEN :DATAI AND :DATAF) AND&#13;&#10;(M.CENTROCUSTO LIKE :CENTROCUSTO) AND&#13;&#10;((:CODIGOLANCAMENTO = 0) OR (O.CODIGOLANCAMENTO = :CODIGOLANCAMENTO))&#13;&#10;&#13;&#10;ORDER BY C.DESCRICAO, M.DATA, M.COMPLEMENTO" PropData=""/>
