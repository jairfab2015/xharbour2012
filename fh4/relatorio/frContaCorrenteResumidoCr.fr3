CLA := SALDOCLA + &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;      END&#13;&#10;      ELSE BEGIN&#13;&#10;         SALDO := SALDO - &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;         SALDOCLI := SALDOCLI - &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;         SALDOGERAL := SALDOGERAL - &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;         SALDOCLA := SALDOCLA - &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;      END&#13;&#10;    END;&#13;&#10;&#13;&#10;    if (&#60;DSCC.&#34;VALORSALDO&#34;&#62; &#60;= 0) THEN BEGIN&#13;&#10;       SALDORECEBIDO := SALDORECEBIDO + &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;       SALDORECEBIDOCLI := SALDORECEBIDOCLI + &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;       SALDORECEBIDOGERAL := SALDORECEBIDOGERAL + &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;       SALDORECEBIDOCLA := SALDORECEBIDOCLA + &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;    END&#13;&#10;    ELSE BEGIN&#13;&#10;       SALDORECEBER  := SALDORECEBER +  &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;       SALDORECEBERCLI  := SALDORECEBERCLI +  &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;       SALDORECEBERGERAL  := SALDORECEBERGERAL +  &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;       SALDORECEBERCLA  := SALDORECEBERCLA +  &#60;DSCC.&#34;VALOR&#34;&#62;;&#13;&#10;    END;&#13;&#10;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Memo9OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  COD  := VARTOSTR(&#60;DSCC.&#34;CODIGOCLIENTE&#34;&#62;);&#13;&#10;  WHILE (LENGTH(COD) &#60; 5) DO&#13;&#10;    COD := '0' + COD;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SALDOCLI := 0;&#13;&#10;  SALDORECEBERCLI  := 0;&#13;&#10;  SALDORECEBIDOCLI := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SALDO := 0;&#13;&#10;  SALDORECEBER  := 0;&#13;&#10;  SALDORECEBIDO := 0;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader3OnBeforePrint(Sender