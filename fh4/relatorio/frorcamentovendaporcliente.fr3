#62;);&#13;&#10;&#13;&#10;  frorcamentoporcliente.SQL.TEXT := frorcamentoporcliente.SQL.TEXT +&#13;&#10;  ' ORDER BY CLIENTE.NOME, ORCAMENTOVENDA.CODIGOORCAMENTO';&#13;&#10;&#13;&#10;  frorcamentoporcliente.Open;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure cbtodosOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;//  dblcbcliente.enabled := not cbtodos.checked;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALCLIENTE := TOTALCLIENTE + &#60;frorcamentoporcliente.&#34;VALORTOTAL&#34;&#62;;&#13;&#10;  TOTALGERAL   := TOTALGERAL   + &#60;frorcamentoporcliente.&#34;VALORTOTAL&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALCLIENTE := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  frClientePessoa.Open;&#13;&#10;//  frClientePessoa.FetchAll;&#13;&#10;&#13;&#10;  TOTALGERAL := 0;&#13;&#10;end." PropData="">
