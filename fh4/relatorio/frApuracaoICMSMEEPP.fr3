<TfrxReport Version="3.24" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4031" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="37811,6867043287" ReportOptions.Description.Text="" ReportOptions.Name="Apuração de ICMS ME/EPP" ReportOptions.LastChange="39192,4621158796" ScriptLanguage="PascalScript" ScriptText.Text="VAR X_ACUMULADO, MEDIAESTIM, MEDIA : REAL;&#13;&#10;    DATABASE_NAME : STRING;&#13;&#10;    DIA_INICIO, MES_INICIO, ANO_INICIO, MES, ANO, MES_USA, I : INTEGER;&#13;&#10;procedure DadosMestre1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF (APURACAO.EOF = FALSE) THEN BEGIN&#13;&#10;     X_ACUMULADO    := X_ACUMULADO + &#60;APURACAO.&#34;RECEITABRUTA&#34;&#62;;&#13;&#10;     Memo11.TEXT    := &#60;APURACAO.&#34;MESMOVTO&#34;&#62;;&#13;&#10;     Memo12.TEXT    := FORMATFLOAT('#,##0.00', &#60;APURACAO.&#34;RECEITABRUTA&#34;&#62;);&#13;&#10;     Memo13.TEXT    := FORMATFLOAT('#,##0.00', &#60;APURACAO.&#34;PARCELADESONERADA&#34;&#62;);&#13;&#10;     Memo14.TEXT    := FORMATFLOAT('#,##0.00', &#60;APURACAO.&#34;TRIBUTO1&#34;&#62;);&#13;&#10;     Memo15.TEXT    := FORMATFLOAT('#,##0.00', &#60;APURACAO.&#34;TRIBUTO2&#34;&#62;);&#13;&#10;     Memo16.TEXT    := FORMATFLOAT('#,##0.00', &#60;APURACAO.&#34;TRIBUTO3&#34;&#62;);&#13;&#10;     Memo23.TEXT    := FORMATFLOAT('#,##0.00', &#60;APURACAO.&#34;ICMS_RECOLHER&#34;&#62;);&#13;&#10;     Memo22.TEXT    := FORMATFLOAT('#,##0.00', X_ACUMULADO);&#13;&#10;  END&#13;&#10;  ELSE BEGIN&#13;&#10;     Memo11.TEXT    := '  /    ';&#13;&#10;     Memo12.TEXT    := '';&#13;&#10;     Memo13.TEXT    := '';&#13;&#10;     Memo14.TEXT    := '';&#13;&#10;     Memo15.TEXT    := '';&#13;&#10;     Memo16.TEXT    := '';&#13;&#10;     Memo23.TEXT    := '';&#13;&#10;     Memo22.TEXT    := '';&#13;&#10;  END;&#13;&#10;  APURACAO.Next;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Sum_rioDo_Relat_rio1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF APURACAO.ISEMPTY THEN BEGIN&#13;&#10;     MEMO19.TEXT:= MEMO19.TEXT + 'Sem Movimentação';&#13;&#10;     EXIT;&#13;&#10;  END;&#13;&#10;&#13;&#10;  DIA_INICIO   := DAYOF(&#60;DMCadastro.QREmpresa.&#34;INICIOATIVIDADE&#34;&#62;);&#13;&#10;  MES_INICIO   := MONTHOF(&#60;DMCadastro.QREmpresa.&#34;INICIOATIVIDADE&#34;&#62;);&#13;&#10;  ANO_INICIO   := YEAROF(&#60;DMCadastro.QREmpresa.&#34;INICIOATIVIDADE&#34;&#62;);&#13;&#10;&#13;&#10;  MES          := STRTOINT(COPY(&#60;APURACAO.&#34;MESMOVTO&#34;&#62;, 1, 2));&#13;&#10;  ANO          := STRTOINT(COPY(&#60;APURACAO.&#34;MESMOVTO&#34;&#62;, 4, 4));&#13;&#10;&#13;&#10;  IF (ANO = ANO_INICIO) AND (MES &#60;&#62; 1) THEN&#13;&#10;     IF MES &#62;= MES_INICIO THEN&#13;&#10;          MES_USA   := MES - MES_INICIO&#13;&#10;     ELSE&#13;&#10;          MES_USA   := 1&#13;&#10;  ELSE&#13;&#10;     MES_USA   := MES;&#13;&#10;&#13;&#10;  MEDIA        := (X_ACUMULADO / MES_USA) * 12;&#13;&#10;  MEDIAESTIM   := (&#60;APURACAO.&#34;ENQUADRAMENTOFINAL&#34;&#62;);&#13;&#10;&#13;&#10;  IF &#60;DMCadastro.QREmpresa.&#34;SIMPLESESTADUAL&#34;&#62; = 'ME' THEN BEGIN&#13;&#10;    FOR I := MES_USA TO 12 DO BEGIN&#13;&#10;      MEDIA := (X_ACUMULADO / MES_USA) * I;&#13;&#10;      IF (MEDIA &#62; &#60;ENQUADRAMENTO.&#34;MERECEITABRUTAFINAL&#34;&#62;) THEN BEGIN&#13;&#10;        MEMO19.TEXT := MEMO19.TEXT + 'Provável mudança de enquadramento de ME para EPP em ' + MESDOANO('01/' + INTTOSTR(I)+'/2000') + ' de ' + INTTOSTR(ANO);&#13;&#10;        EXIT;&#13;&#10;      END;&#13;&#10;    END;&#13;&#10;  END;&#13;&#10;&#13;&#10;  IF &#60;DMCadastro.QREmpresa.&#34;SIMPLESESTADUAL&#34;&#62; = 'EPP' THEN BEGIN&#13;&#10;    FOR I := MES_USA TO 12 DO BEGIN&#13;&#10;      MEDIA := (X_ACUMULADO / MES_USA) * I;&#13;&#10;      IF (MEDIA &#62; &#60;ENQUADRAMENTO.&#34;EPPRECEITABRUTAFINAL3&#34;&#62;) THEN BEGIN&#13;&#10;        MEMO19.TEXT := MEMO19.TEXT + 'Provável mudança de enquadramento de EPP para NORMAL em ' + MESDOANO('01/'+INTTOSTR(I) + '/2000') + ' de ' + INTTOSTR(ANO);&#13;&#10;        EXIT;&#13;&#10;      END;&#13;&#10;    END;&#13;&#10;  END;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  APURACAO.OPEN;&#13;&#10;  APURACAO.first;&#13;&#10;  X_ACUMULADO := 0;&#13;&#10;&#13;&#10;//  DATABASE_NAME  := SIGHAPARAM('DATABASENAME');&#13;&#10;&#13;&#10;//  IF (DATABASE_NAME = '') OR (DATABASE_NAME = '0') THEN BEGIN&#13;&#10;//     EMPRESA.DATABASE.NAME  := 'IBDBMulti';&#13;&#10;//     APURACAO.DATABASE.NAME := 'IBDBMulti';&#13;&#10;//  END&#13;&#10;//  ELSE BEGIN&#13;&#10;//     EMPRESA.DATABASE.NAME  := DATABASE_NAME;&#13;&#10;//     APURACAO.DATABASE.NAME := DATABASE_NAME;&#13;&#10;//  END;&#13;&#10;end." PropData="">
