<TfrxReport Version="3.24" DotMatrixReport="False" EngineOptions.UseFileCache="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4031" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PrintOptions.Printer="NOTAESTRADA em APL2 (de ROSICLER) na sessão 1" ReportOptions.Author="NAME" ReportOptions.CreateDate="37831,462771412" ReportOptions.Description.Text="" ReportOptions.Name="Personalizado - NOTA FISCAL Nº. 27" ReportOptions.LastChange="39867,4017909028" ReportOptions.VersionMinor="èˆ" ReportOptions.VersionRelease="üÿ’˜Çe4" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  XVALORDESCONTOPRODUTO, XVALORDESCONTOSERVICO,&#13;&#10;  XTOTALPRODUTO, XTOTALSERVICO : Real;&#13;&#10;&#13;&#10;  XCONTAPRODUTO, XCONTASERVICO, xxULTIMAPOSICAO, J, I : Integer;&#13;&#10;&#13;&#10;  CODIGOSFISCAIS, auxDesc : String;&#13;&#10;  FIM, xxACHOU            : Boolean;&#13;&#10;&#13;&#10;  COD_NCM, CODTRIBICMS : array[0..30] of String;&#13;&#10;&#13;&#10;  X_RAZAOSOCIAL, X_CNPJ, X_IE, X_ENDERECO, X_BAIRRO, X_CEP,&#13;&#10;  X_CIDADE, X_ESTADO, X_FONEFAX, xxULTIMALETRA : String;&#13;&#10;&#13;&#10;procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if (&#60;NOTAFISCAL.&#34;DOCUMENTO&#34;&#62; = 101300) then begin&#13;&#10;    //PREMIO   := 'VENDA PREMIADA! CLIENTE PREMIADO: ' + VARTOSTR(&#60;CLIENTE.&#34;CODIGOCLIENTE&#34;&#62;) + ' - ' + &#60;CLIENTE.&#34;NOME&#34;&#62; + ' NF.: ' + VARTOSTR(&#60;NOTAFISCAL.&#34;DOCUMENTO&#34;&#62;);&#13;&#10;    //QRPREMIADA.EXECSQL;&#13;&#10;    //PROGRESS := 'CLIENTE GANHADOR DO PREMIO SURPRESA';&#13;&#10;    //messagebox(chr(13)+chr(13)+'CLIENTE GANHADOR DO PREMIO SURPRESA!'+chr(13)+chr(13)+'CLIENTE GANHADOR DO PREMIO SURPRESA!'+chr(13)+chr(13)+'CLIENTE GANHADOR DO PREMIO SURPRESA!'+chr(13)+chr(13)+'CLIENTE GANHADOR DO PREMIO SURPRESA!'+chr(13)+chr(13)+'CLIENTE GANHADOR DO PREMIO SURPRESA!'+chr(13)+chr(13)+'CLIENTE GANHADOR DO PREMIO SURPRESA!'+chr(13)+chr(13), 'VENDA PREMIADA!');&#13;&#10;  end;&#13;&#10;&#13;&#10;  FOR J := 0 TO 20 DO&#13;&#10;    COD_NCM[J] := '';&#13;&#10;&#13;&#10;  if (&#60;FRNotafiscalDetalhes.&#34;RAZAOSOCIAL&#34;&#62; &#60;&#62; '') then begin&#13;&#10;     X_RAZAOSOCIAL  := &#60;FRNotafiscalDetalhes.&#34;RAZAOSOCIAL&#34;&#62;;&#13;&#10;     X_CNPJ         := &#60;FRNotafiscalDetalhes.&#34;CNPJ&#34;&#62;;&#13;&#10;     X_IE           := &#60;FRNotafiscalDetalhes.&#34;IE&#34;&#62;;&#13;&#10;     X_ENDERECO     := &#60;FRNotafiscalDetalhes.&#34;ENDERECO&#34;&#62; + ' ' + VARTOSTR(&#60;FRNotafiscalDetalhes.&#34;ENDERECONUMERORUA&#34;&#62;);&#13;&#10;     X_BAIRRO       := &#60;FRNotafiscalDetalhes.&#34;ENDERECOBAIRRO&#34;&#62;;&#13;&#10;     X_CEP          := &#60;FRNotafiscalDetalhes.&#34;CEP&#34;&#62;;&#13;&#10;     X_CIDADE       := &#60;FRNotafiscalDetalhes.&#34;CIDADE&#34;&#62;;&#13;&#10;     X_ESTADO       := &#60;FRNotafiscalDetalhes.&#34;ESTADO&#34;&#62;;&#13;&#10;     X_FONEFAX      := FORMATAFONE(&#60;FRNotafiscalDetalhes.&#34;FONEFAX&#34;&#62;);&#13;&#10;  end&#13;&#10;  else if &#60;CLIENTE.&#34;NOME&#34;&#62; &#60;&#62; '' then begin&#13;&#10;     X_RAZAOSOCIAL  := VARTOSTR(&#60;CLIENTE.&#34;CODIGOCLIENTE&#34;&#62;) + ' - ' + &#60;CLIENTE.&#34;NOME&#34;&#62;;&#13;&#10;     IF &#60;CLIENTE.&#34;PESSOAFISICAJURIDICA&#34;&#62; = 'J' THEN&#13;&#10;       X_CNPJ       := FORMATACNPJ(&#60;CLIENTE.&#34;CPFCNPJ&#34;&#62;)&#13;&#10;     ELSE&#13;&#10;       X_CNPJ       := FORMATACPF(&#60;CLIENTE.&#34;CPFCNPJ&#34;&#62;);&#13;&#10;     X_IE           := &#60;CLIENTE.&#34;RGIE&#34;&#62;;&#13;&#10;     X_ENDERECO     := &#60;CLIENTE.&#34;ENDERECO&#34;&#62; + ' ' + VARTOSTR(&#60;CLIENTE.&#34;ENDERECONUMERORUA&#34;&#62;);&#13;&#10;     X_BAIRRO       := &#60;CLIENTE.&#34;ENDERECOBAIRRO&#34;&#62;;&#13;&#10;     X_CEP          := &#60;CLIENTE.&#34;CEP&#34;&#62;;&#13;&#10;     X_CIDADE       := &#60;CLIENTECIDADE.&#34;CIDADE&#34;&#62;;&#13;&#10;     X_ESTADO       := &#60;CLIENTECIDADE.&#34;ESTADO&#34;&#62;;&#13;&#10;     X_FONEFAX      := FORMATAFONE(&#60;CLIENTE.&#34;COMERCIALFONE&#34;&#62;);&#13;&#10;  end&#13;&#10;  else if &#60;FORNECEDOR.&#34;NOME&#34;&#62; &#60;&#62; '' then begin&#13;&#10;     X_RAZAOSOCIAL  := VARTOSTR(&#60;FORNECEDOR.&#34;CODIGO&#34;&#62;) + ' - ' + &#60;FORNECEDOR.&#34;NOME&#34;&#62;;&#13;&#10;     IF &#60;FORNECEDOR.&#34;PESSOAFISICAJURIDICA&#34;&#62; = 'J' THEN&#13;&#10;       X_CNPJ         := FORMATACNPJ(&#60;FORNECEDOR.&#34;CPFCNPJ&#34;&#62;)&#13;&#10;     ELSE&#13;&#10;       X_CNPJ         := FORMATACPF(&#60;FORNECEDOR.&#34;CPFCNPJ&#34;&#62;);&#13;&#10;     X_IE           := &#60;FORNECEDOR.&#34;RGIE&#34;&#62;;&#13;&#10;     X_ENDERECO     := &#60;FORNECEDOR.&#34;ENDERECO&#34;&#62; + ' ' + VARTOSTR(&#60;FORNECEDOR.&#34;ENDERECONUMERORUA&#34;&#62;);&#13;&#10;     X_BAIRRO       := &#60;FORNECEDOR.&#34;ENDERECOBAIRRO&#34;&#62;;&#13;&#10;     X_CEP          := &#60;FORNECEDORCIDADE.&#34;CEP&#34;&#62;;&#13;&#10;     X_CIDADE       := &#60;FORNECEDORCIDADE.&#34;CIDADE&#34;&#62;;&#13;&#10;     X_ESTADO       := &#60;FORNECEDORCIDADE.&#34;ESTADO&#34;&#62;;&#13;&#10;     X_FONEFAX      := FORMATAFONE(&#60;FORNECEDOR.&#34;COMERCIALFONE&#34;&#62;);&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure PageHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  mmDuplicataI.lines.clear;&#13;&#10;  mmDuplicataII.lines.clear;&#13;&#10;  mmDuplicataIII.lines.clear;&#13;&#10;&#13;&#10;  mmCodigoProduto.lines.clear;&#13;&#10;  mmDescricaoProduto.lines.clear;&#13;&#10;//  mmCFOP.lines.clear;&#13;&#10;  mmCST.lines.clear;&#13;&#10;  mmUnidadeProduto.lines.clear;&#13;&#10;  mmQuantidadeProduto.lines.clear;&#13;&#10;//  mmProdutoNCM.lines.clear;&#13;&#10;//  mmClassFiscal.lines.clear;&#13;&#10;  mmValorUnitarioProduto.lines.clear;&#13;&#10;  mmValorTotalProduto.lines.clear;&#13;&#10;  mmPercIcms.lines.clear;&#13;&#10;//  mmDesconto.lines.clear;&#13;&#10;&#13;&#10;//  mmDescricaoServico.lines.clear;&#13;&#10;//  mmValorTotalServico.lines.clear;&#13;&#10;//  mmPercISS.lines.clear;&#13;&#10;//  mmValorISS.lines.clear;&#13;&#10;&#13;&#10;  XCONTAPRODUTO := 12;&#13;&#10;&#13;&#10;  FOR I := 1 TO 1 DO BEGIN&#13;&#10;    IF NOT DUPLICATA.EOF THEN&#13;&#10;      mmDuplicataI.lines.Add(DATETOSTR(&#60;DUPLICATA.&#34;VENCIMENTO&#34;&#62;) +#13+ 'R$' + FORMATFLOAT('#,##0.00', &#60;DUPLICATA.&#34;VALORSALDO&#34;&#62;));&#13;&#10;    DUPLICATA.NEXT;&#13;&#10;  END;&#13;&#10;&#13;&#10;  FOR I := 1 TO 1 DO BEGIN&#13;&#10;    IF NOT DUPLICATA.EOF THEN&#13;&#10;      mmDuplicataII.lines.Add(DATETOSTR(&#60;DUPLICATA.&#34;VENCIMENTO&#34;&#62;) +#13+ 'R$' + FORMATFLOAT('#,##0.00', &#60;DUPLICATA.&#34;VALORSALDO&#34;&#62;));&#13;&#10;    DUPLICATA.NEXT;&#13;&#10;  END;&#13;&#10;&#13;&#10;  FOR I := 1 TO 1 DO BEGIN&#13;&#10;    IF NOT DUPLICATA.EOF THEN&#13;&#10;      mmDuplicataIII.lines.Add(DATETOSTR(&#60;DUPLICATA.&#34;VENCIMENTO&#34;&#62;) +#13+ 'R$' + FORMATFLOAT('#,##0.00', &#60;DUPLICATA.&#34;VALORSALDO&#34;&#62;));&#13;&#10;    DUPLICATA.NEXT;&#13;&#10;  END;&#13;&#10;&#13;&#10;  FOR I := 1 TO 1 DO BEGIN&#13;&#10;    IF NOT DUPLICATA.EOF THEN&#13;&#10;      mmDuplicataIIII.lines.Add(DATETOSTR(&#60;DUPLICATA.&#34;VENCIMENTO&#34;&#62;) +#13+ 'R$' + FORMATFLOAT('#,##0.00', &#60;DUPLICATA.&#34;VALORSALDO&#34;&#62;));&#13;&#10;    DUPLICATA.NEXT;&#13;&#10;  END;&#13;&#10;&#13;&#10;  FOR I := 1 TO XCONTAPRODUTO DO BEGIN&#13;&#10;    IF NOT COMBUSTIVEL.EOF THEN BEGIN&#13;&#10;      mmCodigoProduto.lines.Add(INTTOSTR(&#60;COMBUSTIVEL.&#34;CODIGOCOMBUSTIVEL&#34;&#62;));&#13;&#10;      mmDescricaoProduto.lines.Add(COPY(&#60;COMBUSTIVEL.&#34;DESCRICAO&#34;&#62;,1,60));&#13;&#10;//      mmCFOP.lines.add(COPY(&#60;COMBUSTIVEL.&#34;CODIGOFISCAL&#34;&#62;, 1, 5));&#13;&#10;      mmCST.lines.add(COPY(&#60;COMBUSTIVEL.&#34;CODIGOTRIBUTACAO&#34;&#62;, 1, 3));&#13;&#10;      mmUnidadeProduto.lines.add(COPY(&#60;COMBUSTIVEL.&#34;UNIDADE&#34;&#62;, 1, 3));&#13;&#10;      mmQuantidadeProduto.lines.add(FORMATFLOAT('#,##0.00', &#60;COMBUSTIVEL.&#34;QUANTIDADE&#34;&#62;));&#13;&#10;      mmValorUnitarioProduto.lines.add(&#60;COMBUSTIVEL.&#34;VALORUNITARIO&#34;&#62;);&#13;&#10;      mmValorTotalProduto.lines.add(FORMATFLOAT('#,##0.00', &#60;COMBUSTIVEL.&#34;VALORTOTAL&#34;&#62;));&#13;&#10;      mmPercIcms.lines.add(FORMATFLOAT('#,##0.##', &#60;COMBUSTIVEL.&#34;VALORICMSALIQUOTA&#34;&#62;));&#13;&#10;//      mmPercIPI.lines.add(FORMATFLOAT('#,##0.##', 0));&#13;&#10;//      mmValorIPI.lines.add(FORMATFLOAT('#,##0.##', 0));&#13;&#10;//      mmClassFiscal.lines.add('');&#13;&#10;      If (COPY(&#60;COMBUSTIVEL.&#34;CODIGOTRIBUTACAO&#34;&#62;, 2, 1)='6') Then begin&#13;&#10;        IF MEMOOBS.TEXT = '' THEN&#13;&#10;          MEMOOBS.LINES.ADD('Valores do ICMS Substituição Tributária');&#13;&#10;        MEMOOBS.LINES.ADD(INTTOSTR(&#60;COMBUSTIVEL.&#34;CODIGOCOMBUSTIVEL&#34;&#62;)+' - BCS R$'&#13;&#10;        +FORMATFLOAT('#,##0.00', (&#60;COMBUSTIVEL.&#34;ICMSSUBBASECALCULO&#34;&#62;*&#60;COMBUSTIVEL.&#34;QUANTIDADE&#34;&#62;))&#13;&#10;        +'  ICMS R$'+FORMATFLOAT('#,##0.00', (&#60;COMBUSTIVEL.&#34;ICMSSUB&#34;&#62;*&#60;COMBUSTIVEL.&#34;QUANTIDADE&#34;&#62;)));&#13;&#10;      End;&#13;&#10;      XVALORDESCONTOPRODUTO := XVALORDESCONTOPRODUTO  + &#60;COMBUSTIVEL.&#34;VALORDESCONTO&#34;&#62;;&#13;&#10;      XTOTALPRODUTO := XTOTALPRODUTO + &#60;COMBUSTIVEL.&#34;VALORTOTAL&#34;&#62;;&#13;&#10;      COMBUSTIVEL.NEXT;&#13;&#10;    END&#13;&#10;    ELSE IF NOT PRODUTO.EOF THEN BEGIN&#13;&#10;      mmCodigoProduto.lines.Add(COPY(&#60;PRODUTO.&#34;CODIGOPRODUTO&#34;&#62;,1,15));&#13;&#10;      mmDescricaoProduto.lines.Add(COPY(&#60;PRODUTO.&#34;NOME&#34;&#62;,1,60));&#13;&#10;//      mmCFOP.lines.add(COPY(&#60;PRODUTO.&#34;CODIGOFISCAL&#34;&#62;, 1, 5));&#13;&#10;      mmCST.lines.add(COPY(&#60;PRODUTO.&#34;CODIGOTRIBUTACAO&#34;&#62;, 1, 3));&#13;&#10;      mmUnidadeProduto.lines.add(COPY(&#60;PRODUTO.&#34;UNIDADE&#34;&#62;, 1, 3));&#13;&#10;      mmQuantidadeProduto.lines.add(FORMATFLOAT('#,##0.00', &#60;PRODUTO.&#34;QUANTIDADE&#34;&#62;));&#13;&#10;      mmValorUnitarioProduto.lines.add(FORMATFLOAT('#,##0.00', &#60;PRODUTO.&#34;VALORUNITARIO&#34;&#62;));&#13;&#10;      mmValorTotalProduto.lines.add(FORMATFLOAT('#,##0.00', &#60;PRODUTO.&#34;VALORTOTAL&#34;&#62;));&#13;&#10;      mmPercIcms.lines.add(FORMATFLOAT('#,##0.##', &#60;PRODUTO.&#34;VALORICMSALIQUOTA&#34;&#62;));&#13;&#10;//      mmPercIPI.lines.add(FORMATFLOAT('#,##0.##', &#60;PRODUTO.&#34;VALORIPIALIQUOTA&#34;&#62;));&#13;&#10;//      mmValorIPI.lines.add(FORMATFLOAT('#,##0.##', &#60;PRODUTO.&#34;VALORIPI&#34;&#62;));&#13;&#10;      If (COPY(&#60;PRODUTO.&#34;CODIGOTRIBUTACAO&#34;&#62;, 2, 1)='6') Then Begin&#13;&#10;        IF MEMOOBS.TEXT = '' THEN&#13;&#10;         MEMOOBS.LINES.ADD('Valores do ICMS Substituição Tributária');&#13;&#10;         MEMOOBS.LINES.ADD(&#60;PRODUTO.&#34;CODIGOPRODUTO&#34;&#62;+' - BCS R$'&#13;&#10;         +FORMATFLOAT('#,##0.00', (&#60;PRODUTO.&#34;BASECALCULOICMS&#34;&#62;*&#60;PRODUTO.&#34;QUANTIDADE&#34;&#62;))&#13;&#10;         +'   ICMS  '+FORMATFLOAT('#,##0.00', (&#60;PRODUTO.&#34;ICMSSUBSTITUICAO&#34;&#62;*&#60;PRODUTO.&#34;QUANTIDADE&#34;&#62;)));&#13;&#10;      end;&#13;&#10;&#13;&#10;      IF (&#60;PRODUTO.&#34;CODIGONCM&#34;&#62; &#60;&#62; '') AND (&#60;PRODUTO.&#34;CODIGONCM&#34;&#62; &#60;&#62; '0') THEN BEGIN&#13;&#10;        xxACHOU         := FALSE;&#13;&#10;        xxULTIMAPOSICAO := -1;&#13;&#10;        xxULTIMALETRA   := '';&#13;&#10;&#13;&#10;      END&#13;&#10;      ELSE&#13;&#10;//        mmClassFiscal.lines.add('');&#13;&#10;&#13;&#10;      XVALORDESCONTOPRODUTO := XVALORDESCONTOPRODUTO  + &#60;PRODUTO.&#34;VALORDESCONTO&#34;&#62;;&#13;&#10;      XTOTALPRODUTO := XTOTALPRODUTO + &#60;PRODUTO.&#34;VALORTOTAL&#34;&#62;;&#13;&#10;      PRODUTO.NEXT;&#13;&#10;    END;&#13;&#10;  END;&#13;&#10;&#13;&#10;  FIM := COMBUSTIVEL.EOF;&#13;&#10;&#13;&#10;  IF FIM = TRUE THEN&#13;&#10;    FIM := PRODUTO.EOF;&#13;&#10;&#13;&#10;  IF (FIM = TRUE) THEN BEGIN&#13;&#10;                                                 //---------------------------------------------------------------------&#13;&#10;    IF XVALORDESCONTOPRODUTO &#60;&#62; 0 THEN BEGIN&#13;&#10;      WHILE mmDescricaoProduto.lines.count &#60; XCONTAPRODUTO - 1 DO&#13;&#10;        mmDescricaoProduto.lines.Add('');&#13;&#10;      IF XVALORDESCONTOPRODUTO &#62; 0 THEN&#13;&#10;         auxDesc := 'Desconto '&#13;&#10;      ELSE BEGIN&#13;&#10;         auxDesc := 'Acréscimo ';&#13;&#10;         XVALORDESCONTOPRODUTO := XVALORDESCONTOPRODUTO * (-1); //pra inverter o sinal&#13;&#10;      END;&#13;&#10;      mmDescricaoProduto.lines.Add(auxDesc + 'produtos.: ' + FORMATFLOAT('#,##0.00', XVALORDESCONTOPRODUTO));&#13;&#10;    END;&#13;&#10;&#13;&#10;    IF &#60;NOTAFISCAL.&#34;OBSERVACAOAMPAROLEGAL&#34;&#62; &#60;&#62; '' THEN&#13;&#10;      MEMOOBS.LINES.ADD(&#60;NOTAFISCAL.&#34;OBSERVACAOAMPAROLEGAL&#34;&#62;);&#13;&#10;&#13;&#10;    IF &#60;NOTAFISCAL.&#34;OBSERVACAO&#34;&#62; &#60;&#62; '' THEN&#13;&#10;      MEMOOBS.LINES.ADD(&#60;NOTAFISCAL.&#34;OBSERVACAO&#34;&#62;);&#13;&#10;&#13;&#10;    DEBITOSCO.OPEN;&#13;&#10;    DEBITOSCR.OPEN;&#13;&#10;&#13;&#10;    IF (&#60;DEBITOSCO.&#34;SALDO&#34;&#62; &#62; 0) OR (&#60;DEBITOSCR.&#34;SALDO&#34;&#62; &#62; 0) THEN&#13;&#10;      MEMOOBS.LINES.ADD('**CR**');&#13;&#10;&#13;&#10;  END;&#13;&#10;&#13;&#10;  IF FIM = TRUE THEN&#13;&#10;    CONTA.LAST&#13;&#10;  ELSE&#13;&#10;    CONTA.FIRST;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF FIM = FALSE THEN&#13;&#10;    ENGINE.NEWPAGE;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  NOTAFISCAL.OPEN;&#13;&#10;  CODIGOFISCAL.CLOSE;&#13;&#10;  CODIGOFISCAL.OPEN;&#13;&#10;  CLIENTE.OPEN;&#13;&#10;  CLIENTECIDADE.OPEN;&#13;&#10;  FORNECEDOR.OPEN;&#13;&#10;  FORNECEDORCIDADE.OPEN;&#13;&#10;  DUPLICATA.OPEN;&#13;&#10;  PRODUTO.OPEN;&#13;&#10;  SERVICO.OPEN;&#13;&#10;  CONTA.OPEN;&#13;&#10;&#13;&#10;  XVALORDESCONTOPRODUTO := 0;&#13;&#10;  XVALORDESCONTOSERVICO := 0;&#13;&#10;  CODIGOSFISCAIS        := '';&#13;&#10;  FIM                   := TRUE;&#13;&#10;&#13;&#10;  PRODUTO.FIRST;&#13;&#10;  WHILE NOT PRODUTO.EOF DO BEGIN&#13;&#10;    IF POS(&#60;PRODUTO.&#34;CODIGOFISCAL&#34;&#62;, CODIGOSFISCAIS) &#60;= 0 THEN BEGIN&#13;&#10;      IF CODIGOSFISCAIS = '' THEN&#13;&#10;        CODIGOSFISCAIS := &#60;PRODUTO.&#34;CODIGOFISCAL&#34;&#62;&#13;&#10;      ELSE&#13;&#10;        CODIGOSFISCAIS := CODIGOSFISCAIS + '/' + &#60;PRODUTO.&#34;CODIGOFISCAL&#34;&#62;;&#13;&#10;    END;&#13;&#10;    PRODUTO.NEXT;&#13;&#10;  END;&#13;&#10;&#13;&#10;  IF CODIGOSFISCAIS = '' THEN&#13;&#10;    CODIGOSFISCAIS := &#60;NOTAFISCAL.&#34;CODIGOFISCAL&#34;&#62;;&#13;&#10;&#13;&#10;  PRODUTO.FIRST;&#13;&#10;  SERVICO.FIRST;&#13;&#10;  DUPLICATA.FIRST;&#13;&#10;end." PropData="">
