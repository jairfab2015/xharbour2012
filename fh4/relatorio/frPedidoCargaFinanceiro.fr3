    <TfrxIBXQuery Name="Carga" UserName="Carga" CloseDataSource="True" FieldAliases.Text="CODIGODESTINO=CODIGODESTINO&#13;&#10;MOTORISTA=MOTORISTA&#13;&#10;DATACARGA=DATACARGA&#13;&#10;CODIGOTRANSPORTADOR=CODIGOTRANSPORTADOR&#13;&#10;NOME=NOME&#13;&#10;VEICULO=VEICULO&#13;&#10;PLACA=PLACA&#13;&#10;LIMITECARGA=LIMITECARGA&#13;&#10;TOTALCARGA=TOTALCARGA&#13;&#10;CIDADE=CIDADE" SQL.Text="select&#13;&#10;C.codigodestino,&#13;&#10;C.motorista,&#13;&#10;C.datacarga,&#13;&#10;C.codigotransportador,&#13;&#10;FO.nome,&#13;&#10;V.descricao AS Veiculo,&#13;&#10;V.placa,&#13;&#10;C.limitecarga,&#13;&#10;C.totalcarga,&#13;&#10;ci.cidade&#13;&#10;from cargaromaneio C&#13;&#10;   inner join cidade CI on (C.codigodestino = CI.codigo)&#13;&#10;   inner join fornecedor FO on (C.codigotransportador = FO.codigo)&#13;&#10;   inner join veiculo V on (C.codigoveiculo = V.codigo)&#13;&#10;WHERE C.codigo = :CARGA" PropData="0A506172616D657465727301010C42000000204E616D653D224341524741222044617461547970653D226674537472696E67222045787072657373696F6E3D227369676861706172616D282763617267612729220000"/>
    <TfrxIBXQuery Name="PEDIDO" UserName="PEDIDO" CloseDataSource="True" FieldAliases.Text="DOCUMENTO=DOCUMENTO&#13;&#10;CODIGONF=CODIGONF&#13;&#10;PEDIDO=PEDIDO&#13;&#10;CODIGOPLANO=CODIGOPLANO&#13;&#10;FORMACOBRANCA=FORMACOBRANCA&#13;&#10;CODIGOOPERACAO=CODIGOOPERACAO&#13;&#10;TOTALPEDIDO=TOTALPEDIDO&#13;&#10;VALORTOTALNF=VALORTOTALNF&#13;&#10;CLIENTE=CLIENTE&#13;&#10;NOME=NOME&#13;&#10;DESCRICAO=DESCRICAO&#13;&#10;OPERACAO=OPERACAO&#13;&#10;NUMERODOCUMENTO=NUMERODOCUMENTO&#13;&#10;NUMEROPARCELA=NUMEROPARCELA&#13;&#10;VALORDUPLICATA=VALORDUPLICATA&#13;&#10;VALORSALDO=VALORSALDO" SQL.Text="SELECT DISTINCT PB.DOCUMENTO, NF.CODIGONF, P.PEDIDO, P.CODIGOPLANO, P.FORMACOBRANCA,&#13;&#10;NF.CODIGOOPERACAO, P.VALORTOTAL AS TOTALPEDIDO, NF.VALORTOTALNF, RL.CODIGO AS CLIENTE,&#13;&#10;RL.NOME, RN.DESCRICAO, OP.DESCRICAO AS OPERACAO, CRE.NUMERODOCUMENTO, CRE.NUMEROPARCELA,&#13;&#10;CRE.VALORDUPLICATA, CRE.VALORSALDO&#13;&#10;from cargaromaneio CR inner join&#13;&#10;cargapedido CP on CR.codigo = CP.codigocarga inner join&#13;&#10;pedido P on CP.codigopedido = P.codigo inner join&#13;&#10;pedidoproduto PP on P.codigo = PP.codigopedido inner join&#13;&#10;pedidobaixaproduto PB on PP.codigo = PB.codigopedidoproduto inner join&#13;&#10;notafiscalproduto NFP on PB.codigopedidoproduto = NFP.controleorigem inner join&#13;&#10;notafiscal NF on NFP.codigonf = NF.codigonf inner join&#13;&#10;operacao OP on NF.codigooperacao = OP.codigolancamento inner join&#13;&#10;regranegocio RN on NF.formapagamento = RN.codigo inner join&#13;&#10;relacao RL on NF.codigorelacao = RL.codigorelacao left join&#13;&#10;contareceber CRE ON NF.codigonf = CRE.codigonf&#13;&#10;WHERE&#13;&#10;NF.NOTAFISCALCANCELADA = 0&#13;&#10;AND NF.TIPOMOVIMENTO = 'S'&#13;&#10;AND CP.CODIGOCARGA = :CARGA&#13;&#10;AND P.PEDIDOTIPO = 'V'&#13;&#10;AND P.CONVERTEPARA = 'N'&#13;&#10;AND P.PEDIDOCARGA = '1'&#13;&#10;AND NFP.controletipo = 'P'&#13;&#10;ORDER BY NF.DOCUMENTO" PropData="0A506172616D657465727301010C42000000204E616D653D224341524741222044617461547970653D226674537472696E67222045787072657373696F6E3D227369676861706172616D282763617267612729220000"/>
    <TfrxIBXQuery Name="TOTAIS" UserName="TOTAIS" CloseDataSource="True" FieldAliases.Text="DESCRICAO=DESCRICAO&#13;&#10;TOTAL=TOTAL" SQL.Text="Select distinct RN.descricao, sum(NFP.valortotal) AS TOTAL&#13;&#10;from cargaromaneio CR inner join&#13;&#10;cargapedido CP on CR.codigo = CP.codigocarga inner join&#13;&#10;pedido P on CP.codigopedido = P.codigo inner join&#13;&#10;pedidoproduto PP on P.codigo = PP.codigopedido inner join&#13;&#10;pedidobaixaproduto PB on PP.codigo = PB.codigopedidoproduto inner join&#13;&#10;notafiscalproduto NFP on PB.codigopedidoproduto = NFP.controleorigem inner join&#13;&#10;notafiscal NF on NFP.codigonf = NF.codigonf inner join&#13;&#10;operacao OP on NF.codigooperacao = OP.codigolancamento inner join&#13;&#10;regranegocio RN on NF.formapagamento = RN.codigo&#13;&#10;WHERE&#13;&#10;--NF.OBSERVACAO LIKE '%REF. PED DE VND N°: '||P.PEDIDO||'%'&#13;&#10;NF.NOTAFISCALCANCELADA = 0&#13;&#10;AND NF.TIPOMOVIMENTO = 'S'&#13;&#10;AND CP.CODIGOCARGA = :CARGA&#13;&#10;AND P.PEDIDOTIPO = 'V'&#13;&#10;AND P.CONVERTEPARA = 'N'&#13;&#10;AND P.PEDIDOCARGA = '1'&#13;&#10;AND NFP.controletipo = 'P'&#13;&#10;GROUP BY RN.descricao&#13;&#10;ORDER BY RN.descricao" PropData="0A506172616D657465727301010C42000000204E616D653D224341524741222044617461547970653D226674537472696E67222045787072657373696F6E3D227369676861706172616D282763617267612729220000"/>
