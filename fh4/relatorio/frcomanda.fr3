    <TfrxIBXQuery Name="MESA" UserName="MESA" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT B.*, CAST(P.NOME||&#13;&#10;(CASE WHEN B.ESPECIAL IS NULL THEN '' ELSE '-'||B.ESPECIAL END)||&#13;&#10;(CASE WHEN P2.NOME IS NULL THEN '' ELSE ' / '||P2.NOME END)||&#13;&#10;(CASE WHEN B.ESPECIAL2 IS NULL THEN '' ELSE '-'||B.ESPECIAL2 END)||&#13;&#10;(CASE WHEN P3.NOME IS NULL THEN '' ELSE ' / '||P3.NOME END)||&#13;&#10;(CASE WHEN B.ESPECIAL3 IS NULL THEN '' ELSE '-'||B.ESPECIAL3 END)||&#13;&#10;(CASE WHEN P4.NOME IS NULL THEN '' ELSE ' / '||P4.NOME END)||&#13;&#10;(CASE WHEN B.ESPECIAL4 IS NULL THEN '' ELSE '-'||B.ESPECIAL4 END) AS VARCHAR(255)) AS NOME,&#13;&#10;P.UNIDADE&#13;&#10;FROM BAR_CONTA B&#13;&#10;INNER JOIN PRODUTO P ON P.CODIGO = B.CODIGOPRODUTO&#13;&#10;LEFT JOIN PRODUTO P2 ON P2.CODIGO = B.CODIGOPRODUTO2 AND P2.CODIGO &#60;&#62; '0'&#13;&#10;LEFT JOIN PRODUTO P3 ON P3.CODIGO = B.CODIGOPRODUTO3 AND P3.CODIGO &#60;&#62; '0'&#13;&#10;LEFT JOIN PRODUTO P4 ON P4.CODIGO = B.CODIGOPRODUTO4 AND P4.CODIGO &#60;&#62; '0'&#13;&#10;LEFT JOIN LINHA L ON P.LINHA = L.CODIGO&#13;&#10;WHERE ((B.MESA = :MESA) AND (L.IMPRESSORA = :IMPRESSORA) AND&#13;&#10;(B.SITUACAO = 0)) OR (B.CODIGO = :CODIGO)&#13;&#10;ORDER BY B.POSICAO, B.DATAHORA" PropData=""/>
