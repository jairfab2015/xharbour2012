                  sJsJL     " ReportOptions.VersionMinor="èˆ" ReportOptions.VersionRelease="üÿ’˜Çe4" ScriptLanguage="PascalScript" ScriptText.Text="Var    TOTAL,&#13;&#10;       RECEBIMENTOS, PAGAMENTOS,  EXTRA,&#13;&#10;       VENDAPRODUTO,&#13;&#10;       TOTALAFERICAO, TOTALQTDEBOMBA, TOTALVENDABOMBA, TOTALVENDA,&#13;&#10;       TOTALQTDEVENDA, TOTALDESCONTO, TOTALDIFERENCA, TOTALDIFERENCALITRO,&#13;&#10;       TOTALCHEQUE, VENDAOPERACAO, TOTALPRODUTO, SALDOCAIXA,&#13;&#10;       TOTALPRODUTODESC, TOTALRECEBIDO, SALDOTROCO,TOTALGRUPO,&#13;&#10;       RECEBIMENTO,TOTALPRAZO, PAGAMENTO,TOTALCC, VALORFORMA, TOTALFISICO,&#13;&#10;       FCHEQUEPRE, DIFERENCACC, VALORDIFERENCA, VALORDIFERENCAN_FORM :REAL;&#13;&#10;       TIPOCH, TIPOMOV,  DIFERENCA, PRECO : STRING;&#13;&#10;       DATAI, DATAF : DATE;&#13;&#10;       PRECOATUAL : BOOLEAN;&#13;&#10;&#13;&#10;procedure MasterHeader3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALCHEQUE := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF (&#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'D') THEN&#13;&#10;    TIPOMOV       := 'Pagamento'&#13;&#10;  ELSE IF (&#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'C') THEN&#13;&#10;    TIPOMOV       := 'Recebimento'&#13;&#10;  ELSE&#13;&#10;    TIPOMOV       := 'Extra - Caixa';&#13;&#10;  TOTALGRUPO      := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Band2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF &#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'D' THEN BEGIN&#13;&#10;    TOTAL      := TOTAL      - &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;    TOTALGRUPO := TOTALGRUPO + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;    PAGAMENTO  := PAGAMENTO  + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;  END&#13;&#10;  ELSE IF &#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'C' THEN BEGIN&#13;&#10;    TOTAL      := TOTAL      + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;    TOTALGRUPO := TOTALGRUPO + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;    RECEBIMENTO:= RECEBIMENTO+ &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;  END&#13;&#10;  ELSE IF &#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'E' THEN BEGIN&#13;&#10;      TOTAL      := TOTAL      + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;      TOTALGRUPO := TOTALGRUPO + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;  END;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterHeader6OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALRECEBIDO := 0;&#13;&#10;  SALDOTROCO    := ((&#60;CAIXAABERTURA.&#34;TROCOINICIAL&#34;&#62; - &#60;CAIXAABERTURA.&#34;TROCOFINAL&#34;&#62;) + (&#60;CAIXAABERTURA.&#34;REPOSICAO&#34;&#62; - &#60;CAIXAABERTURA.&#34;RETIRADA&#34;&#62;));&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData4OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALRECEBIDO := TOTALRECEBIDO + &#60;CRRECEBIDAS.&#34;VALORPAGO&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData5OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALAFERICAO       := TOTALAFERICAO + &#60;BOMBAS_MAPA.&#34;AFERICAO&#34;&#62;;&#13;&#10;     TOTALQTDEBOMBA      := TOTALQTDEBOMBA + (&#60;BOMBAS_MAPA.&#34;L_FINAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;L_INICIAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;AFERICAO&#34;&#62;);&#13;&#10;     TOTALVENDABOMBA     := TOTALVENDABOMBA + ((&#60;BOMBAS_MAPA.&#34;L_FINAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;L_INICIAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;AFERICAO&#34;&#62;) * &#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;);&#13;&#10;     TOTALVENDA          := TOTALVENDA + (&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;) ;&#13;&#10;     TOTALQTDEVENDA      := TOTALQTDEVENDA + &#60;BOMBAS_MAPA.&#34;VENDA&#34;&#62;;&#13;&#10;     TOTALDESCONTO       := TOTALDESCONTO + ((&#60;BOMBAS_MAPA.&#34;VENDA&#34;&#62;*&#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;) -(&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;));&#13;&#10;     TOTALDIFERENCALITRO := TOTALDIFERENCALITRO + ((&#60;BOMBAS_MAPA.&#34;L_FINAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;L_INICIAL_MANUAL&#34;&#62;) - &#60;BOMBAS_MAPA.&#34;VENDA&#34;&#62;);&#13;&#10;     //TOTALDIFERENCA    := TOTALDIFERENCA + ((((&#60;BOMBAS_MAPA.&#34;L_FINAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;L_INICIAL_MANUAL&#34;&#62;) * &#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;) - (&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;)+((&#60;BOMBAS_MAPA.&#34;VENDA&#34;&#62;*&#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;) -(&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;))));&#13;&#10;     TOTALDIFERENCA      := TOTALDIFERENCA +     ((&#60;BOMBAS_MAPA.&#34;L_FINAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;L_INICIAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;AFERICAO&#34;&#62;) * &#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;) - (&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;+((&#60;BOMBAS_MAPA.&#34;VENDA&#34;&#62;*&#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;) -(&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;)) );&#13;&#10;&#13;&#10;     PRECOCOMB.CLOSE;&#13;&#10;     PRECOCOMB.PARAMBYNAME('BOMBA').VALUE := (&#60;BOMBAS_MAPA.&#34;CODIGO&#34;&#62;);&#13;&#10;     PRECOCOMB.OPEN;&#13;&#10;&#13;&#10;     IF ((&#60;PRECOCOMB.&#34;PRECOPRAZO&#34;&#62;) &#62; 0 ) THEN&#13;&#10;       PRECO := FORMATFLOAT('#,###0.000',&#60;PRECOCOMB.&#34;PRECOPRAZO&#34;&#62;)&#13;&#10;     ELSE BEGIN&#13;&#10;       PRECOATUAL := TRUE;&#13;&#10;       PRECO := FORMATFLOAT('#,###0.000',(&#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;));&#13;&#10;     END;&#13;&#10;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALAFERICAO       := 0;&#13;&#10;     TOTALQTDEBOMBA      := 0;&#13;&#10;     TOTALVENDABOMBA     := 0;&#13;&#10;     TOTALVENDA          := 0;&#13;&#10;     TOTALQTDEVENDA      := 0;&#13;&#10;     TOTALDESCONTO       := 0;&#13;&#10;     TOTALDIFERENCA      := 0;&#13;&#10;     TOTALDIFERENCALITRO := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     VENDAPRODUTO := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Subreport2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  MOVIMENTONF_MAPA.OPEN;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  VENDAOPERACAO := 0;&#13;&#10;  MOVIMENTONF.OPEN;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData7OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  VALORFORMA    := 0;&#13;&#10;//  DIFERENCACC   := 0;&#13;&#10;//  DIFERENCACC   := (VENDAOPERACAO + &#60;MOVIMENTONF.&#34;TOTALNF&#34;&#62;) - (VENDAPRODUTO + TOTALVENDA);&#13;&#10;  MOVIMENTONF.NEXT;&#13;&#10;  IF (NOT MOVIMENTONF.EOF) THEN BEGIN&#13;&#10;     MOVIMENTONF.PRIOR;&#13;&#10;     VALORFORMA := &#60;MOVIMENTONF.&#34;TOTALNF&#34;&#62;;&#13;&#10;  END&#13;&#10;  ELSE&#13;&#10;     VALORFORMA := (&#60;MOVIMENTONF.&#34;TOTALNF&#34;&#62; - DIFERENCACC);&#13;&#10;&#13;&#10;  VENDAOPERACAO := VENDAOPERACAO + VALORFORMA;&#13;&#10;&#13;&#10;  MasterData19.VISIBLE := (&#60;MOVIMENTONF.&#34;CODIGOLANCAMENTO&#34;&#62; = '1099');&#13;&#10;  Header8.VISIBLE      := (&#60;MOVIMENTONF.&#34;CODIGOLANCAMENTO&#34;&#62; = '1099');&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header4OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALCHEQUE := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData8OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALCHEQUE := TOTALCHEQUE + &#60;CHEQUE.&#34;VALOR&#34;&#62;;&#13;&#10;  IF &#60;CHEQUE.&#34;TIPOCHEQUE&#34;&#62; = 'P' THEN&#13;&#10;     TIPOCH := 'Pré-datado';&#13;&#10;  IF &#60;CHEQUE.&#34;TIPOCHEQUE&#34;&#62; = 'D' THEN&#13;&#10;     TIPOCH := 'Devolvido';&#13;&#10;  IF &#60;CHEQUE.&#34;TIPOCHEQUE&#34;&#62; = 'T' THEN&#13;&#10;     TIPOCH := 'Troca';&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header5OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALCC    := 0;&#13;&#10;  TOTALPRAZO := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData9OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALCC := TOTALCC + &#60;NOTAFISCALCC.&#34;VALOR&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData10OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALPRAZO := TOTALPRAZO + &#60;NOTAFISCALCR.&#34;VALORSALDO&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header7OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALPRODUTO     := 0;&#13;&#10;  TOTALPRODUTODESC := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData11OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALPRODUTO     := TOTALPRODUTO + &#60;NOTAFISCALPRODUTO.&#34;TOTAL&#34;&#62;;&#13;&#10;//     TOTALPRODUTODESC := TOTALPRODUTODESC + &#60;NOTAFISCALPRODUTO.&#34;DESCONTO&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btokOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF (RBRESUMIDO.CHECKED) THEN BEGIN&#13;&#10;     MasterData12.Visible  := false;&#13;&#10;     MasterData13.Visible  := false;&#13;&#10;     MasterData14.Visible  := false;&#13;&#10;     MasterData15.Visible  := false;&#13;&#10;     MasterHeader6.Visible := false;&#13;&#10;     MasterData4.Visible   := false;&#13;&#10;     MasterFooter7.Visible := false;&#13;&#10;  END;&#13;&#10;&#13;&#10;  IF (RBDETALHADO.CHECKED) THEN BEGIN&#13;&#10;     MasterData12.Visible  := true;&#13;&#10;     MasterData13.Visible  := true;&#13;&#10;     MasterData14.Visible  := true;&#13;&#10;     MasterData15.Visible  := true;&#13;&#10;     MasterHeader6.Visible := true;&#13;&#10;     MasterData4.Visible   := true;&#13;&#10;     MasterFooter7.Visible := true;&#13;&#10;  END;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData17OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALFISICO          := 0;&#13;&#10;  SALDOTROCO           := (&#60;CAIXAABERTURA.&#34;TROCOINICIAL&#34;&#62; - &#60;CAIXAABERTURA.&#34;TROCOFINAL&#34;&#62;) + (&#60;CAIXAABERTURA.&#34;REPOSICAO&#34;&#62; - &#60;CAIXAABERTURA.&#34;RETIRADA&#34;&#62;);&#13;&#10;  TOTALFISICO          := &#60;CAIXAABERTURA.&#34;DINHEIRO&#34;&#62;+&#60;CAIXAABERTURA.&#34;CHEQUEAVISTA&#34;&#62;+&#60;CAIXAABERTURA.&#34;CHEQUEPRAZO&#34;&#62;+&#60;CAIXAABERTURA.&#34;CARTAO&#34;&#62;+&#60;CAIXAABERTURA.&#34;VENDAPRAZO&#34;&#62; + &#60;CAIXAABERTURA.&#34;CONTACORRENTE&#34;&#62; + &#60;CAIXAABERTURA.&#34;VALES&#34;&#62;+&#60;CAIXAABERTURA.&#34;OUTROS&#34;&#62; - &#60;CAIXAABERTURA.&#34;CONTRAVALE&#34;&#62;;&#13;&#10;  VALORDIFERENCA       :=((SALDOTROCO+RECEBIMENTO-PAGAMENTO) - TOTALFISICO);&#13;&#10;  VALORDIFERENCAN_FORM :=((SALDOTROCO+RECEBIMENTO-PAGAMENTO) - TOTALFISICO);&#13;&#10;&#13;&#10;  IF (VALORDIFERENCAN_FORM = 0) THEN&#13;&#10;    DIFERENCA := 'FECHAMENTO FÍSICO CONFERE COM O FECHAMENTO CONTÁBIL.'&#13;&#10;  ELSE IF (VALORDIFERENCA &#62; 0) THEN&#13;&#10;    DIFERENCA := 'Diferença do Caixa: FALTA = R$' + FORMATFLOAT('#,##0.00', VALORDIFERENCA)&#13;&#10;  ELSE&#13;&#10;    DIFERENCA := 'Diferença do Caixa: SOBRA = R$' + FORMATFLOAT('#,##0.00', ABS(VALORDIFERENCA));&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData16OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SALDOTROCO    := ((&#60;CAIXAABERTURA.&#34;TROCOINICIAL&#34;&#62; - &#60;CAIXAABERTURA.&#34;TROCOFINAL&#34;&#62;) + (&#60;CAIXAABERTURA.&#34;REPOSICAO&#34;&#62; - &#60;CAIXAABERTURA.&#34;RETIRADA&#34;&#62;));&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData20OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  MasterData20.VISIBLE := (&#60;OBSRETIRADA.&#34;OBSERVACAO&#34;&#62; &#60;&#62; '')&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Footer1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF PRECOATUAL = TRUE THEN&#13;&#10;    SHOWMESSAGE('O REFERIDO CAIXA NÃO POSSUI OS PREÇOS DA DATA DE SEU FECHAMENTO!!!'+#13 +'EM FUNÇÃO DISSO OS PREÇOS EXIBIDOS SÃO DO CADASTRO ATUAL DE COMBUSTIVEIS!!');&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  SALDOTROCO  := 0;&#13;&#10;  TOTAL       := 0;&#13;&#10;  RECEBIMENTO := 0;&#13;&#10;  PAGAMENTO   := 0;&#13;&#10;  TOTAL       := 0;&#13;&#10;  FCHEQUEPRE  := 0;&#13;&#10;  Recebimentos:= 0;&#13;&#10;&#13;&#10;  PRECOATUAL    := False;&#13;&#10;  PAGE1.VISIBLE := True;&#13;&#10;  PAGE2.VISIBLE := True;&#13;&#10;&#13;&#10;//  If (&#60;TEM_COMBUST.&#34;ACHOU&#34;&#62; = 'TRUE') Then&#13;&#10;//     BOMBAS_MAPA.OPEN;&#13;&#10;&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('SELECT M.CODIGOLANCAMENTO, O.HISTORICO AS DESCRICAO, M.TIPOMOVIMENTO, M.STATUS,');&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('V.DESCRICAO AS DESC_VARIAVEL, SUM(M.VALOR) AS VALOR');&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('FROM MOVIMENTOCONTABIL M');&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('INNER JOIN OPERACAOAUTOMATICO O ON O.CODIGOCAIXA = M.CODIGOLANCAMENTO');&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('INNER JOIN VARIAVEL V ON V.CODIGO = O.CODIGOVARIAVEL');&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('WHERE (M.CODIGOCAIXAABERTURA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')) AND M.CNPJ = :CNPJ');&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('GROUP BY M.CODIGOLANCAMENTO, O.HISTORICO, M.TIPOMOVIMENTO, M.STATUS, V.DESCRICAO');&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('ORDER BY M.CODIGOLANCAMENTO, O.HISTORICO, M.TIPOMOVIMENTO, M.STATUS, V.DESCRICAO');&#13;&#10;  MOVIMENTO_MAPA.PARAMBYNAME('CNPJ').VALUE := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  CAIXAABERTURA.SQL.ADD('SELECT MIN(C.DATAABERTURA) AS DATAABERTURA, MAX(C.DATAFECHAMENTO) AS DATAFECHAMENTO, SUM(C.TROCOINICIAL) AS TROCOINICIAL, SUM(C.TROCOFINAL) AS TROCOFINAL,');&#13;&#10;  CAIXAABERTURA.SQL.ADD('SUM(C.DINHEIRO) AS DINHEIRO, SUM(C.CHEQUEAVISTA) AS CHEQUEAVISTA, SUM(C.CHEQUEPRAZO) CHEQUEPRAZO,');&#13;&#10;  CAIXAABERTURA.SQL.ADD('SUM(C.CARTAO) AS CARTAO, SUM(C.VALES) AS VALES, SUM(C.OUTROS) AS OUTROS, SUM(C.CONTRAVALE) AS CONTRAVALE,');&#13;&#10;  CAIXAABERTURA.SQL.ADD('SUM(C.VENDAPRAZO) AS VENDAPRAZO, SUM(C.REPOSICAO) AS REPOSICAO, SUM(C.RETIRADA) AS RETIRADA,');&#13;&#10;  CAIXAABERTURA.SQL.ADD('SUM(C.CONTACORRENTE) AS CONTACORRENTE');&#13;&#10;  CAIXAABERTURA.SQL.ADD('FROM CAIXAABERTURA C');&#13;&#10;  CAIXAABERTURA.SQL.ADD('WHERE (C.CODIGO IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + '))');&#13;&#10;  CAIXAABERTURA.SQL.ADD('AND C.CNPJ = :CNPJ');&#13;&#10;  CAIXAABERTURA.PARAMBYNAME('CNPJ').VALUE := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  CAIXAABERTURA.OPEN;&#13;&#10;&#13;&#10;  DATAI := STRTODATE('01/' + INTTOSTR(MONTHOF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)) + '/' + INTTOSTR(YEAROF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)));&#13;&#10;  IF (MONTHOF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;) = 12) THEN&#13;&#10;    DATAF := STRTODATE('31/12/'+INTTOSTR(YEAROF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)))&#13;&#10;  ELSE&#13;&#10;    DATAF := STRTODATE('01/'+ INTTOSTR(MONTHOF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)+1) + '/' + INTTOSTR(YEAROF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)));&#13;&#10;&#13;&#10;  MOVIMENTONF_MAPA.SQL.ADD('SELECT * FROM FECHAMENTO_DIA_ACUMULADO(:CODIGOCAIXA, :CNPJ, :DATAI - 1, :DATAF)');&#13;&#10;  MOVIMENTONF_MAPA.SQL.ADD('ORDER BY DESCRICAO');&#13;&#10;  MOVIMENTONF_MAPA.PARAMBYNAME('CODIGOCAIXA').VALUE    := SIGHAPARAM('CODIGOCAIXAABERTURA');&#13;&#10;  MOVIMENTONF_MAPA.PARAMBYNAME('DATAI').VALUE          := DATAI;&#13;&#10;  MOVIMENTONF_MAPA.PARAMBYNAME('DATAF').VALUE          := DATAF;&#13;&#10;  MOVIMENTONF_MAPA.PARAMBYNAME('CNPJ').VALUE           := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  BOMBAS_MAPA.SQL.ADD('SELECT * FROM FECHAMENTO_BOMBA_ACUMULADO(:CODIGOCAIXA, :CNPJ)');&#13;&#10;  BOMBAS_MAPA.SQL.ADD('ORDER BY CODIGO, DESCRICAO');&#13;&#10;  BOMBAS_MAPA.PARAMBYNAME('CODIGOCAIXA').VALUE := SIGHAPARAM('CODIGOCAIXAABERTURA');&#13;&#10;  BOMBAS_MAPA.PARAMBYNAME('CNPJ').VALUE        := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  PRECOCOMB.SQL.ADD('SELECT PC.CODIGOCOMB, AVG(PC.PRECOPRAZO) AS PRECOPRAZO');&#13;&#10;  PRECOCOMB.SQL.ADD('FROM PRECOCOMB PC, COMBUSTIVEL C, BOMBA B');&#13;&#10;  PRECOCOMB.SQL.ADD('WHERE B.CODIGO = :BOMBA');&#13;&#10;  PRECOCOMB.SQL.ADD('AND C.CODIGO = B.CODIGOCOMBUSTIVEL');&#13;&#10;  PRECOCOMB.SQL.ADD('AND PC.CODIGOCOMB = C.CODIGO');&#13;&#10;  PRECOCOMB.SQL.ADD('AND PC.CODIGOCAIXA in '+'('+SIGHAPARAM('CODIGOCAIXAABERTURA')+')'+);&#13;&#10;  PRECOCOMB.SQL.ADD('AND PC.CNPJ = :CNPJ');&#13;&#10;  PRECOCOMB.SQL.ADD('GROUP BY 1');&#13;&#10;  PRECOCOMB.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  MOVIMENTO.SQL.ADD('SELECT M.TIPOMOVIMENTO AS TIPO, (CASE WHEN (M.TIPOMOVIMENTO = ''D'') THEN M.HISTORICO ELSE O.HISTORICO END) AS HISTORICO,');&#13;&#10;  MOVIMENTO.SQL.ADD('M.STATUS, SUM(M.VALOR) AS VALOR');&#13;&#10;  MOVIMENTO.SQL.ADD('FROM MOVIMENTOCONTABIL M, OPERACAOAUTOMATICO O, OPERACAO OP');&#13;&#10;  MOVIMENTO.SQL.ADD('WHERE (M.CODIGOCAIXAABERTURA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + '))');&#13;&#10;  MOVIMENTO.SQL.ADD('AND (M.CODIGOLANCAMENTO = O.CODIGOCAIXA)');&#13;&#10;  MOVIMENTO.SQL.ADD('AND (O.CODIGOLANCAMENTO = OP.CODIGOLANCAMENTO)');&#13;&#10;//  MOVIMENTO.SQL.ADD('AND (OP.OPERACAO &#60;&#62; 1)');&#13;&#10;  MOVIMENTO.SQL.ADD('AND M.CNPJ = :CNPJ');&#13;&#10;  MOVIMENTO.SQL.ADD('GROUP BY 1, 2, 3');&#13;&#10;  MOVIMENTO.SQL.ADD('ORDER BY 1, 2, 3');&#13;&#10;  MOVIMENTO.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  MOVIMENTONF.SQL.ADD('SELECT O.CODIGOLANCAMENTO, O.DESCRICAO, SUM(BC.VALOR)AS TOTALNF,  COUNT(*) AS QTDA');&#13;&#10;  MOVIMENTONF.SQL.ADD('FROM OPERACAO O, BAR_CONTAPARCIAL BC');&#13;&#10;  MOVIMENTONF.SQL.ADD('WHERE BC.OPERACAO = O.CODIGOLANCAMENTO');&#13;&#10;  MOVIMENTONF.SQL.ADD('AND BC.CAIXAABERTURA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;//  MOVIMENTONF.SQL.ADD('AND NF.TIPOMOVIMENTO = ''S''');&#13;&#10;//  MOVIMENTONF.SQL.ADD('AND NF.NOTAFISCALCANCELADA = 0');&#13;&#10;  MOVIMENTONF.SQL.ADD('AND BC.CNPJ = :CNPJ');&#13;&#10;  MOVIMENTONF.SQL.ADD('GROUP BY O.CODIGOLANCAMENTO, O.DESCRICAO');&#13;&#10;  MOVIMENTONF.SQL.ADD('ORDER BY O.DESCRICAO');&#13;&#10;  MOVIMENTONF.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  CHEQUECADASTRADO.SQL.ADD('SELECT SUM(MC.VALOR) AS VALORCHEQUE');&#13;&#10;  CHEQUECADASTRADO.SQL.ADD('FROM ((MOVIMENTOCONTABIL MC');&#13;&#10;  CHEQUECADASTRADO.SQL.ADD('INNER JOIN OPERACAOAUTOMATICO O ON MC.CODIGOLANCAMENTO = O.CODIGOCAIXA)');&#13;&#10;  CHEQUECADASTRADO.SQL.ADD('INNER JOIN VARIAVEL V ON O.CODIGOVARIAVEL = V.CODIGO)');&#13;&#10;  CHEQUECADASTRADO.SQL.ADD('WHERE UPPER(V.DESCRICAO) = ''CADASTROCHEQUEPRE''');&#13;&#10;  CHEQUECADASTRADO.SQL.ADD('AND MC.CODIGOCAIXAABERTURA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  CHEQUECADASTRADO.SQL.ADD('AND MC.CNPJ = :CNPJ');&#13;&#10;  CHEQUECADASTRADO.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  TEM_COMBUST.SQL.ADD('SELECT DISTINCT ''TRUE'' AS ACHOU FROM NOTAFISCAL');&#13;&#10;  TEM_COMBUST.SQL.ADD('WHERE CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ') AND VALORCOMBUSTIVEL &#62; 0');&#13;&#10;  TEM_COMBUST.SQL.ADD('AND CNPJ = :CNPJ');&#13;&#10;  TEM_COMBUST.SQL.ADD('UNION');&#13;&#10;  TEM_COMBUST.SQL.ADD('SELECT DISTINCT ''TRUE'' AS ACHOU FROM ENCERRANTE');&#13;&#10;  TEM_COMBUST.SQL.ADD('WHERE CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ') AND');&#13;&#10;  TEM_COMBUST.SQL.ADD('CNPJ = :CNPJ');&#13;&#10;  TEM_COMBUST.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  CHEQUE.SQL.ADD('SELECT * FROM CHEQUE');&#13;&#10;  CHEQUE.SQL.ADD('WHERE USUARIO IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;&#13;&#10;  NFGERAL.SQL.ADD('SELECT O.DESCRICAO, R.NOME, NF.*');&#13;&#10;  NFGERAL.SQL.ADD('FROM OPERACAO O, NOTAFISCAL NF, RELACAO R');&#13;&#10;  NFGERAL.SQL.ADD('WHERE NF.CODIGOOPERACAO = O.CODIGOLANCAMENTO');&#13;&#10;  NFGERAL.SQL.ADD('AND NF.CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  NFGERAL.SQL.ADD('AND NF.TIPOMOVIMENTO = ''S''');&#13;&#10;  NFGERAL.SQL.ADD('AND NF.NOTAFISCALCANCELADA = 0');&#13;&#10;  NFGERAL.SQL.ADD('AND R.CODIGORELACAO = NF.CODIGORELACAO');&#13;&#10;  NFGERAL.SQL.ADD('AND NF.CNPJ = :CNPJ');&#13;&#10;  NFGERAL.SQL.ADD('ORDER BY O.DESCRICAO');&#13;&#10;  NFGERAL.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  NOTAFISCALCR.SQL.ADD('SELECT NF.VALORTOTALNF AS TOTALNF, C.NOME, CR.*');&#13;&#10;  NOTAFISCALCR.SQL.ADD('FROM NOTAFISCAL NF, CONTARECEBER CR, CLIENTE C, OPERACAO O');&#13;&#10;  NOTAFISCALCR.SQL.ADD('WHERE NF.CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND NF.CODIGOOPERACAO = O.CODIGOLANCAMENTO');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND O.TIPOOPERACAO IN (2,5)');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND NF.TIPOMOVIMENTO = ''S''');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND NF.NOTAFISCALCANCELADA = 0');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND NF.CNPJ = :CNPJ');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND CR.CODIGONF = NF.CODIGONF');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND CR.CODIGOCLIENTE = C.CODIGOCLIENTE');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND CAST(CR.DATAEMISSAO AS DATE) = CAST(NF.DATAEMISSAO AS DATE)');&#13;&#10;  NOTAFISCALCR.SQL.ADD('ORDER BY CR.NUMERODOCUMENTO');&#13;&#10;  NOTAFISCALCR.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  NOTAFISCALCC.SQL.ADD('SELECT NF.VALORTOTALNF AS TOTALNF, R.NOME, CC.*');&#13;&#10;  NOTAFISCALCC.SQL.ADD('FROM NOTAFISCAL NF, CONTACORRENTE CC, RELACAO R');&#13;&#10;  NOTAFISCALCC.SQL.ADD('WHERE NF.CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  NOTAFISCALCC.SQL.ADD('AND NF.TIPOMOVIMENTO = ''S''');&#13;&#10;  NOTAFISCALCC.SQL.ADD('AND NF.NOTAFISCALCANCELADA = 0');&#13;&#10;  NOTAFISCALCC.SQL.ADD('AND NF.CNPJ = :CNPJ');&#13;&#10;  NOTAFISCALCC.SQL.ADD('AND CC.NUMERODOCUMENTO = NF.DOCUMENTO');&#13;&#10;  NOTAFISCALCC.SQL.ADD('AND CC.CODIGORELACAO = R.CODIGORELACAO');&#13;&#10;  NOTAFISCALCC.SQL.ADD('AND CC.DATAEMISSAO = NF.DATAEMISSAO');&#13;&#10;  NOTAFISCALCC.SQL.ADD('ORDER BY NUMERODOCUMENTO');&#13;&#10;  NOTAFISCALCC.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('SELECT BC.CODIGOPRODUTO, PT.NOME, BC.VALORUNITARIO,');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('SUM(BC.QUANTIDADE) AS QTDE, SUM(BC.VALORTOTAL) AS TOTAL');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('FROM BAR_CONTA BC, PRODUTO PT');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('WHERE BC.CAIXAABERTURA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('AND BC.CODIGOPRODUTO = PT.CODIGO');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('AND BC.CNPJ = :CNPJ');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('GROUP BY 2, 1, 3');&#13;&#10;  NOTAFISCALPRODUTO.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  CRRECEBIDAS.SQL.ADD('SELECT CRP.NUMERODUPLICATA, CRP.NUMEROPARCELA, CRP.VALORPAGO,');&#13;&#10;  CRRECEBIDAS.SQL.ADD('CR.NUMERODOCUMENTO, CR.CODIGOCLIENTE, CL.NOME');&#13;&#10;  CRRECEBIDAS.SQL.ADD('FROM CONTARECEBERPARCIAL CRP, CONTARECEBER CR, CLIENTE CL');&#13;&#10;  CRRECEBIDAS.SQL.ADD('WHERE CRP.CODIGOCAIXAABERTURA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  CRRECEBIDAS.SQL.ADD('AND CRP.NUMERODUPLICATA = CR.NUMERODUPLICATA');&#13;&#10;  CRRECEBIDAS.SQL.ADD('AND CRP.NUMEROPARCELA = CR.NUMEROPARCELA');&#13;&#10;  CRRECEBIDAS.SQL.ADD('AND CRP.STATUS = CR.STATUS');&#13;&#10;  CRRECEBIDAS.SQL.ADD('AND CRP.CNPJ = CR.CNPJ AND CR.CNPJ = :CNPJ');&#13;&#10;  CRRECEBIDAS.SQL.ADD('AND CR.CODIGOCLIENTE = CL.CODIGOCLIENTE');&#13;&#10;  CRRECEBIDAS.SQL.ADD('AND CRP.VALORPAGO &#62; 0');&#13;&#10;  CRRECEBIDAS.SQL.ADD('ORDER BY CR.NUMERODOCUMENTO');&#13;&#10;  CRRECEBIDAS.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('SELECT O.CODIGOLANCAMENTO, O.DESCRICAO, NF.STATUS, SUM(NFM.VALOR) AS TOTALNF, COUNT(*) AS QTDA');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('FROM OPERACAO O, NOTAFISCAL NF, NOTAFISCALMULTOPERACAO NFM');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('WHERE NFM.CODIGOLANCAMENTO = O.CODIGOLANCAMENTO');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('AND NFM.CODIGONF = NF.CODIGONF');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('AND NF.CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('AND NF.TIPOMOVIMENTO = ''S''');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('AND NF.NOTAFISCALCANCELADA = 0');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('AND NF.CNPJ = :CNPJ');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('GROUP BY O.CODIGOLANCAMENTO, O.DESCRICAO, NF.STATUS');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('ORDER BY O.DESCRICAO');&#13;&#10;  MOVIMENTONFMFP.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  OBSRETIRADA.SQL.ADD('SELECT C.OBSERVACAO');&#13;&#10;  OBSRETIRADA.SQL.ADD('FROM CAIXAABERTURA C');&#13;&#10;  OBSRETIRADA.SQL.ADD('WHERE (C.CODIGO IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + '))');&#13;&#10;  OBSRETIRADA.SQL.ADD('AND C.CNPJ = :CNPJ');&#13;&#10;  OBSRETIRADA.PARAMBYNAME('CNPJ').VALUE := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  FUNCIONARIO.SQL.ADD('SELECT CODIGO, APELIDO');&#13;&#10;  FUNCIONARIO.SQL.ADD('FROM FUNCIONARIO');&#13;&#10;  FUNCIONARIO.SQL.ADD('WHERE CODIGO IN (SELECT CODIGOFUNCIONARIO');&#13;&#10;  FUNCIONARIO.SQL.ADD('                 FROM CAIXAABERTURA C');&#13;&#10;  FUNCIONARIO.SQL.ADD('                 WHERE (CODIGO IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')))');&#13;&#10;  FUNCIONARIO.OPEN;&#13;&#10;&#13;&#10;  WHILE NOT FUNCIONARIO.EOF DO BEGIN&#13;&#10;    ReportSummary2.VISIBLE := (FUNCIONARIO.RECORDCOUNT = 1);&#13;&#10;    FUNCIONARIO.NEXT;&#13;&#10;  END;&#13;&#10;end." PropData="">
