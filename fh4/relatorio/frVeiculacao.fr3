    <TfrxIBXQuery Name="RELATORIO" UserName="RELATORIO" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT JD.DATA, JP.CODIGOCLIENTE, JD.CODIGOCONTRATO, JE.EDICAO, JD.TAMANHO, JPG.PREFIXO, JC.DESCRISCAO, JP.TIPOPUBLICIDADE, F.APELIDO, JD.OBSERVACOES, CL.NOME, P.NOME AS NOMEPRODUTO&#13;&#10;FROM CLIENTE CL&#13;&#10;   INNER JOIN JPUBLICIDADE JP ON (CL.CODIGOCLIENTE = JP.CODIGOCLIENTE)&#13;&#10;   INNER JOIN JDATAS JD ON (JP.CODIGO = JD.CODIGOCONTRATO)&#13;&#10;   INNER JOIN JCOR JC ON (JD.COR = JC.CODIGO)&#13;&#10;   INNER JOIN JEDICOES JE ON (JD.CODIGOEDICAO = JE.CODIGO)&#13;&#10;   INNER JOIN PRODUTO P ON (P.CODIGO = JE.CODIGOPRODUTO)&#13;&#10;   INNER JOIN JPAGINA JPG ON (JD.PAGINA = JPG.CODIGO)&#13;&#10;   INNER JOIN FUNCIONARIO F ON (JP.CODIGOVENDEDOR = F.CODIGO)&#13;&#10;&#13;&#10;WHERE JD.DATA BETWEEN :DATAI AND :DATAF&#13;&#10;" PropData=""/>
