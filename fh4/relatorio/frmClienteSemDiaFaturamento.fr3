    <TfrxIBXQuery Name="CLIENTE" UserName="CLIENTE" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT DISTINCT C.CODIGOCLIENTE, C.NOME, r.codigorelacao, r.codigo FROM CLIENTE C LEFT JOIN&#13;&#10;RELACAO R ON C.codigocliente = R.codigo LEFT JOIN&#13;&#10;CLIENTEDIAFATURAMENTO CDF ON R.codigorelacao = CDF.codigocliente&#13;&#10;WHERE R.TIPOPESSOA = 'C'&#13;&#10;AND C.CODIGOCLIENTE NOT IN&#13;&#10;(SELECT R.CODIGO&#13;&#10;FROM CLIENTEDIAFATURAMENTO CDF, RELACAO R&#13;&#10;WHERE CDF.CODIGOCLIENTE = R.CODIGORELACAO)" PropData="0A506172616D65746572730100"/>
