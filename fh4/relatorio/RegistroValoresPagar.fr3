    <TfrxIBXQuery Name="QRConsulta" UserName="QRConsulta" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT CAST(CP.DATAEMISSAO AS DATE) AS DATAEMISSAO,&#13;&#10;CP.NUMERODUPLICATA, CP.VALORDOCUMENTO,&#13;&#10;&#13;&#10;(SELECT MAX(CPP.NUMEROPARCELA)&#13;&#10;FROM CONTAPAGAR CPP&#13;&#10;WHERE CP.NUMERODOCUMENTO = CPP.NUMERODOCUMENTO&#13;&#10;AND CP.STATUS = CPP.STATUS&#13;&#10;AND CP.CNPJ = CPP.CNPJ) AS QTDPARC,&#13;&#10;&#13;&#10;CP.NUMEROPARCELA, CP.VALORDUPLICATA, CP.DATAVENCIMENTO,&#13;&#10;&#13;&#10;(SELECT MAX(CPP.DATAPAGAMENTO)&#13;&#10; FROM CONTAPAGARPARCIAL CPP&#13;&#10; WHERE CP.NUMERODUPLICATA = CPP.NUMERODUPLICATA&#13;&#10; AND CP.NUMEROPARCELA = CPP.NUMEROPARCELA&#13;&#10; AND CP.STATUS = CPP.STATUS&#13;&#10; AND CP.CNPJ = CPP.CNPJ) AS DATAPAGAMENTO,&#13;&#10;&#13;&#10;(SELECT SUM(CPP.VALORPAGO)&#13;&#10;FROM CONTAPAGARPARCIAL CPP&#13;&#10;WHERE CP.NUMERODUPLICATA = CPP.NUMERODUPLICATA&#13;&#10;AND CP.NUMEROPARCELA = CPP.NUMEROPARCELA&#13;&#10;AND CP.STATUS = CPP.STATUS&#13;&#10;AND CP.CNPJ = CPP.CNPJ) AS VALORPAGO,&#13;&#10;&#13;&#10;CP.VALORSALDO&#13;&#10;&#13;&#10;FROM CONTAPAGAR CP&#13;&#10;INNER JOIN FORNECEDOR F ON CP.CODIGOFORNECEDOR = F.CODIGO&#13;&#10;&#13;&#10;WHERE CP.DATAEMISSAO &#60;= :DATA&#13;&#10;AND ((CP.STATUS = '''') OR (CP.STATUS = :STATUS))&#13;&#10;AND (CP.CNPJ = :CNPJ)&#13;&#10;&#13;&#10;ORDER BY DATAEMISSAO, NUMERODUPLICATA ASC" PropData=""/>
