<TfrxReport Version="3.24" DotMatrixReport="False" EngineOptions.UseFileCache="True" IniFile="\Software\Fast Reports" PreviewOptions.Buttons="4031" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PrintOptions.Printer="Default" ReportOptions.CreateDate="39182,6016030093" ReportOptions.Description.Text="" ReportOptions.Name="0" ReportOptions.LastChange="39960,4694348495" ScriptLanguage="PascalScript" ScriptText.Text="begin&#13;&#10;       QRTRANSFERENCIA.CLOSE;&#13;&#10;       QRTRANSFERENCIA.SQL.TEXT := 'SELECT T.CODIGOBEMPATRIMONIAL, B.DESCRICAO AS DESCRICAOBEM, T.CODIGOLOCALANTIGO, L1.DESCRICAO AS DESCRICAOLOCALANTIGO, ' +&#13;&#10;                                   'T.CODIGOLOCALNOVO, L2.DESCRICAO AS DESCRICAOLOCALNOVO, T.DATA, T.MOTIVO ' +&#13;&#10;                                   'FROM TRANSFERENCIABENSPATRIMONIAIS T ' +&#13;&#10;                                   'INNER JOIN BENSPATRIMONIAIS B     ON T.CODIGOBEMPATRIMONIAL = B.CODIGO ' +&#13;&#10;                                   'INNER JOIN LOCALBEMPATRIMONIAL L1 ON T.CODIGOLOCALANTIGO = L1.CODIGO ' +&#13;&#10;                                   'INNER JOIN LOCALBEMPATRIMONIAL L2 ON T.CODIGOLOCALNOVO = L2.CODIGO ' +&#13;&#10;                                   'WHERE T.DATA BETWEEN :DATAI AND :DATAF AND (B.STATUS = ''#'') OR (B.STATUS = :STATUSOUT)';&#13;&#10;&#13;&#10;       QRTRANSFERENCIA.PARAMBYNAME('DATAI').VALUE := STRTODATE(SIGHAPARAM('DATAI'));&#13;&#10;       QRTRANSFERENCIA.PARAMBYNAME('DATAF').VALUE := STRTODATE(SIGHAPARAM('DATAF'));&#13;&#10;&#13;&#10;       IF SIGHAPARAM('BEMI') &#60;&#62; '0' THEN BEGIN&#13;&#10;               QRTRANSFERENCIA.SQL.ADD('AND T.CODIGOBEMPATRIMONIAL BETWEEN :BEMI AND :BEMF');&#13;&#10;               QRTRANSFERENCIA.PARAMBYNAME('BEMI').VALUE := SIGHAPARAM('BEMI');&#13;&#10;               QRTRANSFERENCIA.PARAMBYNAME('BEMF').VALUE := SIGHAPARAM('BEMF');&#13;&#10;       END;&#13;&#10;       IF SIGHAPARAM('LOCALANTIGO') &#60;&#62; '0' THEN BEGIN&#13;&#10;               QRTRANSFERENCIA.SQL.ADD('AND T.CODIGOLOCALANTIGO = :LOCALANTIGO');&#13;&#10;               QRTRANSFERENCIA.PARAMBYNAME('LOCALANTIGO').VALUE := SIGHAPARAM('LOCALANTIGO');&#13;&#10;       END;&#13;&#10;       IF SIGHAPARAM('LOCALNOVO') &#60;&#62; '0' THEN BEGIN&#13;&#10;               QRTRANSFERENCIA.SQL.ADD('AND T.CODIGOLOCALNOVO = :LOCALNOVO');&#13;&#10;               QRTRANSFERENCIA.PARAMBYNAME('LOCALNOVO').VALUE := SIGHAPARAM('LOCALNOVO');&#13;&#10;       END;&#13;&#10;&#13;&#10;       {IF (SIGHAPARAM('STATUSOUT')= '*') AND (SIGHAPARAM('STATUSOUT')= '#') THEN&#13;&#10;               QRTRANSFERENCIA.SQL.ADD('AND B.STATUS = :STATUSOUT');  }&#13;&#10;&#13;&#10;&#13;&#10;       QRTRANSFERENCIA.OPEN;&#13;&#10;end." PropData="">
