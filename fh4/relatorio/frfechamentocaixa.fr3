    <TfrxIBXQuery Name="VENDAPRODUTOS" UserName="VENDAPRODUTOS" CloseDataSource="True" FieldAliases.Text="" SQL.Text="  SELECT  L.DESCRICAO AS LINHA, SUM (NFP.VALORUNITARIO) AS VALORUNITARIO,&#13;&#10;  SUM (NFP.QUANTIDADE) AS QTDE, SUM(NFP.VALORTOTAL) AS TOTAL, SUM (NFP.VALORDESCONTO) AS DESCONTO&#13;&#10;  FROM NOTAFISCAL NF, NOTAFISCALPRODUTO NFP, PRODUTO P, LINHA L&#13;&#10;  WHERE NF.CODIGOCAIXA = :CAIXAABERTURA&#13;&#10;  AND NF.TIPOMOVIMENTO = 'S'&#13;&#10;  AND NF.NOTAFISCALCANCELADA = 0&#13;&#10;  AND NOT (NF.TIPODOCUMENTO = 'N' AND NF.MODELO = '01' AND NF.OBSERVACAO IS NOT NULL AND NF.OBSERVACAO LIKE '%NF Ref. Cupom Fiscal%')&#13;&#10;  AND NFP.CANCELADOITEM = 0&#13;&#10;  AND NF.CODIGONF = NFP.CODIGONF&#13;&#10;  AND NFP.CODIGOPRODUTO = P.CODIGO&#13;&#10;  AND P.LINHA = L.CODIGO&#13;&#10;  AND NF.DOCUMENTO NOT IN (SELECT NFDEVOLUCAO&#13;&#10;                           FROM NOTAFISCAL&#13;&#10;                           WHERE CODIGOCAIXA = :CAIXAABERTURA&#13;&#10;                           AND TIPOMOVIMENTO = 'ED')&#13;&#10;  GROUP BY 1" PropData=""/>
