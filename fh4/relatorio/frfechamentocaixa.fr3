          4   7   0ÝA                                             sJsJL     " ReportOptions.VersionMinor="èˆ" ReportOptions.VersionRelease="üÿ’˜Çe4" ScriptLanguage="PascalScript" ScriptText.Text="Var    TOTAL,&#13;&#10;       RECEBIMENTOS, PAGAMENTOS,  EXTRA,&#13;&#10;       VENDAPRODUTO,&#13;&#10;       TOTALAFERICAO, TOTALQTDEBOMBA, TOTALVENDABOMBA, TOTALVENDA,&#13;&#10;       TOTALQTDEVENDA, TOTALDESCONTO, TOTALDIFERENCA, TOTALDIFERENCALITRO,&#13;&#10;       TOTALCHEQUE, VENDAOPERACAO, TOTALPRODUTO, SALDOCAIXA,&#13;&#10;       TOTALPRODUTODESC, TOTALRECEBIDO, SALDOTROCO,TOTALGRUPO,&#13;&#10;       RECEBIMENTO,TOTALPRAZO, PAGAMENTO,TOTALCC, VALORFORMA, TOTALFISICO,&#13;&#10;       FCHEQUEPRE, DIFERENCACC, VALORDIFERENCA, VALORDIFERENCAN_FORM, CONTA :REAL;&#13;&#10;       TIPOCH, TIPOMOV, DIFERENCA, PRECO : STRING;&#13;&#10;       DATAI, DATAF : STRING;&#13;&#10;       PRECOATUAL : BOOLEAN;&#13;&#10;&#13;&#10;       // PAGE 11 E SUBREPORT 8 E 9&#13;&#10;       TOTALVENDAGNV, TOTALQTDEVENDAGNV: REAL;  // controle de venda de gnv&#13;&#10;       TOTALPRODUTOS, TOTALPRODUTO2 : REAL;&#13;&#10;       PRECOGNV : STRING;&#13;&#10;&#13;&#10;       COMB_DIN,  COMB_CART,  COMB_CHEQUE,  COMB_NOTAS,  COMB_CC,  COMB_TOTAL,&#13;&#10;        GNV_DIN,   GNV_CART,   GNV_CHEQUE,   GNV_NOTAS,   GNV_CC,   GNV_TOTAL,&#13;&#10;       PROD_DIN,  PROD_CART,  PROD_CHEQUE,  PROD_NOTAS,  PROD_CC,  PROD_TOTAL,&#13;&#10;       TOTAL_DIN, TOTAL_CART, TOTAL_CHEQUE, TOTAL_NOTAS, TOTAL_CC, TOTALGERALCAIXA : REAL;&#13;&#10;&#13;&#10;procedure MasterHeader3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALCHEQUE := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure GroupHeader1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     IF (&#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'D') THEN&#13;&#10;       TIPOMOV       := 'Pagamento'&#13;&#10;     ELSE IF (&#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'C') THEN&#13;&#10;       TIPOMOV       := 'Recebimento'&#13;&#10;     ELSE&#13;&#10;       TIPOMOV       := 'Extra - Caixa';&#13;&#10;     TOTALGRUPO      := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Band2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF &#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'D' THEN BEGIN&#13;&#10;    TOTAL      := TOTAL       - &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;    TOTALGRUPO := TOTALGRUPO  + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;    PAGAMENTO  := PAGAMENTO   + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;  END&#13;&#10;  ELSE IF &#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'C' THEN BEGIN&#13;&#10;    TOTAL      := TOTAL       + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;    TOTALGRUPO := TOTALGRUPO  + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;    RECEBIMENTO:= RECEBIMENTO + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;  END&#13;&#10;  ELSE IF &#60;MOVIMENTO.&#34;TIPO&#34;&#62; = 'E' THEN BEGIN&#13;&#10;    TOTAL      := TOTAL       + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;    TOTALGRUPO := TOTALGRUPO  + &#60;MOVIMENTO.&#34;VALOR&#34;&#62;;&#13;&#10;  END;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterHeader6OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALRECEBIDO := 0;&#13;&#10;     SALDOTROCO    := ((&#60;CAIXAABERTURA.&#34;TROCOINICIAL&#34;&#62; - &#60;CAIXAABERTURA.&#34;TROCOFINAL&#34;&#62;) + (&#60;CAIXAABERTURA.&#34;REPOSICAO&#34;&#62; - &#60;CAIXAABERTURA.&#34;RETIRADA&#34;&#62;));&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData4OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALRECEBIDO := TOTALRECEBIDO + &#60;CRRECEBIDAS.&#34;VALORPAGO&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData5OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TOTALAFERICAO       := TOTALAFERICAO + &#60;BOMBAS_MAPA.&#34;AFERICAO&#34;&#62;;&#13;&#10;  TOTALQTDEBOMBA      := TOTALQTDEBOMBA + (&#60;BOMBAS_MAPA.&#34;L_FINAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;L_INICIAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;AFERICAO&#34;&#62;);&#13;&#10;  TOTALVENDABOMBA     := TOTALVENDABOMBA + ((&#60;BOMBAS_MAPA.&#34;L_FINAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;L_INICIAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;AFERICAO&#34;&#62;) * &#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;);&#13;&#10;  TOTALVENDA          := TOTALVENDA + (&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;) ;&#13;&#10;  TOTALQTDEVENDA      := TOTALQTDEVENDA + &#60;BOMBAS_MAPA.&#34;VENDA&#34;&#62;;&#13;&#10;  TOTALDESCONTO       := TOTALDESCONTO + ((&#60;BOMBAS_MAPA.&#34;VENDA&#34;&#62;*&#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;) -(&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;));&#13;&#10;  TOTALDIFERENCALITRO := TOTALDIFERENCALITRO + ((&#60;BOMBAS_MAPA.&#34;L_FINAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;L_INICIAL_MANUAL&#34;&#62;) - &#60;BOMBAS_MAPA.&#34;VENDA&#34;&#62;);&#13;&#10;  //TOTALDIFERENCA    := TOTALDIFERENCA + ((((&#60;BOMBAS_MAPA.&#34;L_FINAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;L_INICIAL_MANUAL&#34;&#62;) * &#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;) - (&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;)+((&#60;BOMBAS_MAPA.&#34;VENDA&#34;&#62;*&#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;) -(&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;))));&#13;&#10;  TOTALDIFERENCA      := TOTALDIFERENCA +     ((&#60;BOMBAS_MAPA.&#34;L_FINAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;L_INICIAL_MANUAL&#34;&#62; - &#60;BOMBAS_MAPA.&#34;AFERICAO&#34;&#62;) * &#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;) - (&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;+((&#60;BOMBAS_MAPA.&#34;VENDA&#34;&#62;*&#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;) -(&#60;BOMBAS_MAPA.&#34;VALORTOTALVENDA&#34;&#62;-&#60;BOMBAS_MAPA.&#34;VALORDESCONTOVENDA&#34;&#62;)) );&#13;&#10;&#13;&#10;  PRECOCOMB.CLOSE;&#13;&#10;  PRECOCOMB.PARAMBYNAME('BOMBA').VALUE := (&#60;BOMBAS_MAPA.&#34;CODIGO&#34;&#62;);&#13;&#10;  PRECOCOMB.OPEN;&#13;&#10;&#13;&#10;  PRECOBOMBA.CLOSE;&#13;&#10;  PRECOBOMBA.PARAMBYNAME('BOMBA').VALUE := (&#60;BOMBAS_MAPA.&#34;CODIGO&#34;&#62;);&#13;&#10;  PRECOBOMBA.OPEN;&#13;&#10;&#13;&#10;  IF ((&#60;PRECOBOMBA.&#34;PRECOBOMBACAIXA&#34;&#62;) &#62; 0 ) THEN&#13;&#10;    PRECO := FORMATFLOAT('#,###0.000',&#60;PRECOBOMBA.&#34;PRECOBOMBACAIXA&#34;&#62;)&#13;&#10;  ELSE BEGIN&#13;&#10;   // PRECOATUAL := TRUE;&#13;&#10;    PRECO := FORMATFLOAT('#,###0.000',(&#60;BOMBAS_MAPA.&#34;PRECOREFERENCIA&#34;&#62;));&#13;&#10;    PRECO := FORMATFLOAT('#,###0.000',(&#60;PRECOCOMB.&#34;PRECOPRAZO&#34;&#62;));&#13;&#10;  END;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALAFERICAO       := 0;&#13;&#10;     TOTALQTDEBOMBA      := 0;&#13;&#10;     TOTALVENDABOMBA     := 0;&#13;&#10;     TOTALVENDA          := 0;&#13;&#10;     TOTALQTDEVENDA      := 0;&#13;&#10;     TOTALDESCONTO       := 0;&#13;&#10;     TOTALDIFERENCA      := 0;&#13;&#10;     TOTALDIFERENCALITRO := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     VENDAPRODUTO := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData6OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     if (&#60;MOVIMENTONF_MAPA.&#34;DESCRICAO&#34;&#62; = 'PRODUTOS') or (&#60;MOVIMENTONF_MAPA.&#34;DESCRICAO&#34;&#62; = 'SERVIÇOS') then begin&#13;&#10;        MasterData6.Visible := True;&#13;&#10;        VENDAPRODUTO := VENDAPRODUTO + &#60;MOVIMENTONF_MAPA.&#34;DIA_VALOR&#34;&#62;;&#13;&#10;     end&#13;&#10;     else&#13;&#10;        MasterData6.Visible := False;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Subreport2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     MOVIMENTONF_MAPA.OPEN;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header3OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     VENDAOPERACAO := 0;&#13;&#10;     MOVIMENTONF.OPEN;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData7OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  VALORFORMA    := 0;&#13;&#10;  DIFERENCACC   := 0;&#13;&#10;  //Comment by Tuba on 29/07/2009 - S&#13;&#10;//  DIFERENCACC   := (VENDAOPERACAO + &#60;MOVIMENTONF.&#34;TOTALNF&#34;&#62;) - (VENDAPRODUTO + TOTALVENDA);&#13;&#10;  MOVIMENTONF.NEXT;&#13;&#10;  IF (NOT MOVIMENTONF.EOF) THEN BEGIN&#13;&#10;     MOVIMENTONF.PRIOR;&#13;&#10;     VALORFORMA := &#60;MOVIMENTONF.&#34;TOTALNF&#34;&#62;;&#13;&#10;  END&#13;&#10;  ELSE&#13;&#10;     VALORFORMA := (&#60;MOVIMENTONF.&#34;TOTALNF&#34;&#62; - DIFERENCACC);&#13;&#10;&#13;&#10;  VENDAOPERACAO := VENDAOPERACAO + VALORFORMA;&#13;&#10;&#13;&#10;//  MasterData19.VISIBLE := (&#60;MOVIMENTONF.&#34;CODIGOLANCAMENTO&#34;&#62; = '1099');&#13;&#10;//  Header8.VISIBLE      := (&#60;MOVIMENTONF.&#34;CODIGOLANCAMENTO&#34;&#62; = '1099');&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header4OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALCHEQUE := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData8OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALCHEQUE := TOTALCHEQUE + &#60;CHEQUE.&#34;VALOR&#34;&#62;;&#13;&#10;     IF &#60;CHEQUE.&#34;TIPOCHEQUE&#34;&#62; = 'P' THEN&#13;&#10;        TIPOCH := 'Pré-datado';&#13;&#10;     IF &#60;CHEQUE.&#34;TIPOCHEQUE&#34;&#62; = 'D' THEN&#13;&#10;        TIPOCH := 'Devolvido';&#13;&#10;     IF &#60;CHEQUE.&#34;TIPOCHEQUE&#34;&#62; = 'T' THEN&#13;&#10;        TIPOCH := 'Troca';&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header5OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALCC    := 0;&#13;&#10;     TOTALPRAZO := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData9OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALCC := TOTALCC + &#60;NOTAFISCALCC.&#34;VALOR&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData10OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALPRAZO := TOTALPRAZO + &#60;NOTAFISCALCR.&#34;VALORDOCUMENTO&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header7OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALPRODUTO     := 0;&#13;&#10;     TOTALPRODUTODESC := 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData11OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;     TOTALPRODUTO     := TOTALPRODUTO + &#60;NOTAFISCALPRODUTO.&#34;TOTAL&#34;&#62;;&#13;&#10;     TOTALPRODUTODESC := TOTALPRODUTODESC + &#60;NOTAFISCALPRODUTO.&#34;DESCONTO&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure btokOnClick(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  IF (RBRESUMIDO.CHECKED) THEN BEGIN&#13;&#10;     MasterData12.Visible  := false;&#13;&#10;     MasterData13.Visible  := false;&#13;&#10;     MasterData14.Visible  := false;&#13;&#10;     MasterData15.Visible  := false;&#13;&#10;     MasterHeader6.Visible := false;&#13;&#10;     MasterData4.Visible   := false;&#13;&#10;     MasterFooter7.Visible := false;&#13;&#10;//     MasterData19.Visible  := false;&#13;&#10;     PAGE11.VISIBLE := FALSE;&#13;&#10;     PAGE12.VISIBLE := FALSE;&#13;&#10;     PAGE13.VISIBLE := FALSE;&#13;&#10;  END;&#13;&#10;&#13;&#10;  IF (RBDETALHADO.CHECKED) THEN BEGIN&#13;&#10;     MasterData12.Visible  := true;&#13;&#10;     MasterData13.Visible  := true;&#13;&#10;     MasterData14.Visible  := true;&#13;&#10;     MasterData15.Visible  := true;&#13;&#10;     MasterHeader6.Visible := true;&#13;&#10;     MasterData4.Visible   := true;&#13;&#10;     MasterFooter7.Visible := true;&#13;&#10;     MasterData19.Visible  := true;&#13;&#10;     PAGE11.VISIBLE := FALSE;&#13;&#10;     PAGE12.VISIBLE := FALSE;&#13;&#10;     PAGE13.VISIBLE := FALSE;&#13;&#10;  END;&#13;&#10;&#13;&#10;  IF (RBRESUMIDO2.CHECKED) THEN BEGIN&#13;&#10;     PAGE1.VISIBLE := false;&#13;&#10;     PAGE2.VISIBLE 