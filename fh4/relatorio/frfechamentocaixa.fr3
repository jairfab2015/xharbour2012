ATUAL    := False;&#13;&#10;  PAGE1.VISIBLE := True;&#13;&#10;  PAGE2.VISIBLE := True;&#13;&#10;  PAGE3.VISIBLE := True;&#13;&#10;&#13;&#10;//  If (&#60;TEM_COMBUST.&#34;ACHOU&#34;&#62; = 'TRUE') Then&#13;&#10;//     BOMBAS_MAPA.OPEN;&#13;&#10;&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('SELECT M.CODIGOLANCAMENTO, O.HISTORICO AS DESCRICAO, M.TIPOMOVIMENTO, M.STATUS,');&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('V.DESCRICAO AS DESC_VARIAVEL, SUM(M.VALOR) AS VALOR');&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('FROM MOVIMENTOCONTABIL M');&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('INNER JOIN OPERACAOAUTOMATICO O ON O.CODIGOCAIXA = M.CODIGOLANCAMENTO');&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('INNER JOIN VARIAVEL V ON V.CODIGO = O.CODIGOVARIAVEL');&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('WHERE (M.CODIGOCAIXAABERTURA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')) AND M.CNPJ = :CNPJ');&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('GROUP BY M.CODIGOLANCAMENTO, O.HISTORICO, M.TIPOMOVIMENTO, M.STATUS, V.DESCRICAO');&#13;&#10;  MOVIMENTO_MAPA.SQL.ADD('ORDER BY M.CODIGOLANCAMENTO, O.HISTORICO, M.TIPOMOVIMENTO, M.STATUS, V.DESCRICAO');&#13;&#10;  MOVIMENTO_MAPA.PARAMBYNAME('CNPJ').VALUE := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  CAIXAABERTURA.SQL.ADD('SELECT MIN(C.DATAABERTURA) AS DATAABERTURA, MAX(C.DATAFECHAMENTO) AS DATAFECHAMENTO, SUM(C.TROCOINICIAL) AS TROCOINICIAL, SUM(C.TROCOFINAL) AS TROCOFINAL,');&#13;&#10;  CAIXAABERTURA.SQL.ADD('SUM(C.DINHEIRO) AS DINHEIRO, SUM(C.CHEQUEAVISTA) AS CHEQUEAVISTA, SUM(C.CHEQUEPRAZO) CHEQUEPRAZO,');&#13;&#10;  CAIXAABERTURA.SQL.ADD('SUM(C.CARTAO) AS CARTAO, SUM(C.VALES) AS VALES, SUM(C.OUTROS) AS OUTROS, SUM(C.CONTRAVALE) AS CONTRAVALE,');&#13;&#10;  CAIXAABERTURA.SQL.ADD('SUM(C.VENDAPRAZO) AS VENDAPRAZO, SUM(C.REPOSICAO) AS REPOSICAO, SUM(C.RETIRADA) AS RETIRADA,');&#13;&#10;  CAIXAABERTURA.SQL.ADD('SUM(C.CONTACORRENTE) AS CONTACORRENTE');&#13;&#10;  CAIXAABERTURA.SQL.ADD('FROM CAIXAABERTURA C');&#13;&#10;  CAIXAABERTURA.SQL.ADD('WHERE (C.CODIGO IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + '))');&#13;&#10;  CAIXAABERTURA.SQL.ADD('AND C.CNPJ = :CNPJ');&#13;&#10;  CAIXAABERTURA.PARAMBYNAME('CNPJ').VALUE := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  CAIXAABERTURA.OPEN;&#13;&#10;&#13;&#10;  DATAI := ('01/' + INTTOSTR(MONTHOF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)) + '/' + INTTOSTR(YEAROF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)));&#13;&#10;  IF (MONTHOF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;) = 12) THEN&#13;&#10;    DATAF := ('31/12/'+INTTOSTR(YEAROF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)))&#13;&#10;  ELSE&#13;&#10;    DATAF := ('01/'+ INTTOSTR(MONTHOF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)+1) + '/' + INTTOSTR(YEAROF(&#60;CAIXAABERTURA.&#34;DATAABERTURA&#34;&#62;)));&#13;&#10;&#13;&#10;  MOVIMENTONF_MAPA.SQL.ADD('SELECT * FROM FECHAMENTO_DIA_ACUMULADO(:CODIGOCAIXA, :CNPJ, :DATAI - 1, :DATAF)');&#13;&#10;  MOVIMENTONF_MAPA.SQL.ADD('ORDER BY DESCRICAO');&#13;&#10;  MOVIMENTONF_MAPA.PARAMBYNAME('CODIGOCAIXA').VALUE    := SIGHAPARAM('CODIGOCAIXAABERTURA');&#13;&#10;  MOVIMENTONF_MAPA.PARAMBYNAME('DATAI').VALUE          := DATAI;&#13;&#10;  MOVIMENTONF_MAPA.PARAMBYNAME('DATAF').VALUE          := DATAF;&#13;&#10;  MOVIMENTONF_MAPA.PARAMBYNAME('CNPJ').VALUE           := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  BOMBAS_MAPA.SQL.ADD('SELECT * FROM FECHAMENTO_BOMBA_ACUMULADO(:CODIGOCAIXA, :CNPJ, :COMB)');&#13;&#10;  BOMBAS_MAPA.SQL.ADD('ORDER BY CODIGO, DESCRICAO');&#13;&#10;  BOMBAS_MAPA.PARAMBYNAME('CODIGOCAIXA').VALUE := SIGHAPARAM('CODIGOCAIXAABERTURA');&#13;&#10;  BOMBAS_MAPA.PARAMBYNAME('CNPJ').VALUE        := SIGHAPARAM('CNPJ');&#13;&#10;  BOMBAS_MAPA.PARAMBYNAME('COMB').VALUE        := 0;&#13;&#10;&#13;&#10;  PRECOCOMB.SQL.ADD('SELECT PC.CODIGOCOMB, AVG(PC.PRECOPRAZO) AS PRECOPRAZO');&#13;&#10;  PRECOCOMB.SQL.ADD('FROM PRECOCOMB PC, COMBUSTIVEL C, BOMBA B');&#13;&#10;  PRECOCOMB.SQL.ADD('WHERE B.CODIGO = :BOMBA');&#13;&#10;  PRECOCOMB.SQL.ADD('AND C.CODIGO = B.CODIGOCOMBUSTIVEL');&#13;&#10;  PRECOCOMB.SQL.ADD('AND PC.CODIGOCOMB = C.CODIGO AND B.ATIVO = 1');&#13;&#10;  PRECOCOMB.SQL.ADD('AND PC.CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  PRECOCOMB.SQL.ADD('AND PC.CNPJ = :CNPJ');&#13;&#10;  PRECOCOMB.SQL.ADD('GROUP BY 1');&#13;&#10;  PRECOCOMB.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  PRECOBOMBA.SQL.ADD('SELECT PB.CODIGOBOMBA, (PB.precobombacaixa) AS PRECOBOMBACAIXA');&#13;&#10;  PRECOBOMBA.SQL.ADD('FROM PRECOBOMBA PB');&#13;&#10;  PRECOBOMBA.SQL.ADD('WHERE PB.CODIGOBOMBA = :BOMBA');&#13;&#10;  PRECOBOMBA.SQL.ADD('AND PB.CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  PRECOBOMBA.SQL.ADD('AND PB.CNPJ = :CNPJ');&#13;&#10;  PRECOBOMBA.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  MOVIMENTO.SQL.ADD('SELECT M.TIPOMOVIMENTO AS TIPO, (CASE WHEN (M.TIPOMOVIMENTO = ''D'') THEN M.HISTORICO ELSE O.HISTORICO END) AS HISTORICO,');&#13;&#10;  MOVIMENTO.SQL.ADD('M.STATUS, SUM(M.VALOR) AS VALOR');&#13;&#10;  MOVIMENTO.SQL.ADD('FROM MOVIMENTOCONTABIL M, OPERACAOAUTOMATICO O, OPERACAO OP');&#13;&#10;  MOVIMENTO.SQL.ADD('WHERE (M.CODIGOCAIXAABERTURA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + '))');&#13;&#10;  MOVIMENTO.SQL.ADD('AND (M.CODIGOLANCAMENTO = O.CODIGOCAIXA)');&#13;&#10;  MOVIMENTO.SQL.ADD('AND (O.CODIGOLANCAMENTO = OP.CODIGOLANCAMENTO)');&#13;&#10;  MOVIMENTO.SQL.ADD('--AND (OP.OPERACAO &#60;&#62; 1)');&#13;&#10;  MOVIMENTO.SQL.ADD('AND M.CNPJ = :CNPJ');&#13;&#10;  MOVIMENTO.SQL.ADD('GROUP BY 1, 2, 3');&#13;&#10;  MOVIMENTO.SQL.ADD('ORDER BY 1, 2, 3');&#13;&#10;  MOVIMENTO.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  MOVIMENTONF.SQL.ADD('SELECT O.CODIGOLANCAMENTO,');&#13;&#10;  MOVIMENTONF.SQL.ADD('((CASE WHEN NF.TIPOMOVIMENTO = ''ED'' THEN ''(-) '' ELSE ''(+) '' END)||O.DESCRICAO) AS DESCRICAO,');&#13;&#10;  MOVIMENTONF.SQL.ADD('NF.STATUS,');&#13;&#10;  MOVIMENTONF.SQL.ADD('SUM(CASE WHEN NF.TIPOMOVIMENTO = ''ED'' THEN NF.VALORTOTALNF * -1 ELSE NF.VALORTOTALNF END) AS TOTALNF,');&#13;&#10;  MOVIMENTONF.SQL.ADD('COUNT(*) AS QTDA');&#13;&#10;  MOVIMENTONF.SQL.ADD('FROM OPERACAO O, NOTAFISCAL NF');&#13;&#10;  MOVIMENTONF.SQL.ADD('WHERE NF.CODIGOOPERACAO = O.CODIGOLANCAMENTO');&#13;&#10;  MOVIMENTONF.SQL.ADD('AND NF.CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  MOVIMENTONF.SQL.ADD('AND NF.TIPOMOVIMENTO IN (''S'', ''ED'')');&#13;&#10;  MOVIMENTONF.SQL.ADD('AND NF.NOTAFISCALCANCELADA = 0');&#13;&#10;  //Add by Tuba on 29/07/2009 - SighaPAF&#13;&#10;  MOVIMENTONF.SQL.ADD('AND NOT (NF.TIPODOCUMENTO = ''N'' AND NF.MODELO = ''01'' AND NF.OBSERVACAO IS NOT NULL AND NF.OBSERVACAO LIKE ''%NF Ref. Cupom Fiscal%'')');&#13;&#10;  MOVIMENTONF.SQL.ADD('AND NF.CNPJ = :CNPJ');&#13;&#10;  MOVIMENTONF.SQL.ADD('GROUP BY 1,2,3');&#13;&#10;  MOVIMENTONF.SQL.ADD('ORDER BY 2');&#13;&#10;  MOVIMENTONF.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;&#13;&#10;  CHEQUECADASTRADO.SQL.ADD('SELECT SUM(MC.VALOR) AS VALORCHEQUE');&#13;&#10;  CHEQUECADASTRADO.SQL.ADD('FROM ((MOVIMENTOCONTABIL MC');&#13;&#10;  CHEQUECADASTRADO.SQL.ADD('INNER JOIN OPERACAOAUTOMATICO O ON MC.CODIGOLANCAMENTO = O.CODIGOCAIXA)');&#13;&#10;  CHEQUECADASTRADO.SQL.ADD('INNER JOIN VARIAVEL V ON O.CODIGOVARIAVEL = V.CODIGO)');&#13;&#10;  CHEQUECADASTRADO.SQL.ADD('WHERE UPPER(V.DESCRICAO) = ''CADASTROCHEQUEPRE''');&#13;&#10;  CHEQUECADASTRADO.SQL.ADD('AND MC.CODIGOCAIXAABERTURA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  CHEQUECADASTRADO.SQL.ADD('AND MC.CNPJ = :CNPJ');&#13;&#10;  CHEQUECADASTRADO.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  TEM_COMBUST.SQL.ADD('SELECT DISTINCT ''TRUE'' AS ACHOU FROM NOTAFISCAL');&#13;&#10;  TEM_COMBUST.SQL.ADD('WHERE CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ') AND VALORCOMBUSTIVEL &#62; 0');&#13;&#10;  TEM_COMBUST.SQL.ADD('AND CNPJ = :CNPJ');&#13;&#10;  TEM_COMBUST.SQL.ADD('UNION');&#13;&#10;  TEM_COMBUST.SQL.ADD('SELECT DISTINCT ''TRUE'' AS ACHOU FROM ENCERRANTE');&#13;&#10;  TEM_COMBUST.SQL.ADD('WHERE CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ') AND');&#13;&#10;  TEM_COMBUST.SQL.ADD('CNPJ = :CNPJ');&#13;&#10;  TEM_COMBUST.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  CHEQUE.SQL.ADD('SELECT * FROM CHEQUE');&#13;&#10;  CHEQUE.SQL.ADD('WHERE USUARIO IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;&#13;&#10;  NFGERAL.SQL.ADD('SELECT O.DESCRICAO, R.NOME, NF.*');&#13;&#10;  NFGERAL.SQL.ADD('FROM OPERACAO O, NOTAFISCAL NF, RELACAO R');&#13;&#10;  NFGERAL.SQL.ADD('WHERE NF.CODIGOOPERACAO = O.CODIGOLANCAMENTO');&#13;&#10;  NFGERAL.SQL.ADD('AND NF.CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  NFGERAL.SQL.ADD('AND NF.TIPOMOVIMENTO = ''S''');&#13;&#10;  NFGERAL.SQL.ADD('AND NF.NOTAFISCALCANCELADA = 0');&#13;&#10;  NFGERAL.SQL.ADD('AND R.CODIGORELACAO = NF.CODIGORELACAO');&#13;&#10;  //Add by Tuba on 29/07/2009 - SighaPAF&#13;&#10;  NFGERAL.SQL.ADD('AND NOT (NF.TIPODOCUMENTO = ''N'' AND NF.MODELO = ''01'' AND NF.OBSERVACAO IS NOT NULL AND NF.OBSERVACAO LIKE ''%NF Ref. Cupom Fiscal%'')');&#13;&#10;  NFGERAL.SQL.ADD('AND NF.CNPJ = :CNPJ');&#13;&#10;  NFGERAL.SQL.ADD('ORDER BY O.DESCRICAO');&#13;&#10;  NFGERAL.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  NOTAFISCALCR.SQL.ADD('SELECT DISTINCT NF.VALORTOTALNF AS TOTALNF, C.NOME, CR.VALORDOCUMENTO, CR.NUMERODOCUMENTO');&#13;&#10;  NOTAFISCALCR.SQL.ADD('FROM NOTAFISCAL NF, CONTARECEBER CR, CLIENTE C, OPERACAO O');&#13;&#10;  NOTAFISCALCR.SQL.ADD('WHERE NF.CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND NF.CODIGOOPERACAO = O.CODIGOLANCAMENTO');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND O.TIPOOPERACAO IN (2,5)');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND NF.TIPOMOVIMENTO = ''S''');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND NF.NOTAFISCALCANCELADA = 0');&#13;&#10;  //Add by Tuba on 29/07/2009 - SighaPAF&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND NOT (NF.TIPODOCUMENTO = ''N'' AND NF.MODELO = ''01'' AND NF.OBSERVACAO IS NOT NULL AND NF.OBSERVACAO LIKE ''%NF Ref. Cupom Fiscal%'')');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND NF.CNPJ = :CNPJ');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND CR.CODIGONF = NF.CODIGONF');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND CR.CODIGOCLIENTE = C.CODIGOCLIENTE');&#13;&#10;  NOTAFISCALCR.SQL.ADD('AND CAST(CR.DATAEMISSAO AS DATE) = CAST(NF.DATAEMISSAO AS DATE)');&#13;&#10;  NOTAFISCALCR.SQL.ADD('ORDER BY CR.NUMERODOCUMENTO');&#13;&#10;  NOTAFISCALCR.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  NOTAFISCALCC.SQL.ADD('SELECT NF.VALORTOTALNF AS TOTALNF, R.NOME, CC.*');&#13;&#10;  NOTAFISCALCC.SQL.ADD('FROM NOTAFISCAL NF, CONTACORRENTE CC, RELACAO R');&#13;&#10;  NOTAFISCALCC.SQL.ADD('WHERE NF.CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  NOTAFISCALCC.SQL.ADD('AND NF.TIPOMOVIMENTO = ''S''');&#13;&#10;  NOTAFISCALCC.SQL.ADD('AND NF.NOTAFISCALCANCELADA = 0');&#13;&#10;  //Add by Tuba on 29/07/2009 - SighaPAF&#13;&#10;  NOTAFISCALCC.SQL.ADD('AND NOT (NF.TIPODOCUMENTO = ''N'' AND NF.MODELO = ''01'' AND NF.OBSERVACAO IS NOT NULL AND NF.OBSERVACAO LIKE ''%NF Ref. Cupom Fiscal%'')');&#13;&#10;  NOTAFISCALCC.SQL.ADD('AND NF.CNPJ = :CNPJ');&#13;&#10;  NOTAFISCALCC.SQL.ADD('AND CC.NUMERODOCUMENTO = NF.DOCUMENTO');&#13;&#10;  NOTAFISCALCC.SQL.ADD('AND CC.CODIGORELACAO = R.CODIGORELACAO');&#13;&#10;  NOTAFISCALCC.SQL.ADD('AND CC.DATAEMISSAO = NF.DATAEMISSAO');&#13;&#10;  NOTAFISCALCC.SQL.ADD('AND CC.CANCELADO = 0');&#13;&#10;  NOTAFISCALCC.SQL.ADD('ORDER BY NUMERODOCUMENTO');&#13;&#10;  NOTAFISCALCC.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('SELECT P.NOME, NFP.CODIGOPRODUTO, NFP.VALORUNITARIO,');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('SUM(NFP.QUANTIDADE) AS QTDE, SUM(NFP.VALORTOTAL) AS TOTAL, SUM(NFP.VALORDESCONTO) AS DESCONTO');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('FROM NOTAFISCAL NF, NOTAFISCALPRODUTO NFP, PRODUTO P');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('WHERE NF.CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('AND NF.TIPOMOVIMENTO = ''S''');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('AND NF.NOTAFISCALCANCELADA = 0');&#13;&#10;  //Add by Tuba on 29/07/2009 - SighaPAF&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('AND NOT (NF.TIPODOCUMENTO = ''N'' AND NF.MODELO = ''01'' AND NF.OBSERVACAO IS NOT NULL AND NF.OBSERVACAO LIKE ''%NF Ref. Cupom Fiscal%'')');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('AND NFP.CANCELADOITEM = 0');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('AND NF.CNPJ = :CNPJ');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('AND NF.CODIGONF = NFP.CODIGONF');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('AND NFP.CODIGOPRODUTO = P.CODIGO');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('AND NF.DOCUMENTO NOT IN (SELECT NFDEVOLUCAO');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('                         FROM NOTAFISCAL');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('                         WHERE CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('                         AND TIPOMOVIMENTO = ''ED'')');&#13;&#10;  NOTAFISCALPRODUTO.SQL.ADD('GROUP BY 2, 1, 3');&#13;&#10;  NOTAFISCALPRODUTO.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  CRRECEBIDAS.SQL.ADD('SELECT CRP.NUMERODUPLICATA, CRP.NUMEROPARCELA, CRP.VALORPAGO,');&#13;&#10;  CRRECEBIDAS.SQL.ADD('CR.NUMERODOCUMENTO, CR.CODIGOCLIENTE, CL.NOME');&#13;&#10;  CRRECEBIDAS.SQL.ADD('FROM CONTARECEBERPARCIAL CRP, CONTARECEBER CR, CLIENTE CL');&#13;&#10;  CRRECEBIDAS.SQL.ADD('WHERE CRP.CODIGOCAIXAABERTURA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  CRRECEBIDAS.SQL.ADD('AND CRP.NUMERODUPLICATA = CR.NUMERODUPLICATA');&#13;&#10;  CRRECEBIDAS.SQL.ADD('AND CRP.NUMEROPARCELA = CR.NUMEROPARCELA');&#13;&#10;  CRRECEBIDAS.SQL.ADD('AND CRP.STATUS = CR.STATUS');&#13;&#10;  CRRECEBIDAS.SQL.ADD('AND CRP.CNPJ = CR.CNPJ AND CR.CNPJ = :CNPJ');&#13;&#10;  CRRECEBIDAS.SQL.ADD('AND CR.CODIGOCLIENTE = CL.CODIGOCLIENTE');&#13;&#10;  CRRECEBIDAS.SQL.ADD('AND CRP.VALORPAGO &#62; 0');&#13;&#10;  CRRECEBIDAS.SQL.ADD('ORDER BY CR.NUMERODOCUMENTO');&#13;&#10;  CRRECEBIDAS.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('SELECT O.CODIGOLANCAMENTO, O.DESCRICAO, NF.STATUS, SUM(NFM.VALOR) AS TOTALNF, COUNT(*) AS QTDA');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('FROM OPERACAO O, NOTAFISCAL NF, NOTAFISCALMULTOPERACAO NFM');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('WHERE NFM.CODIGOLANCAMENTO = O.CODIGOLANCAMENTO');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('AND NFM.CODIGONF = NF.CODIGONF');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('AND NF.CODIGOCAIXA IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('AND NF.TIPOMOVIMENTO = ''S''');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('AND NF.NOTAFISCALCANCELADA = 0');&#13;&#10;  //Add by Tuba on 29/07/2009 - SighaPAF&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('AND NOT (NF.TIPODOCUMENTO = ''N'' AND NF.MODELO = ''01'' AND NF.OBSERVACAO IS NOT NULL AND NF.OBSERVACAO LIKE ''%NF Ref. Cupom Fiscal%'')');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('AND NF.CNPJ = :CNPJ');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('GROUP BY O.CODIGOLANCAMENTO, O.DESCRICAO, NF.STATUS');&#13;&#10;  MOVIMENTONFMFP.SQL.ADD('ORDER BY O.DESCRICAO');&#13;&#10;  MOVIMENTONFMFP.PARAMBYNAME('CNPJ').VALUE  := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  OBSRETIRADA.SQL.ADD('SELECT C.OBSERVACAO');&#13;&#10;  OBSRETIRADA.SQL.ADD('FROM CAIXAABERTURA C');&#13;&#10;  OBSRETIRADA.SQL.ADD('WHERE (C.CODIGO IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + '))');&#13;&#10;  OBSRETIRADA.SQL.ADD('AND C.CNPJ = :CNPJ');&#13;&#10;  OBSRETIRADA.PARAMBYNAME('CNPJ').VALUE := SIGHAPARAM('CNPJ');&#13;&#10;&#13;&#10;  FUNCIONARIO.SQL.ADD('SELECT CODIGO, NOME, APELIDO');&#13;&#10;  FUNCIONARIO.SQL.ADD('FROM FUNCIONARIO');&#13;&#10;  FUNCIONARIO.SQL.ADD('WHERE CODIGO IN (SELECT CODIGOFUNCIONARIO');&#13;&#10;  FUNCIONARIO.SQL.ADD('                 FROM CAIXAABERTURA C');&#13;&#10;  FUNCIONARIO.SQL.ADD('                 WHERE (CODIGO IN (' + SIGHAPARAM('CODIGOCAIXAABERTURA') + ')))');&#13;&#10;  FUNCIONARIO.OPEN;&#13;&#10;&#13;&#10;  WHILE NOT FUNCIONARIO.EOF DO BEGIN&#13;&#10;    ReportSummary2.VISIBLE := (FUNCIONARIO.RECORDCOUNT = 1);&#13;&#10;    FUNCIONARIO.NEXT;&#13;&#10;  END;&#13;&#10;end." PropData="">
