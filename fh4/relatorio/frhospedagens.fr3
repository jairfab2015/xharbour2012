    <TfrxIBXQuery Name="HOSPEDAGEM" UserName="HOSPEDAGEM" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT &#13;&#10;&#13;&#10;H.CODIGO,&#13;&#10;CAST(H.ENTRADA AS DATE) AS ENTRADA,&#13;&#10;H.F_APARTAMENTO,&#13;&#10;CLAS.DESCRICAO AS CLASSE,&#13;&#10;D.VALOR AS DIARIABASE,&#13;&#10;H.VALORDIARIA AS DIARIACOBRADA,&#13;&#10;HOS.NOME AS NOMEHOSPEDE,&#13;&#10;F.NOME AS NOMEEMPRESA,&#13;&#10;H.NADULTOS, &#13;&#10;H.NCRIANCAS&#13;&#10;&#13;&#10;FROM HTL_HOSPEDAGEM H, HTL_HOSPEDAGEM_HOSPEDE HH, HTL_DIARIA D,&#13;&#10;     HTL_APARTAMENTO_CLASSE CLAS, HTL_HOSPEDE HOS, FORNECEDOR F&#13;&#10;&#13;&#10;WHERE&#13;&#10;(HH.F_HOSPEDE BETWEEN :F_HOSPEDEI AND :F_HOSPEDEF)&#13;&#10;AND (H.F_DIARIA = D.CODIGO) AND (HH.F_HOSPEDE = HOS.CODIGO)&#13;&#10;AND (HH.F_EMPRESA = F.CODIGO) AND (D.F_CLASSE = CLAS.CODIGO)&#13;&#10;AND (H.CODIGO = HH.F_HOSPEDAGEM)&#13;&#10;&#13;&#10;ORDER BY HH.F_HOSPEDE, H.ENTRADA" PropData=""/>
