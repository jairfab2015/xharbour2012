    <TfrxIBXQuery Name="TRANSFERENCIA" UserName="TRANSFERENCIA" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT TR.DATA, TR.QUANTIDADE, TR.ATENDIDO,&#13;&#10;LE.DESCRICAO||' - '||P.CODIGO ||' - '|| P.NOME AS PRODUTOORIGEMLOCALDESCRICAO,&#13;&#10;LE1.DESCRICAO||' - '||P1.CODIGO ||' - '||P1.NOME AS PRODUTODESTINOLOCALDESCRICAO,&#13;&#10;TR.PRODUTOORIGEMOBS,TR.PRODUTODESTINOOBS&#13;&#10;&#13;&#10;FROM TRANSFERENCIA TR, LOCALESTOQUE LE, LOCALESTOQUE LE1, PRODUTO P, PRODUTO P1&#13;&#10;&#13;&#10;WHERE (TR.CODIGOTRANSFERENCIA = :CODIGOTRANSFERENCIA) AND&#13;&#10;(TR.PRODUTOORIGEMLOCALESTOQUE = LE.CODIGO) AND&#13;&#10;(TR.PRODUTODESTINOLOCALESTOQUE = LE1.CODIGO) AND&#13;&#10;(TR.PRODUTOORIGEM = P.CODIGO) AND&#13;&#10;(TR.PRODUTODESTINO = P1.CODIGO) AND&#13;&#10;(TR.CNPJ = :CNPJ)&#13;&#10;&#13;&#10;ORDER BY TR.CODIGO" PropData=""/>
