 if RELOFICIAL = 'S' THEN&#13;&#10;      begin&#13;&#10;        PAGINA := PAGINA + 1;&#13;&#10;        pgEncerramento.Tag := PAGINA;&#13;&#10;      end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure pgAberturaOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   if RELOFICIAL = 'S' THEN&#13;&#10;      PAGINA := STRTOINT(SIGHAPARAM('PAGINAINICIAL'));&#13;&#10;end;&#13;&#10;&#13;&#10;procedure Header1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;{ Rodrigo- teste - se não descomentar pode excluir&#13;&#10;  if RELOFICIAL = 'S' THEN BEGIN //Se é oficial mostra a partir da 3º página&#13;&#10;     if PAGINA &#62; 2 THEN&#13;&#10;        Header1.Visible := True&#13;&#10;     else&#13;&#10;        Header1.Visible := False;&#13;&#10;  END&#13;&#10;  ELSE BEGIN&#13;&#10;     if PAGINA &#62; 1 THEN&#13;&#10;        Header1.Visible := True&#13;&#10;     else&#13;&#10;        Header1.Visible := False;&#13;&#10;  END;&#13;&#10;}&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;  pgAbertura.MirrorMargins := (SIGHAPARAM('MODOIMPRESSAO') = '1');&#13;&#10;  pgMovimento.MirrorMargins := (SIGHAPARAM('MODOIMPRESSAO') = '1');&#13;&#10;  pgEncerramento.MirrorMargins := (SIGHAPARAM('MODOIMPRESSAO') = '1');&#13;&#10;  PAGINA  := STRTOINT(SIGHAPARAM('PAGINAINICIAL'));&#13;&#10;  RELOFICIAL := (SIGHAPARAM('RELATORIOOFICIAL'));&#13;&#10;  NUMERO := STRTOINT(SIGHAPARAM('NUMERO'));&#13;&#10;&#13;&#10;  if RELOFICIAL = 'S' THEN //Se é oficial mostra as páginas de abertura e encerramento&#13;&#10;     begin&#13;&#10;       pgAbertura.visible := True;&#13;&#10;       pgEncerramento.visible := True;&#13;&#10;       Report.EngineOptions.DoublePass := True;&#13;&#10;     end&#13;&#10;  else&#13;&#10;     begin&#13;&#10;       PAGINA := PAGINA -1;&#13;&#10;       pgAbertura.visible := False;&#13;&#10;       pgEncerramento.visible := False;&#13;&#10;       Report.EngineOptions.DoublePass := False;&#13;&#10;     end;&#13;&#10;&#13;&#10;  TEXTO := '';&#13;&#10;  if pgAbertura.MirrorMargins = True then //Se a impressão é anverso e verso a numeração será por página&#13;&#10;     TEXTO := 'Página: '&#13;&#10;  else&#13;&#10;     TEXTO := 'Folha: ';   //Se somente anverso será por folha&#13;&#10;&#13;&#10;&#13;&#10;  //Rodrigo 12/03/08 ord&#13;&#10;  if RELOFICIAL = 'N' THEN //Se for conciliação aumenta fonte para facilicar conferências na contabilidade&#13;&#10;     begin&#13;&#10;      MEMO7.FONT.SIZE    := 9;&#13;&#10;      MEMO9.FONT.SIZE    := 9;&#13;&#10;      MEMO14.FONT.SIZE   := 9;&#13;&#10;      MEMO15.FONT.SIZE   := 9;&#13;&#10;      MEMO16.FONT.SIZE   := 9;&#13;&#10;      MEMO7.HEIGHT       := 38;&#13;&#10;      MEMO9.HEIGHT       := 38;&#13;&#10;      MEMO14.HEIGHT      := 38;&#13;&#10;      MEMO15.HEIGHT      := 38;&#13;&#10;      MEMO16.HEIGHT      := 38;&#13;&#10;      MasterData1.Height := 20;&#13;&#10;     end&#13;&#10;&#13;&#10;end." PropData="">
