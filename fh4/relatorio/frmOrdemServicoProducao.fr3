    <TfrxIBXQuery Name="QRYPRODUCAO" UserName="QRYPRODUCAO" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT N.CODIGOPRODUTO, SUM(N.QUANTIDADE) AS ENTRADA, P.NOME,&#13;&#10;L.DESCRICAO AS LINHA, C.CODIGOSERVICO, S.DESCRICAO , S.UNIDADE,&#13;&#10;C.QUANTIDADE AS QTDASERV&#13;&#10;FROM NOTAFISCALPRODUTO N, PRODUTO P, COMPOSICAOSERVICO C,&#13;&#10;SERVICO S, LINHA L&#13;&#10;WHERE N.CONTROLETIPO = 'E' AND N.TIPOMOVIMENTO = 'E'&#13;&#10;AND N.CODIGOPRODUTO = P.CODIGO AND N.DATA BETWEEN :DATAI AND :DATAF&#13;&#10;AND ((N.STATUS = '#') OR (N.STATUS = :STATUS))&#13;&#10;AND ((P.ORIGEMPRODUTO = 'N') OR (P.ORIGEMPRODUTO = 'I'))&#13;&#10;AND C.CODIGOCOMPOSICAO = N.CODIGOPRODUTO&#13;&#10;AND N.CNPJ = :CNPJ&#13;&#10;AND C.CODIGOSERVICO = S.CODIGO AND P.LINHA = L.CODIGO&#13;&#10;GROUP BY N.CODIGOPRODUTO, P.NOME, L.DESCRICAO, C.CODIGOSERVICO,&#13;&#10;S.DESCRICAO, S.UNIDADE, C.QUANTIDADE&#13;&#10;ORDER BY N.CODIGOPRODUTO" PropData=""/>
