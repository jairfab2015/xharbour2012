    <TfrxIBXQuery Name="qrJPublicidade" UserName="qrJPublicidade" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT JP.CODIGO AS CONTRATO, JP.datacadastro, JP.TIPOPUBLICIDADE, C.NOME AS CLIENTE,&#13;&#10;JD.DATA, JPAG.PREFIXO AS PAGINA, JD.TAMANHO, S.DESCRICAO AS SERVICO,&#13;&#10;JC.DESCRISCAO AS COR, JD.VALOR, JD.OBSERVACOES,F.nome, R.descricao, JP.VALOR AS TOTAL&#13;&#10;&#13;&#10;FROM JPUBLICIDADE JP, CLIENTE C, JDATAS JD, JPAGINA JPAG, JCOR JC, SERVICO S , funcionario F, regranegocio R&#13;&#10;&#13;&#10;WHERE JP.CODIGOCLIENTE = C.CODIGOCLIENTE&#13;&#10;AND JP.CODIGO BETWEEN :CI AND :CF&#13;&#10;AND JD.CODIGOCONTRATO BETWEEN :CI AND :CF&#13;&#10;AND JD.DATA BETWEEN :DATAI AND :DATAF&#13;&#10;AND JP.CODIGOSERVICO = S.CODIGO&#13;&#10;AND JD.CODIGOCONTRATO = JP.CODIGO&#13;&#10;AND JD.PAGINA = JPAG.CODIGO&#13;&#10;AND JD.COR = JC.CODIGO&#13;&#10;AND STATUS = '*'&#13;&#10;AND JP.codigovendedor = F.codigo&#13;&#10;and JP.CODIGOPLANO = R.codigo" PropData=""/>
