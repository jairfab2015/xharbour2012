    <TfrxIBXQuery Name="QRTransportadora" UserName="QRTransportadora" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT CL.NOME, CL.ENDERECO, CL.ENDERECONUMERORUA, CL.ENDERECOBAIRRO,&#13;&#10;CL.ENDERECOCXPOSTAL, CL.CEP, CL.COMERCIALFONE,&#13;&#10;C.CIDADE AS DESCRICAOCIDADE, C.ESTADO&#13;&#10;FROM CLIENTE CL, CIDADE C&#13;&#10;WHERE CL.CODIGOCLIENTE = :CODIGO&#13;&#10;AND CL.CIDADE = C.CODIGO" PropData="0A506172616D657465727301010C4D000000204E616D653D22434F4449474F222044617461547970653D226674496E7465676572222045787072657373696F6E3D225349474841504152414D28275452414E53504F525441444F52412729220000"/>
