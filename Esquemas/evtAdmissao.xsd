                  <xs:element name="tpInscricao">
                    <xs:simpleType>
                      <xs:annotation>
                        <xs:documentation>Tipo de Inscrição, conforme tabela 5.</xs:documentation>
                      </xs:annotation>
                      <xs:restriction base="xs:byte">
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="nrInscricao">
                    <xs:simpleType>
                      <xs:annotation>
                        <xs:documentation>Número de Inscrição</xs:documentation>
                      </xs:annotation>
                      <xs:restriction base="xs:string">
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="ideTrabSubstituido" minOccurs="0">
              <xs:annotation>
                <xs:documentation>Identificação do trabalhador substituído.  O preenchimento é obrigatório se o {motivoContratacao} for igual a [1].</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="cpfTrabSubstituido">
                    <xs:simpleType>
                      <xs:annotation>
                        <xs:documentation>Cpf do trabalhador substituído.</xs:documentation>
                      </xs:annotation>
                      <xs:restriction base="xs:string">
                        <xs:pattern value="[0-9]{11}"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="matricTrabSubstituido">
                    <xs:simpleType>
                      <xs:annotation>
                        <xs:documentation>Matrícula do trabalhador que está sendo substituído</xs:documentation>
                      </xs:annotation>
                      <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="30"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="localTrabalho" type="TLocalTrab">
        <xs:annotation>
          <xs:documentation>Informações do local de trabalho</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="horContratual" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Informações do Horário Contratual do Trabalhador.   O preenchimento é obrigatório se {tpRegimeJor} = [1].</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="qtdHoras">
              <xs:simpleType>
                <xs:annotation>
                  <xs:documentation>Quantidade média de horas relativas à jornada semanal do trabalhador</xs:documentation>
                </xs:annotation>
                <xs:restriction base="xs:positiveInteger">
                  <xs:totalDigits value="3"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
            <xs:element name="horario" type="THorario">
              <xs:annotation>
                <xs:documentation>Informações diárias do horário contratual</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="infoAtivDesemp" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Atividades desempenhadas pelo trabalhador</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="descAtividade" minOccurs="0" maxOccurs="99">
              <xs:annotation>
                <xs:documentation>Descrição das Atividades Desempenhadas</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="descAtivDesemp">
                    <xs:simpleType>
                      <xs:annotation>
                        <xs:documentation>Descrição da atividade desempenhada</xs:documentation>
                      </xs:annotation>
                      <xs:restriction base="xs:string">
                        <xs:whiteSpace value="preserve"/>
                        <xs:minLength value="1"/>
                        <xs:maxLength value="1500"/>
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="filiacaoSindical" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Filiação Sindical do Trabalhador</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="cnpjSindTrabalhador">
              <xs:simpleType>
                <xs:annotation>
                  <xs:documentation>CNPJ do Sindicato</xs:documentation>
                </xs:annotation>
                <xs:restriction base="xs:string">
                  <xs:pattern value="[0-9]{14}"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="alvaraJudicial" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Informações do alvará judicial, preenchimento obrigatório em caso de contratação de menores de 14 anos, em qualquer categoria, e de maiores de 14 e menores de 16, em categoria diferente de "Aprendiz".</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element name="nrProcJud">
              <xs:simpleType>
                <xs:annotation>
                  <xs:documentation>Identificação do Número do Processo Judicial</xs:documentation>
                </xs:annotation>
                <xs:restriction base="xs:string">
                  <xs:minLength value="1"/>
                  <xs:maxLength value="20"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TRemun">
    <xs:annotation>
      <xs:documentation>Remuneração e periodicidade de pagamento</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="vlrSalFixo">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Salário fixo do trabalhador, correspondente à parte fixa da remuneração.</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:decimal">
            <xs:totalDigits value="14"/>
            <xs:fractionDigits value="2"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="unidSalFixo">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Unidade de pagamento da parte fixa da remuneração, conforme tabela abaixo:
            1 - Por Hora
            2 - Por Dia
            3 - Por Semana
            4 - Por Quinzena
            5 - Por Mês
            6 - Por Tarefa</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:byte">
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="descSalVariavel" minOccurs="0">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Descrição do salário variável e como este é calculado.  ex: Comissões pagas no percentual de 10% sobre as vendas.</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:whiteSpace value="preserve"/>
            <xs:minLength value="0"/>
            <xs:maxLength value="90"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TLocalTrab">
    <xs:annotation>
      <xs:documentation>Informações do Local de Trabalho</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="tpInscricao">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Tipo de Inscrição, conforme tabela 5.</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:byte">
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="nrInscricao">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Número de Inscrição</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="codLotacao">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Código da Lotação</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="30"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="descComplementar" minOccurs="0">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Descrição complementar</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="80"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="THorario">
    <xs:annotation>
      <xs:documentation>Informações de Horário Contratual</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="dia">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Indica o dia do horário:
            1 - Segunda-Feira;
            2 - Terça-Feira;
            3 - Quarta-Feira;
            4 - Quinta-Feira;
            5 - Sexta-Feira;
            6 - Sábado;
            7 - Domingo;
            8 - Dia variável</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:byte">
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="codHorContratual">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Código do Horário Contratual</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="30"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TFgts">
    <xs:annotation>
      <xs:documentation>Informações do FGTS</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="optanteFGTS">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Opção pelo FGTS:
            1 - Optante;
            2 - Não Optante</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:enumeration value="S"/>
            <xs:enumeration value="N"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="dtOpcaoFGTS">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Data de Opção pelo FGTS</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:date">
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TSucVinculo">
    <xs:annotation>
      <xs:documentation>Informações da sucessão de vínculo trabalhista</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="cnpjEmpregadorAnterior">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>CNPJ do empregador anterior</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{14}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="matriculaAnterior">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Matrícula do trabalhador no empregador anterior.</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="30"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="dtInicioVinculo">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Data de Início do Vínculo</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:date">
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="observacao" minOccurs="0">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Observação</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="80"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TCessaoTrab">
    <xs:annotation>
      <xs:documentation>Informações de cessão de trabalhador</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="cnpjCedente">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>CNPJ do Cedente</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{14}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="matriculaCedente">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Matrícula do Trabalhador na Empresa Cedente</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="30"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="dtAdmissaoCedente">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Data de admissão na Empresa Cedente</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:date">
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="infoOnus">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation>Ônus da Cessão:
            1 - Ônus do Cedente;
            2 - Ônus do Cessionário;
            3 - Ônus do Cedente e Cessionário;</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:byte">
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:schema>
